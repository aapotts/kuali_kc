<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>
    <!-- ========== form bean definitions ================================== -->
    <form-beans>
    <form-bean name="KualiForm" type="org.kuali.rice.kns.web.struts.form.KualiForm" />
      <form-bean name="ActionListForm" type="org.kuali.rice.kew.actionlist.web.ActionListFormOld" />
      <form-bean name="PreferencesForm" type="org.kuali.rice.kew.preferences.web.PreferencesForm" />
      <form-bean name="ActionListFilterForm" type="org.kuali.rice.kew.actionlist.web.ActionListFilterForm" />
      <form-bean name="BackdoorForm" type="org.kuali.rice.kew.web.backdoor.BackdoorForm" />
      <form-bean name="DocHandlerForm" type="org.kuali.rice.kew.routing.web.DocHandlerForm" />
      <form-bean name="SuperUserForm" type="org.kuali.rice.kew.superuser.web.SuperUserForm"/>
      <form-bean name="Rule2Form" type="org.kuali.rice.kew.rule.web.Rule2Form" />
      <form-bean name="FeedbackForm" type="org.kuali.rice.kew.feedback.web.FeedbackForm"/>
      <form-bean name="StatsForm" type="org.kuali.rice.kew.stats.web.StatsForm" />
      <form-bean name="QuickLinksForm" type="org.kuali.rice.kew.quicklinks.web.QuickLinksForm"/>
      <form-bean name="NoteForm" type="org.kuali.rice.kew.notes.web.NoteForm" />
      <form-bean name="HelpForm" type="org.kuali.rice.kew.help.web.HelpForm" />
      <form-bean name="IngesterForm" type="org.kuali.rice.kew.batch.web.IngesterForm" />

      <!-- Removing Remove/replace functionality for now, as per Application Roadmap committee, will revisit in 1.1
      <form-bean name="RemoveReplaceForm" type="org.kuali.rice.kew.removereplace.web.RemoveReplaceForm"/>
      -->


    </form-beans>

    <!-- =================== exception handler definitions ================= -->
    <global-exceptions>
		  <exception handler="org.kuali.rice.kew.web.StrutsExceptionHandler" key="error.global.message"
                 path="/WEB-INF/jsp/Error.jsp" scope="request" type="java.lang.Exception"/>
    </global-exceptions>

    <!-- ========== global forward definitions ============================= -->
    <global-forwards type="org.apache.struts.action.ActionForward">
   	    <forward name="WorkflowError" path="/WEB-INF/jsp/Error.jsp" />
   	    <forward name="DeleteRouteHeaderConfirmation" path="/WEB-INF/jsp/DeleteRouteHeaderConfirmation.jsp" />
   	    <forward name="NotAuthorized" path="/WEB-INF/jsp/NotAuthorized.jsp" />
   	    <!-- <forward name="route_report" path="/WEB-INF/jsp/RoutingReportForward.jsp"/>-->
   	    <forward name="ActionListNew" contextRelative="true" path="/kew/ActionList.do" redirect="true"/>
    </global-forwards>

    <!--  ========== action mapping definitions ============================= -->
    <action-mappings>

	  <!-- The following should be considered temporary mappings to reduce immediate impact when URLS are updated
	       They should be removed before the Rice 1.0 release so that we don't need to continue supporting them!!!
	   -->
      <action path="/RuleQuickLinks" scope="request" validate="false" type="org.apache.struts.actions.ForwardAction"
     		parameter="/kew/RuleQuickLinks.do"/>
      <action path="/RuleQuickLinks" scope="request" validate="false" type="org.apache.struts.actions.ForwardAction"
     		parameter="/kew/RuleQuickLinks.do"/>
      <action path="/RoutingReport" scope="request" validate="false" type="org.apache.struts.actions.ForwardAction"
            parameter="/kew/RoutingReport.do"/>
	  <action path="/DocumentSearch" scope="request" validate="false" type="org.apache.struts.actions.ForwardAction"
	  		parameter="/kr/lookup.do?businessObjectClassName=org.kuali.rice.kew.docsearch.DocSearchCriteriaDTO"/>
      <action path="/ActionList" scope="request" validate="false" type="org.apache.struts.actions.ForwardAction"
     		parameter="/kew/ActionList.do"/>
      <action path="/ActionListFilter" scope="request" validate="false" type="org.apache.struts.actions.ForwardAction"
     		parameter="/kew/ActionListFilter.do"/>
      <action path="/RouteLog" scope="request" validate="false"
            type="org.kuali.rice.kns.web.struts.action.ForwardWithQueryParametersAction"
     		parameter="/kew/RouteLog.do"/>
      <action path="/ActionListOld" scope="request" validate="false" parameter="methodToCall"
        type="org.kuali.rice.kew.actionlist.web.ActionListActionOld" name="ActionListForm"
        input="/WEB-INF/jsp/ActionList/ActionList.jsp" >
        <forward name="viewActionList" path="/WEB-INF/jsp/ActionList/ActionList.jsp"/>
        <forward name="count" path="/WEB-INF/jsp/ActionList/ActionListCount.jsp"/>
        <forward name="requiredStateError" path="/WEB-INF/jsp/ActionList/ActionList.jsp"/>
        <forward name="finalStateError" path="/WEB-INF/jsp/ActionList/ActionList.jsp"/>
      </action>
      <action path="/ActionListFilterOld" scope="request" validate="false" name="ActionListFilterForm"
        input="/WEB-INF/jsp/ActionList/ActionListFilter.jsp"
        type="org.kuali.rice.kew.actionlist.web.ActionListFilterActionOld" parameter="methodToCall">
        <forward name="viewFilter" path="/WEB-INF/jsp/ActionList/ActionListFilter.jsp" />
        <forward name="viewActionList" path="/ActionListOld.do?methodToCall=start" redirect="true" />
        <forward name="requiredStateError" path="/WEB-INF/jsp/ActionList/ActionListFilter.jsp"/>
        <forward name="finalStateError" path="/WEB-INF/jsp/ActionList/ActionListFilter.jsp"/>
      </action>

      <!-- end temporary mappings -->

      <action path="/SuperUser" type="org.kuali.rice.kew.superuser.web.SuperUserAction" input="/WEB-INF/jsp/superuser/SuperUser.jsp" name="SuperUserForm" scope="request" parameter="methodToCall">
		 <forward name="basic" path="/WEB-INF/jsp/superuser/SuperUser.jsp" />
		 <forward name="actionTaken" path="/WEB-INF/jsp/DocHandlerActionTaken.jsp" />
        <forward name="requiredStateError" path="/WEB-INF/jsp/superuser/SuperUser.jsp"/>
        <forward name="finalStateError" path="/WEB-INF/jsp/superuser/SuperUser.jsp"/>
      </action>

      <action path="/Error" scope="request" validate="false" type="org.kuali.rice.kew.web.ModuleRelativeForwardAction"
             parameter="/WEB-INF/jsp/Error.jsp"/>

      <action path="/Preferences" name="PreferencesForm" scope="request" validate="false"
        input="/WEB-INF/jsp/Preferences/Preferences.jsp" parameter="methodToCall"
        type="org.kuali.rice.kew.preferences.web.PreferencesAction">
        <forward name="viewPreferences" path="/WEB-INF/jsp/Preferences/Preferences.jsp"/>
        <forward name="viewActionList" path="/ActionList.do?methodToCall=start" redirect="true"/>
        <forward name="requiredStateError" path="/WEB-INF/jsp/Preferences/Preferences.jsp"/>
        <forward name="finalStateError" path="/WEB-INF/jsp/Preferences/Preferences.jsp"/>
      </action>

      <action path="/Backdoor" name="BackdoorForm" scope="request" parameter="methodToCall"
        input="/WEB-INF/jsp/backdoor/Backdoor.jsp"
        type="org.kuali.rice.kew.web.backdoor.BackdoorAction">
        <forward name="viewBackdoor" path="/WEB-INF/jsp/backdoor/Backdoor.jsp"/>
        <forward name="viewPortal" path="/WEB-INF/jsp/backdoor/index.jsp"/>
        <forward name="requiredStateError" path="/WEB-INF/jsp/backdoor/Backdoor.jsp"/>
        <forward name="finalStateError" path="/WEB-INF/jsp/backdoor/Backdoor.jsp"/>
      </action>

      <action path="/Portal" name="BackdoorForm" scope="request" parameter="methodToCall"
      	input="/WEB-INF/jsp/backdoor/index.jsp"
      	type="org.kuali.rice.kew.web.backdoor.BackdoorAction">
      	<forward name="viewPortal" path="/WEB-INF/jsp/backdoor/index.jsp"/>
      </action>

      <action path="/Administration" name="BackdoorForm" scope="request" parameter="methodToCall"
        type="org.kuali.rice.kew.web.backdoor.BackdoorAction">
        <forward name="administration" path="/WEB-INF/jsp/backdoor/Admin.jsp"/>
      </action>

      <action path="/Stats" name="StatsForm" scope="request" validate="false"
          input="/WEB-INF/jsp/stats/StatsReport.jsp"
          type="org.kuali.rice.kew.stats.web.StatsAction" parameter="methodToCall">
          <forward name="basic" path="/WEB-INF/jsp/stats/StatsReport.jsp"/>
          <forward name="requiredStateError" path="/WEB-INF/jsp/stats/StatsReport.jsp"/>
          <forward name="finalStateError" path="/WEB-INF/jsp/stats/StatsReport.jsp"/>
      </action>


      <action path="/DocHandler" name="DocHandlerForm" scope="request" parameter="methodToCall"
        type="org.kuali.rice.kew.routing.web.ClientAppDocHandlerRedirectAction" >
        <!-- forward name="documentLocked" path="/WEB-INF/jsp/DocumentLocked.jsp" /-->
        <forward name="requiredStateError" path="/WEB-INF/jsp/DocumentLocked.jsp"/>
        <forward name="finalStateError" path="/WEB-INF/jsp/DocumentLocked.jsp"/>
      </action>

       <action path="/WorkflowDocHandler" name="DocHandlerForm" scope="request" parameter="methodToCall"
        type="org.kuali.rice.kew.routing.web.WorkflowDocHandlerAction">
        <forward name="actionTaken" path="/WEB-INF/jsp/DocHandlerActionTaken.jsp" />
        <forward name="requiredStateError" path="/WEB-INF/jsp/DocHandlerActionTaken.jsp"/>
        <forward name="finalStateError" path="/WEB-INF/jsp/DocHandlerActionTaken.jsp"/>
       </action>

       <action path="/Rule" type="org.kuali.rice.kew.rule.web.Rule2Action"
          input="/WEB-INF/jsp/rules/RuleEdit.jsp"
          name="Rule2Form" scope="request" parameter="methodToCall" validate="true">
          <forward path="/WEB-INF/jsp/rules/RuleDocHandler.jsp" name="docHandler" />

          <forward path="/WEB-INF/jsp/rules/RuleEdit.jsp" name="basic" />
          <forward path="/WEB-INF/jsp/routetemplate/RuleGlobal.jsp" name="global" />
          <forward path="/WEB-INF/jsp/rules/RuleReport.jsp" name="report" />
          <forward path="/DelegateRule.do" name="delegateEdit"/>
          <forward name="requiredStateError" path="/WEB-INF/jsp/rules/RuleReport.jsp"/>
          <forward name="finalStateError" path="/WEB-INF/jsp/rules/RuleReport.jsp"/>
       </action>

       <action path="/DelegateRule" type="org.kuali.rice.kew.rule.web.DelegateRule2Action"
          input="/WEB-INF/jsp/rules/DelegateRule.jsp"
          name="Rule2Form" scope="request" parameter="methodToCall" validate="true">
          <forward path="/WEB-INF/jsp/rules/DelegateRule.jsp" name="basic" />
          <forward path="/WEB-INF/jsp/rules/RuleReport.jsp" name="report" />
          <forward name="requiredStateError" path="/WEB-INF/jsp/rules/RuleReport.jsp"/>
          <forward name="finalStateError" path="/WEB-INF/jsp/rules/RuleReport.jsp"/>
       </action>

      <action path="/Feedback"
          type="org.kuali.rice.kew.feedback.web.FeedbackAction"
          name="FeedbackForm"
          validate="false"
          parameter="methodToCall"
          scope="request">
          <forward name="start" path="/WEB-INF/jsp/Feedback.jsp" />
          <forward name="sent" path="/WEB-INF/jsp/FeedbackSent.jsp" />
          <forward name="requiredStateError" path="/WEB-INF/jsp/Feedback.jsp"/>
          <forward name="finalStateError" path="/WEB-INF/jsp/Feedback.jsp"/>
      </action>

      <action path="/QuickLinks" name="QuickLinksForm" scope="request" validate="false" type="org.kuali.rice.kew.quicklinks.web.QuickLinksAction" parameter="methodToCall">
	      <forward name="basic" path="/WEB-INF/jsp/quicklinks/QuickLinks.jsp"/>
	      <forward name="requiredStateError" path="/WEB-INF/jsp/quicklinks/QuickLinks.jsp"/>
          <forward name="finalStateError" path="/WEB-INF/jsp/quicklinks/QuickLinks.jsp"/>
      </action>

      <action path="/Note" name="NoteForm" scope="request" validate="false"
          input="/WEB-INF/jsp/notes/AllNotesReport.jsp"
          type="org.kuali.rice.kew.notes.web.NoteAction" parameter="methodToCall">
          <forward name="allNotesReport" path="/WEB-INF/jsp/notes/AllNotesReport.jsp" />
          <forward name="requiredStateError" path="/WEB-INF/jsp/notes/AllNotesReport.jsp"/>
          <forward name="finalStateError" path="/WEB-INF/jsp/notes/AllNotesReport.jsp"/>
      </action>

      <action path="/Help" name="HelpForm" scope="request" validate="false"
      	type="org.kuali.rice.kew.help.web.HelpAction" parameter="methodToCall" input="/WEB-INF/jsp/help/HelpEntry.jsp">
      	<forward name="basic" path="/WEB-INF/jsp/help/HelpEntry.jsp"/>
      	<forward name="summary" path="/WEB-INF/jsp/help/HelpSummary.jsp"/>
      	<forward name="getSearch" path="/WEB-INF/jsp/help/HelpSearch.jsp"/>
 		<forward name="report" path="/WEB-INF/jsp/help/HelpReport.jsp"/>
		<forward name="popHelp" path="/WEB-INF/jsp/help/HelpPop.jsp"/>
		<forward name="requiredStateError" path="/WEB-INF/jsp/help/HelpReport.jsp"/>
        <forward name="finalStateError" path="/WEB-INF/jsp/help/HelpReport.jsp"/>
        <forward name="delete" path="/WEB-INF/jsp/help/HelpDelete.jsp"/>
      </action>

	  <action path="/Ingester" name="IngesterForm" scope="request" validate="false"
      	type="org.kuali.rice.kew.batch.web.IngesterAction">
      	<forward name="view" path="/WEB-INF/jsp/batch/ingester.jsp"/>
	  </action>

	  <!-- Removing Remove/replace functionality for now, as per Application Roadmap committee, will revisit in 1.1
       <action path="/RemoveReplace" name="RemoveReplaceForm" scope="request" validate="true"
         input="/WEB-INF/jsp/removereplace/RemoveReplaceEntry.jsp"
         type="org.kuali.rice.kew.removereplace.web.RemoveReplaceAction" parameter="methodToCall">
         <forward name="basic" path="/WEB-INF/jsp/removereplace/RemoveReplaceEntry.jsp"/>
         <forward name="report" path="/WEB-INF/jsp/removereplace/RemoveReplaceReport.jsp"/>
         <forward name="summary" path="/WEB-INF/jsp/removereplace/RemoveReplaceReport.jsp"/>
         <forward name="docHandler" path="/WEB-INF/jsp/removereplace/RemoveReplaceReport.jsp" />
         <forward name="actionTaken" path="/WEB-INF/jsp/DocHandlerActionTaken.jsp" />
         <forward name="requiredStateError" path="/WEB-INF/jsp/removereplace/RemoveReplaceEntry.jsp"/>
         <forward name="finalStateError" path="/WEB-INF/jsp/removereplace/RemoveReplaceEntry.jsp"/>
       </action>
     -->

    </action-mappings>

	<!-- =================== controller definitions ======================== -->
    <controller processorClass="org.kuali.rice.kew.web.StrutsRequestProcessor" />

	<!-- ======== message resource Definitions ======== -->
    <message-resources parameter="org.kuali.rice.kew.ApplicationResources" />

	<!-- ======== plug-in definitions ======== -->
</struts-config>