<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.kra.bo.CreditType">

        <inquiry>
            <title>Credit Type</title>

            <inquirySections><inquirySection title="Credit Type"><inquiryFields>
                <field attributeName="creditTypeCode" forceInquiry="true" />
                <field attributeName="description" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Credit Type Lookup</title>
			<menubar>
				<![CDATA[<a href="/SampleRiceClient/index.html">Main</a>]]>
			</menubar>
            <instructions>Look up a Credit Type</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="creditTypeCode" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="creditTypeCode" required="false" />
                <lookupField attributeName="addsToHundred" required="false" />
                <lookupField attributeName="description" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="creditTypeCode" forceInquiry="true" />
                <field attributeName="addsToHundred" forceInquiry="true" />
                <field attributeName="description" forceInquiry="false"/>
            </resultFields>
        </lookup>

        <titleAttribute>creditTypeCode</titleAttribute>

        <attributes>

            <attribute name="creditTypeCode">
                <label>Credit Type Code</label>
                <shortLabel>Type</shortLabel>

                <maxLength>7</maxLength>
                <validationPattern><alphaNumeric /></validationPattern>

                <control>
                    <text size="6" />
                </control>

                <summary>Credit Type Code</summary>
                <description>Credit Type Code</description>
			</attribute>

            <attributeReference name="addsToHundred" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericBoolean">
                <label>Adds To Hundred </label>
                <shortLabel>Adds To Hundred</shortLabel>
                <summary>Adds To Hundred</summary>
                <description>Adds to Hundred description...</description>
			</attributeReference>

            <attributeReference name="description" sourceClassName="org.kuali.kra.bo.KraAttributeReferenceDummy" sourceAttributeName="description" />

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />
		</attributes>
    </businessObject>

</dictionaryEntry>