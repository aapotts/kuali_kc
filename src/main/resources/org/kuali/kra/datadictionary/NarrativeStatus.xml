<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.kra.proposaldevelopment.bo.NarrativeStatus">

        <inquiry>
            <title>Narrative Status</title>

            <inquirySections><inquirySection title="Narrative Status"><inquiryFields>
            	<field attributeName="narrativeStatusCode" forceInquiry="true"/>
				<field attributeName="description"/>

            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Narrative Status Lookup</title>
			<menubar>
				<![CDATA[<a href="index.jsp">Main</a>]]>
			</menubar>
            <instructions>Look up a Narrative Status</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                	<sortAttribute attributeName="narrativeStatusCode"/>

                </sortAttributes>
            </defaultSort>

            <lookupFields>
            	<lookupField attributeName="narrativeStatusCode" required="false"/>
				<lookupField attributeName="description" required="false"/>

            </lookupFields>

            <resultFields>
            	<field attributeName="narrativeStatusCode" forceInquiry="true"/>
				<field attributeName="description" forceInquiry="false"/>

            </resultFields>
        </lookup>

        <titleAttribute>narrativeStatusCode</titleAttribute>
        <attributes>

	        <attribute name="narrativeStatusCode">
			<label>Narrative Status Code </label>
			<shortLabel>Narrative Status Code </shortLabel>
			<maxLength>3</maxLength>
			<validationPattern><anyCharacter /></validationPattern>
			<control>
				<text size="3" />
			</control>
			<summary>Narrative Status Code </summary>
			<description>Narrative Status Code </description>
			</attribute>

			<attribute name="description">
			<label>Description </label>
			<shortLabel>Description </shortLabel>
			<maxLength>20</maxLength>
			<validationPattern><anyCharacter /></validationPattern>
			<control>
				<text size="20" />
			</control>
			<summary>Description </summary>
			<description>Description </description>
			</attribute>
<attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

		</attributes>
    </businessObject>

</dictionaryEntry>
