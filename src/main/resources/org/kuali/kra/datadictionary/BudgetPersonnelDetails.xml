<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.kra.budget.bo.BudgetPersonnelDetails">

        <inquiry>
            <title>Personnel Budget Details</title>
			<inquirableClass>org.kuali.kra.budget.inquiry.BudgetPersonnelDetailsInquirable</inquirableClass>
            <inquirySections><inquirySection title="Personnel Budget Details"><inquiryFields>
				<field attributeName="underrecoveryAmount"/>
				<field attributeName="costSharingAmount"/>
				<field attributeName="costSharingPercent"/>
				<field attributeName="startDate"/>
				<field attributeName="endDate"/>
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>


        <lookup>
            <title>Budget Personnel Details Lookup</title>
			<menubar>
				<![CDATA[<a href="index.jsp">Main</a>]]>
			</menubar>
            <instructions>Look up a Budget Personnel Details</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                	<sortAttribute attributeName="budgetPeriod"/>
					<sortAttribute attributeName="lineItemNumber"/>
					<sortAttribute attributeName="personNumber"/>
					<sortAttribute attributeName="proposalNumber"/>
					<sortAttribute attributeName="versionNumber"/>

                </sortAttributes>
            </defaultSort>

            <lookupFields>
            	<lookupField attributeName="budgetPeriod" required="false"/>
				<lookupField attributeName="lineItemNumber" required="false"/>
				<lookupField attributeName="personNumber" required="false"/>
				<lookupField attributeName="proposalNumber" required="false"/>
				<lookupField attributeName="versionNumber" required="false"/>
				<lookupField attributeName="applyInRateFlag" required="false"/>
				<lookupField attributeName="budgetJustification" required="false"/>
				<lookupField attributeName="costSharingAmount" required="false"/>
				<lookupField attributeName="costSharingPercent" required="false"/>
				<lookupField attributeName="endDate" required="false"/>
				<lookupField attributeName="jobCode" required="false"/>
				<lookupField attributeName="lineItemDescription" required="false"/>
				<lookupField attributeName="onOffCampusFlag" required="false"/>
				<lookupField attributeName="percentCharged" required="false"/>
				<lookupField attributeName="percentEffort" required="false"/>
				<lookupField attributeName="periodTypeCode" required="false"/>
				<lookupField attributeName="personId" required="false"/>
				<lookupField attributeName="salaryRequested" required="false"/>
				<lookupField attributeName="sequenceNumber" required="false"/>
				<lookupField attributeName="startDate" required="false"/>
				<lookupField attributeName="underrecoveryAmount" required="false"/>

            </lookupFields>

            <resultFields>
            	<field attributeName="budgetPeriod" forceInquiry="true"/>
				<field attributeName="lineItemNumber" forceInquiry="true"/>
				<field attributeName="personNumber" forceInquiry="true"/>
				<field attributeName="proposalNumber" forceInquiry="true"/>
				<field attributeName="versionNumber" forceInquiry="true"/>
				<field attributeName="applyInRateFlag" forceInquiry="false"/>
				<field attributeName="budgetJustification" forceInquiry="false"/>
				<field attributeName="costSharingAmount" forceInquiry="false"/>
				<field attributeName="costSharingPercent" forceInquiry="false"/>
				<field attributeName="endDate" forceInquiry="false"/>
				<field attributeName="jobCode" forceInquiry="false"/>
				<field attributeName="lineItemDescription" forceInquiry="false"/>
				<field attributeName="onOffCampusFlag" forceInquiry="false"/>
				<field attributeName="percentCharged" forceInquiry="false"/>
				<field attributeName="percentEffort" forceInquiry="false"/>
				<field attributeName="periodTypeCode" forceInquiry="false"/>
				<field attributeName="personId" forceInquiry="false"/>
				<field attributeName="salaryRequested" forceInquiry="false"/>
				<field attributeName="sequenceNumber" forceInquiry="false"/>
				<field attributeName="startDate" forceInquiry="false"/>
				<field attributeName="underrecoveryAmount" forceInquiry="false"/>

            </resultFields>
        </lookup>

        <titleAttribute>budgetPeriod</titleAttribute>
		<!-- <titleAttribute>lineItemNumber</titleAttribute>
		<titleAttribute>personNumber</titleAttribute>
		<titleAttribute>proposalNumber</titleAttribute>
		<titleAttribute>versionNumber</titleAttribute> -->
        <attributes>

	        <attribute name="budgetPeriod">
			<label>Budget Period </label>
			<shortLabel>Budget Period </shortLabel>
			<maxLength>3</maxLength>
			<validationPattern><numeric /></validationPattern>
			<control>
				<text size="3" />
			</control>
			<summary>Budget Period </summary>
			<description>Budget Period </description>
			</attribute>

			<attribute name="lineItemNumber">
			<label>Line Item Number </label>
			<shortLabel>Line Item Number </shortLabel>
			<maxLength>3</maxLength>
			<validationPattern><numeric /></validationPattern>
			<control>
				<text size="3" />
			</control>
			<summary>Line Item Number </summary>
			<description>Line Item Number </description>
			</attribute>

			<attribute name="personNumber">
			<label>Person Number </label>
			<shortLabel>Person Number </shortLabel>
			<maxLength>3</maxLength>
			<validationPattern><numeric /></validationPattern>
			<control>
				<text size="3" />
			</control>
			<summary>Person Number </summary>
			<description>Person Number </description>
			</attribute>

			<attribute name="proposalNumber">
			<label>Proposal Number </label>
			<shortLabel>Proposal Number </shortLabel>
			<maxLength>12</maxLength>
			<validationPattern><alphaNumeric /></validationPattern>
			<control>
				<text size="12" />
			</control>
			<summary>Proposal Number </summary>
			<description>Proposal Number </description>
			</attribute>

			<attribute name="versionNumber">
			<label>Version Number </label>
			<shortLabel>Version Number </shortLabel>
			<maxLength>3</maxLength>
			<validationPattern><numeric /></validationPattern>
			<control>
				<text size="3" />
			</control>
			<summary>Version Number </summary>
			<description>Version Number </description>
			</attribute>

			<attribute name="applyInRateFlag">
			<label>Apply In Rate Flag </label>
			<shortLabel>Apply In Rate Flag </shortLabel>
			<maxLength>1</maxLength>
			<validationPattern><alphaNumeric /></validationPattern>
			<control>
				<text size="1" />
			</control>
			<summary>Apply In Rate Flag </summary>
			<description>Apply In Rate Flag </description>
			</attribute>

			<attribute name="budgetJustification">
			<label>Budget Justification </label>
			<shortLabel>Budget Justification </shortLabel>
			<maxLength>0</maxLength>
			<validationPattern><alphaNumeric /></validationPattern>
			<control>
				<text size="0" />
			</control>
			<summary>Budget Justification </summary>
			<description>Budget Justification </description>
			</attribute>

            <attributeReference name="costSharingAmount" sourceClassName="org.kuali.kra.bo.KraAttributeReferenceDummy" sourceAttributeName="genericAmount">
			<label>Cost Sharing Amount </label>
			<shortLabel>Cost Sharing Amount </shortLabel>
			<summary>Cost Sharing Amount </summary>
			<description>Cost Sharing Amount </description>
			</attributeReference>

			<!-- can we use generic rate for percent too precision 5.2 ?? -->
            <attributeReference name="costSharingPercent" sourceClassName="org.kuali.kra.bo.KraAttributeReferenceDummy" sourceAttributeName="genericRate">
			<label>Cost Sharing Percent </label>
			<shortLabel>Cost Sharing Percent </shortLabel>
			<summary>Cost Sharing Percent </summary>
			<description>Cost Sharing Percent </description>
			</attributeReference>

            <attributeReference name="endDate" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericDate" >
                <label>End Date</label>
                <shortLabel>End Date</shortLabel>
                <required>true</required>
                <summary>End Date</summary>
                <description>End Date.</description>
      		</attributeReference>

			<attribute name="jobCode">
			<label>Job Code </label>
			<shortLabel>Job Code </shortLabel>
			<maxLength>6</maxLength>
			<validationPattern><alphaNumeric /></validationPattern>
			<control>
				<text size="6" />
			</control>
			<summary>Job Code </summary>
			<description>Job Code </description>
			</attribute>

			<attribute name="lineItemDescription">
			<label>Description </label>
			<shortLabel>Description </shortLabel>
			<maxLength>80</maxLength>
			<validationPattern><anyCharacter allowWhitespace="true"/></validationPattern>
			<control>
					<textarea rows="3" cols="30" />
			</control>
			<summary>Description </summary>
			<description>Description </description>
			</attribute>

			<attribute name="onOffCampusFlag">
			<label>On Off Campus Flag </label>
			<shortLabel>On Off Campus Flag </shortLabel>
			<maxLength>1</maxLength>
			<validationPattern><alphaNumeric /></validationPattern>
			<control>
				<text size="1" />
			</control>
			<summary>On Off Campus Flag </summary>
			<description>On Off Campus Flag </description>
			</attribute>

			<attribute name="percentCharged">
			<label>% Charged </label>
			<shortLabel>% Charged </shortLabel>
			<maxLength>6</maxLength>
			<validationPattern><floatingPoint allowNegative="false" /></validationPattern>
			<control>
				<text size="6" />
			</control>
			<summary>% Charged </summary>
			<description>% Charged </description>
			</attribute>

			<attribute name="percentEffort">
			<label>% Effort </label>
			<shortLabel>% Effort </shortLabel>
			<maxLength>6</maxLength>
			<validationPattern><floatingPoint allowNegative="false" /></validationPattern>
			<control>
				<text size="6" />
			</control>
			<summary>% Effort </summary>
			<description>% Effort </description>
			</attribute>

			<attribute name="periodTypeCode">
			<label>Period Type </label>
			<shortLabel>Period Type </shortLabel>
			<maxLength>3</maxLength>
			<validationPattern><alphaNumeric /></validationPattern>
			<required>false</required>
			<control>
				<!--<select valuesFinderClass="org.kuali.kra.budget.lookup.keyvalue.BudgetPeriodTypeValuesFinder" />  -->
				<select valuesFinderClass="org.kuali.core.lookup.keyvalues.PersistableBusinessObjectValuesFinder" businessObjectClass="org.kuali.kra.budget.bo.BudgetPeriodType" keyAttribute="budgetPeriodTypeCode" labelAttribute="description" includeKeyInLabel="false"/>
			</control>
			<summary>Period Type </summary>
			<description>Period Type </description>
			</attribute>

			<attribute name="personSequenceNumber">
			<label>Person </label>
			<shortLabel>Person </shortLabel>
			<maxLength>9</maxLength>
			<validationPattern><alphaNumeric /></validationPattern>
			<control>
				<select valuesFinderClass="org.kuali.kra.budget.lookup.keyvalue.BudgetPersonValuesFinder" />
			</control>
			<summary>Person </summary>
			<description>Person </description>
			</attribute>

			<attributeReference name="personId" sourceClassName="org.kuali.kra.bo.Person" sourceAttributeName="personId" />

            <attributeReference name="salaryRequested" sourceClassName="org.kuali.kra.bo.KraAttributeReferenceDummy" sourceAttributeName="genericAmount">
			<label>Requested Salary</label>
			<shortLabel>Requested Salary</shortLabel>
			<summary>Requested Salary</summary>
			<description>Requested Salary</description>
			</attributeReference>

			<attribute name="sequenceNumber">
			<label>Sequence Number </label>
			<shortLabel>Sequence Number </shortLabel>
			<maxLength>3</maxLength>
			<validationPattern><numeric /></validationPattern>
			<control>
				<text size="3" />
			</control>
			<summary>Sequence Number </summary>
			<description>Sequence Number </description>
			</attribute>

            <attributeReference name="startDate" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericDate" >
                <label>Start Date</label>
                <shortLabel>Start Date</shortLabel>
				<required>true</required>
                <summary>Start Date</summary>
                <description>Start Date.</description>
      		</attributeReference>

            <attributeReference name="underrecoveryAmount" sourceClassName="org.kuali.kra.bo.KraAttributeReferenceDummy" sourceAttributeName="genericAmount">
			<label>Unrecovered F&amp;A </label>
			<shortLabel>Unrecovered F&amp;A </shortLabel>
			<summary>Unrecovered F&amp;A </summary>
			<description>Unrecovered F&amp;A </description>
			</attributeReference>

		</attributes>

        <objectHelp parameterNamespace="KRA-B" parameterDetailType="D" parameterName="budgetPersonnelDetailsHelpUrl"/> 
    </businessObject>

</dictionaryEntry>
