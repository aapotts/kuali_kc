<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.kra.budget.bo.RateClassType">

        <inquiry>
            <title>Rate Class Type</title>

            <inquirySections><inquirySection title="Rate Class Type"><inquiryFields>
            	<field attributeName="rateClassType" forceInquiry="true"/>
				<field attributeName="description"/>
				<field attributeName="sortId"/>
				<field attributeName="prefixActivityType"/>

            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Rate Class Type Lookup</title>
			<menubar>
				<![CDATA[<a href="index.jsp">Main</a>]]>
			</menubar>
            <instructions>Look up a Rate Class Type</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                	<sortAttribute attributeName="rateClassType"/>

                </sortAttributes>
            </defaultSort>

            <lookupFields>
            	<lookupField attributeName="rateClassType" required="false"/>
				<lookupField attributeName="description" required="false"/>
				<lookupField attributeName="prefixActivityType" />

            </lookupFields>

            <resultFields>
            	<field attributeName="rateClassType" forceInquiry="true"/>
				<field attributeName="description" forceInquiry="false"/>
				<field attributeName="sortId" forceInquiry="false"/>
				<field attributeName="prefixActivityType" forceInquiry="false"/>

            </resultFields>
        </lookup>

        <titleAttribute>rateClassType</titleAttribute>
        <attributes>

	        <attribute name="rateClassType">
			<label>Rate Class Type </label>
			<shortLabel>Rate Class Type </shortLabel>
			<maxLength>1</maxLength>
			<validationPattern><alphaNumeric/></validationPattern>
			<control>
				<text size="1" />
			</control>
			<summary>Rate Class Type </summary>
			<description>Rate Class Type </description>
			</attribute>

            <attributeReference name="prefixActivityType" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericBoolean">
                <label>Prefix Activity Type?</label>
                <shortLabel>Prefix Activity Type?</shortLabel>
                <summary>Prefix Activity Type</summary>
                <description>Prefix Activity Type in rates page</description>
			</attributeReference>
            <attribute name="sortId">
                <label>Sort Id</label>
                <shortLabel>Sort Id</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="3" />
                </control>

                <summary>Sort Id</summary>
                <description>This sort id is used for sorting budget category.</description>
			</attribute>

            <attributeReference name="description" sourceClassName="org.kuali.kra.bo.KraAttributeReferenceDummy" sourceAttributeName="description" />

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

		</attributes>
    </businessObject>

</dictionaryEntry>
