<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.kra.budget.bo.BudgetPeriodType">

        <inquiry>
            <title>Budget Period Type</title>

            <inquirySections><inquirySection title="Budget Period Type"><inquiryFields>
            	<field attributeName="budgetPeriodTypeCode" forceInquiry="true"/>
				<field attributeName="description"/>

            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Budget Period Type Lookup</title>
			<menubar>
				<![CDATA[<a href="index.jsp">Main</a>]]>
			</menubar>
            <instructions>Look up a Budget Period Type</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                	<sortAttribute attributeName="budgetPeriodTypeCode"/>

                </sortAttributes>
            </defaultSort>

            <lookupFields>
            	<lookupField attributeName="budgetPeriodTypeCode" required="false"/>
				<lookupField attributeName="description" required="false"/>

            </lookupFields>

            <resultFields>
            	<field attributeName="budgetPeriodTypeCode" forceInquiry="true"/>
				<field attributeName="description" forceInquiry="false"/>

            </resultFields>
        </lookup>

        <titleAttribute>budgetPeriodTypeCode</titleAttribute>
        <attributes>

	        <attribute name="budgetPeriodTypeCode">
			<label>Budget Period Type Code </label>
			<shortLabel>Budget Period Type Code </shortLabel>
			<maxLength>3</maxLength>
			<validationPattern><numeric /></validationPattern>
			<control>
				<text size="3" />
			</control>
			<summary>Budget Period Type Code </summary>
			<description>Budget Period Type Code </description>
			</attribute>

			<attribute name="description">
			<label>Description </label>
			<shortLabel>Description </shortLabel>
			<maxLength>200</maxLength>
			<validationPattern><anyCharacter allowWhitespace="true"/></validationPattern>
			<control>
				<text size="200" />
			</control>
			<summary>Description </summary>
			<description>Description </description>
			</attribute>

		</attributes>
    </businessObject>

</dictionaryEntry>
