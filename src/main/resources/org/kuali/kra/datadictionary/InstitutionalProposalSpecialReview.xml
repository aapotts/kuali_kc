<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

    <bean id="InstitutionalProposalSpecialReview" parent="InstitutionalProposalSpecialReview-parentBean" />
    <bean id="InstitutionalProposalSpecialReview-parentBean" abstract="true" parent="BusinessObjectEntry">
        <property name="businessObjectClass" value="org.kuali.kra.institutionalproposal.home.InstitutionalProposalSpecialReview" />
        <property name="inquiryDefinition" >
            <ref bean="InstitutionalProposalSpecialReview-inquiryDefinition" />
        </property>
        <property name="lookupDefinition" >
            <ref bean="InstitutionalProposalSpecialReview-lookupDefinition" />
        </property>
        <property name="titleAttribute" value="proposalSpecialReviewId" />
        <property name="attributes" >
            <list>
                <ref bean="InstitutionalProposalSpecialReview-proposalSpecialReviewId" />
                <ref bean="InstitutionalProposalSpecialReview-proposalId" />
                <ref bean="InstitutionalProposalSpecialReview-proposalNumber" />
                <ref bean="InstitutionalProposalSpecialReview-sequenceNumber" />
                <ref bean="InstitutionalProposalSpecialReview-specialReviewNumber" />
                <ref bean="InstitutionalProposalSpecialReview-specialReviewCode" />
                <ref bean="InstitutionalProposalSpecialReview-approvalTypeCode" />
                <ref bean="InstitutionalProposalSpecialReview-protocolNumber" />
                <ref bean="InstitutionalProposalSpecialReview-applicationDate" />
                <ref bean="InstitutionalProposalSpecialReview-approvalDate" />
                <ref bean="InstitutionalProposalSpecialReview-comments" />
                <ref bean="InstitutionalProposalSpecialReview-versionNumber" />
            </list>
        </property>
    </bean>

    <!-- Attribute Definitions -->
    
    <bean id="InstitutionalProposalSpecialReview-proposalSpecialReviewId" parent="InstitutionalProposalSpecialReview-proposalSpecialReviewId-parentBean" />
    <bean id="InstitutionalProposalSpecialReview-proposalSpecialReviewId-parentBean" abstract="true" parent="AttributeDefinition" >
        <property name="name" value="proposalSpecialReviewId" />
        <property name="forceUppercase" value="false" />
        <property name="label" value="Proposal Special Review Id" />
        <property name="shortLabel" value="Proposal Special Review Id" />
        <property name="maxLength" value="22" />
        <property name="validationPattern" >
            <bean parent="NumericValidationPattern" />
        </property>
        <property name="control" >
            <bean parent="TextControlDefinition"
                p:size="10" />
        </property>
        <property name="summary" value="Proposal Special Review Id" />
        <property name="description" value="Proposal Special Review Id" />
    </bean>

    <bean id="InstitutionalProposalSpecialReview-proposalId" parent="InstitutionalProposalSpecialReview-proposalId-parentBean" />
    <bean id="InstitutionalProposalSpecialReview-proposalId-parentBean" abstract="true" parent="AttributeDefinition" >
        <property name="name" value="proposalId" />
        <property name="forceUppercase" value="false" />
        <property name="label" value="Proposal Id" />
        <property name="shortLabel" value="Proposal Id" />
        <property name="maxLength" value="22" />
        <property name="validationPattern" >
            <bean parent="NumericValidationPattern" />
        </property>
        <property name="control" >
            <bean parent="TextControlDefinition"
                p:size="10" />
        </property>
        <property name="summary" value="Proposal Id" />
        <property name="description" value="Proposal Id" />
    </bean>

    <bean id="InstitutionalProposalSpecialReview-proposalNumber" parent="InstitutionalProposalSpecialReview-proposalNumber-parentBean" />
    <bean id="InstitutionalProposalSpecialReview-proposalNumber-parentBean" abstract="true" parent="AttributeDefinition" >
        <property name="name" value="proposalNumber" />
        <property name="forceUppercase" value="false" />
        <property name="label" value="Proposal Number" />
        <property name="shortLabel" value="Proposal Number" />
        <property name="maxLength" value="8" />
        <property name="validationPattern" >
            <bean parent="AlphaNumericValidationPattern" />
        </property>
        <property name="control" >
            <bean parent="TextControlDefinition"
                p:size="10" />
        </property>
        <property name="summary" value="Proposal Number" />
        <property name="description" value="Proposal Number" />
    </bean>

    <bean id="InstitutionalProposalSpecialReview-sequenceNumber" parent="InstitutionalProposalSpecialReview-sequenceNumber-parentBean" />
    <bean id="InstitutionalProposalSpecialReview-sequenceNumber-parentBean" abstract="true" parent="AttributeDefinition" >
        <property name="name" value="sequenceNumber" />
        <property name="forceUppercase" value="false" />
        <property name="label" value="Sequence Number" />
        <property name="shortLabel" value="Sequence Number" />
        <property name="maxLength" value="22" />
        <property name="validationPattern" >
            <bean parent="NumericValidationPattern" />
        </property>
        <property name="control" >
            <bean parent="TextControlDefinition"
                p:size="10" />
        </property>
        <property name="summary" value="Sequence Number" />
        <property name="description" value="Sequence Number" />
    </bean>

    <bean id="InstitutionalProposalSpecialReview-specialReviewNumber" parent="InstitutionalProposalSpecialReview-specialReviewNumber-parentBean" />
    <bean id="InstitutionalProposalSpecialReview-specialReviewNumber-parentBean" abstract="true" parent="AttributeDefinition" >
        <property name="name" value="specialReviewNumber" />
        <property name="forceUppercase" value="false" />
        <property name="label" value="Special Review Number" />
        <property name="shortLabel" value="Special Review Number" />
        <property name="maxLength" value="22" />
        <property name="validationPattern" >
            <bean parent="NumericValidationPattern" />
        </property>
        <property name="control" >
            <bean parent="TextControlDefinition"
                p:size="10" />
        </property>
        <property name="summary" value="Special Review Number" />
        <property name="description" value="Special Review Number" />
    </bean>

    <bean id="InstitutionalProposalSpecialReview-specialReviewCode" parent="InstitutionalProposalSpecialReview-specialReviewCode-parentBean" />
    <bean id="InstitutionalProposalSpecialReview-specialReviewCode-parentBean" abstract="true" parent="AttributeDefinition" >
        <property name="name" value="specialReviewCode" />
        <property name="forceUppercase" value="false" />
        <property name="label" value="Special Review Code" />
        <property name="shortLabel" value="Special Review Code" />
        <property name="maxLength" value="3" />
        <property name="validationPattern" >
            <bean parent="AlphaNumericValidationPattern" />
        </property>
        <property name="control" >
            <bean parent="TextControlDefinition"
                p:size="5" />
        </property>
        <property name="summary" value="Special Review Code" />
        <property name="description" value="Special Review Code" />
    </bean>

    <bean id="InstitutionalProposalSpecialReview-approvalTypeCode" parent="InstitutionalProposalSpecialReview-approvalTypeCode-parentBean" />
    <bean id="InstitutionalProposalSpecialReview-approvalTypeCode-parentBean" abstract="true" parent="AttributeDefinition" >
        <property name="name" value="approvalTypeCode" />
        <property name="forceUppercase" value="false" />
        <property name="label" value="Approval Type Code" />
        <property name="shortLabel" value="Approval Type Code" />
        <property name="maxLength" value="3" />
        <property name="validationPattern" >
            <bean parent="AlphaNumericValidationPattern" />
        </property>
        <property name="control" >
            <bean parent="TextControlDefinition"
                p:size="5" />
        </property>
        <property name="summary" value="Approval Type Code" />
        <property name="description" value="Approval Type Code" />
    </bean>

    <bean id="InstitutionalProposalSpecialReview-protocolNumber" parent="InstitutionalProposalSpecialReview-protocolNumber-parentBean" />
    <bean id="InstitutionalProposalSpecialReview-protocolNumber-parentBean" abstract="true" parent="AttributeDefinition" >
        <property name="name" value="protocolNumber" />
        <property name="forceUppercase" value="false" />
        <property name="label" value="Protocol Number" />
        <property name="shortLabel" value="Protocol Number" />
        <property name="maxLength" value="20" />
        <property name="validationPattern" >
            <bean parent="AlphaNumericValidationPattern" />
        </property>
        <property name="control" >
            <bean parent="TextControlDefinition"
                p:size="10" />
        </property>
        <property name="summary" value="Protocol Number" />
        <property name="description" value="Protocol Number" />
    </bean>

    <bean id="InstitutionalProposalSpecialReview-applicationDate" parent="InstitutionalProposalSpecialReview-applicationDate-parentBean" />
    <bean id="InstitutionalProposalSpecialReview-applicationDate-parentBean" abstract="true" parent="KraAttributeReferenceDummy-genericDate" >
        <property name="name" value="applicationDate" />
        <property name="label" value="Application Date" />
        <property name="shortLabel" value="Application Date" />
        <property name="summary" value="Application Date" />
        <property name="description" value="Application Date" />
    </bean>

    <bean id="InstitutionalProposalSpecialReview-approvalDate" parent="InstitutionalProposalSpecialReview-approvalDate-parentBean" />
    <bean id="InstitutionalProposalSpecialReview-approvalDate-parentBean" abstract="true" parent="KraAttributeReferenceDummy-genericDate" >
        <property name="name" value="approvalDate" />
        <property name="label" value="Approval Date" />
        <property name="shortLabel" value="Approval Date" />
        <property name="summary" value="Approval Date" />
        <property name="description" value="Approval Date" />
    </bean>

    <bean id="InstitutionalProposalSpecialReview-comments" parent="InstitutionalProposalSpecialReview-comments-parentBean" />
    <bean id="InstitutionalProposalSpecialReview-comments-parentBean" abstract="true" parent="AttributeDefinition" >
        <property name="name" value="comments" />
        <property name="forceUppercase" value="false" />
        <property name="label" value="Comments" />
        <property name="shortLabel" value="Comments" />
        <property name="maxLength" value="4000" />
        <property name="validationPattern" >
            <bean parent="AlphaNumericValidationPattern" />
        </property>
        <property name="control" >
            <bean parent="TextareaControlDefinition"
                p:rows="3"
                p:cols="20" />
        </property>
        <property name="summary" value="Comments" />
        <property name="description" value="Comments" />
    </bean>

    <bean id="InstitutionalProposalSpecialReview-versionNumber" parent="InstitutionalProposalSpecialReview-versionNumber-parentBean" />
    <bean id="InstitutionalProposalSpecialReview-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
    </bean>

    <!-- Business Object Inquiry Definition -->
    <bean id="InstitutionalProposalSpecialReview-inquiryDefinition" parent="InstitutionalProposalSpecialReview-inquiryDefinition-parentBean" />
    <bean id="InstitutionalProposalSpecialReview-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
        <property name="title" value="Proposal Special Review Id" />
        <property name="inquirySections" >
            <list>
                <bean parent="InquirySectionDefinition">
                    <property name="title" value="Proposal Special Review Id" />
                    <property name="numberOfColumns" value="1" />
                    <property name="inquiryFields" >
                        <list>
                            <bean parent="FieldDefinition"
                                p:attributeName="proposalSpecialReviewId" 
                                p:forceInquiry="true" />
                            <bean parent="FieldDefinition"
                                p:attributeName="proposalId" />
                            <bean parent="FieldDefinition"
                                p:attributeName="proposalNumber" />
                            <bean parent="FieldDefinition"
                                p:attributeName="sequenceNumber" />
                            <bean parent="FieldDefinition"
                                p:attributeName="specialReviewNumber" />
                            <bean parent="FieldDefinition"
                                p:attributeName="specialReviewCode" />
                            <bean parent="FieldDefinition"
                                p:attributeName="approvalTypeCode" />
                            <bean parent="FieldDefinition"
                                p:attributeName="protocolNumber" />
                            <bean parent="FieldDefinition"
                                p:attributeName="applicationDate" />
                            <bean parent="FieldDefinition"
                                p:attributeName="approvalDate" />
                            <bean parent="FieldDefinition"
                                p:attributeName="comments" />
                        </list>
                    </property>
                </bean>
            </list>
        </property>
    </bean>

    <!-- Business Object Lookup Definition -->
    <bean id="InstitutionalProposalSpecialReview-lookupDefinition" parent="InstitutionalProposalSpecialReview-lookupDefinition-parentBean" />
    <bean id="InstitutionalProposalSpecialReview-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
        <property name="title" value="Proposal Special Review Lookup" />
        <property name="menubar" value="&lt;a href=&quot;index.jsp&quot;&gt;Main&lt;/a&gt;" />
        <property name="instructions" value="Look up a Proposal Special Review" />
        <property name="defaultSort" >
            <bean parent="SortDefinition" >
        </bean>
        </property>
        <property name="lookupFields" >
            <list>
                <bean parent="FieldDefinition"
                    p:attributeName="proposalSpecialReviewId" 
                    p:forceInquiry="true" />
                <bean parent="FieldDefinition"
                    p:attributeName="proposalId" />
                <bean parent="FieldDefinition"
                    p:attributeName="proposalNumber" />
                <bean parent="FieldDefinition"
                    p:attributeName="sequenceNumber" />
                <bean parent="FieldDefinition"
                    p:attributeName="specialReviewNumber" />
                <bean parent="FieldDefinition"
                    p:attributeName="specialReviewCode" />
                <bean parent="FieldDefinition"
                    p:attributeName="approvalTypeCode" />
                <bean parent="FieldDefinition"
                    p:attributeName="protocolNumber" />
                <bean parent="FieldDefinition"
                    p:attributeName="applicationDate" />
                <bean parent="FieldDefinition"
                    p:attributeName="approvalDate" />
                <bean parent="FieldDefinition"
                    p:attributeName="comments" />
            </list>
        </property>
        <property name="resultFields" >
            <list>
                <bean parent="FieldDefinition"
                    p:attributeName="proposalSpecialReviewId" 
                    p:forceInquiry="true" />
                <bean parent="FieldDefinition"
                    p:attributeName="proposalId" />
                <bean parent="FieldDefinition"
                    p:attributeName="proposalNumber" />
                <bean parent="FieldDefinition"
                    p:attributeName="sequenceNumber" />
                <bean parent="FieldDefinition"
                    p:attributeName="specialReviewNumber" />
                <bean parent="FieldDefinition"
                    p:attributeName="specialReviewCode" />
                <bean parent="FieldDefinition"
                    p:attributeName="approvalTypeCode" />
                <bean parent="FieldDefinition"
                    p:attributeName="protocolNumber" />
                <bean parent="FieldDefinition"
                    p:attributeName="applicationDate" />
                <bean parent="FieldDefinition"
                    p:attributeName="approvalDate" />
                <bean parent="FieldDefinition"
                    p:attributeName="comments" />
            </list>
        </property>
    </bean>
</beans>
