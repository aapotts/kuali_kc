<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="AwardPrintChangeReport" parent="AwardPrintChangeReport-parentBean" />

  <bean id="AwardPrintChangeReport-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kra.award.printing.AwardPrintChangeReport" />
    <property name="titleAttribute" value="AwardPrintChangeReport" />
    <property name="attributes" >
      <list>
		<ref bean="AwardPrintChangeReport-awardVersion" />
		<ref bean="AwardPrintChangeReport-transactionId" />
        <ref bean="AwardPrintChangeReport-requireSignature" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->

  <bean id="AwardPrintChangeReport-awardVersion" parent="AwardPrintChangeReport-awardVersion-parentBean" />
  <bean id="AwardPrintChangeReport-awardVersion-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="awardVersion" />
    <property name="label" value="Award Version" />
    <property name="shortLabel" value="Award Version" />
    <property name="maxLength" value="4" />
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.kra.award.lookup.keyvalue.AwardSequenceNumberValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
    <property name="summary" value="Award Version" />
    <property name="description" value="Award Version" />
  </bean>    
  
  <bean id="AwardPrintChangeReport-transactionId" parent="AwardPrintChangeReport-transactionId-parentBean" />
  <bean id="AwardPrintChangeReport-transactionId-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="transactionId" />
    <property name="label" value="Transaction" />
    <property name="shortLabel" value="Transaction" />
    <property name="maxLength" value="10" />
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.kra.award.lookup.keyvalue.AwardTransactionValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
    <property name="summary" value="Transaction" />
    <property name="description" value="Transaction" />
  </bean>      


  
  <bean id="AwardPrintChangeReport-requireSignature" parent="AwardPrintChangeReport-requireSignature-parentBean" />
  <bean id="AwardPrintChangeReport-requireSignature-parentBean" abstract="true" parent="AttributeReferenceDummy-genericBoolean">
    <property name="name" value="requireSignature" />
    <property name="label" value="Signature Required" />
    <property name="shortLabel" value="Signature Required" />
    <property name="summary" value="Signature Required" />
    <property name="description" value="Signature Required" />
  </bean>                        
                          
 

</beans>
