<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="SpecialReview" parent="SpecialReview-parentBean" />

  <bean id="SpecialReview-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kra.bo.SpecialReview" />
		<property name="objectLabel" value="Special Review" />
    <property name="inquiryDefinition" >
      <ref bean="SpecialReview-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="SpecialReview-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="specialReviewCode" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KC-PD"
            p:parameterName="proposalDevelopmentSpecialReviewHelpUrl"
            p:parameterDetailType="D" />
    </property>
    <property name="attributes" >
      <list>
        <ref bean="SpecialReview-specialReviewCode" />
        <ref bean="SpecialReview-description" />
        <ref bean="SpecialReview-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="SpecialReview-specialReviewCode" parent="SpecialReview-specialReviewCode-parentBean" />

  <bean id="SpecialReview-specialReviewCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="specialReviewCode" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Special Review Code" />
    <property name="shortLabel" value="Special Review Code" />
    <property name="maxLength" value="3" />
    <property name="validationPattern" >
      <bean parent="AlphaNumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
    <property name="summary" value="Special Review Code" />
    <property name="description" value="Special Review Code" />
  </bean>
  <bean id="SpecialReview-description" parent="SpecialReview-description-parentBean" />

  <bean id="SpecialReview-description-parentBean" abstract="true" parent="KraAttributeReferenceDummy-description">
  </bean>
  <bean id="SpecialReview-versionNumber" parent="SpecialReview-versionNumber-parentBean" />

  <bean id="SpecialReview-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="SpecialReview-inquiryDefinition" parent="SpecialReview-inquiryDefinition-parentBean" />

  <bean id="SpecialReview-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Special Review Approval Type" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Special Review Approval Type" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="specialReviewCode"
                    p:forceInquiry="true" />
              <bean parent="FieldDefinition"
                    p:attributeName="description" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="SpecialReview-lookupDefinition" parent="SpecialReview-lookupDefinition-parentBean" />

  <bean id="SpecialReview-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Special Review Approval Type Lookup" />
    <property name="menubar" value="&lt;a href=&quot;index.jsp&quot;&gt;Main&lt;/a&gt;" />

    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="specialReviewCode" />
        <bean parent="FieldDefinition"
              p:attributeName="description" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="specialReviewCode"
              p:forceInquiry="true" />
        <bean parent="FieldDefinition"
              p:attributeName="description" />
      </list>
    </property>
  </bean>
</beans>
