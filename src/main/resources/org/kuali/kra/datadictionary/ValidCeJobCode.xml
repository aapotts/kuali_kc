<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.kra.budget.bo.ValidCeJobCode">

        <inquiry>
            <title>Valid Ce Job Codes</title>

            <inquirySections><inquirySection title="Valid Ce Job Codes"><inquiryFields>
            	<field attributeName="costElement" forceInquiry="true"/>
				<field attributeName="jobCode" forceInquiry="true"/>

            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Valid Ce Job Codes Lookup</title>
			<menubar>
				<![CDATA[<a href="index.jsp">Main</a>]]>
			</menubar>
            <instructions>Look up a Valid Ce Job Codes</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                	<sortAttribute attributeName="costElement"/>
					<sortAttribute attributeName="jobCode"/>

                </sortAttributes>
            </defaultSort>

            <lookupFields>
            	<lookupField attributeName="costElement" required="false"/>
				<lookupField attributeName="jobCode" required="false"/>

            </lookupFields>

            <resultFields>
            	<field attributeName="costElement" forceInquiry="true"/>
				<field attributeName="jobCode" forceInquiry="true"/>

            </resultFields>
        </lookup>

        <titleAttribute>costElement</titleAttribute>
		<!-- <titleAttribute>jobCode</titleAttribute> -->
        <attributes>

	        <attribute name="costElement">
			<label>Cost Element </label>
			<shortLabel>Cost Element </shortLabel>
			<maxLength>8</maxLength>
			<validationPattern><alphaNumeric /></validationPattern>
			<control>
				<text size="8" />
			</control>
			<summary>Cost Element </summary>
			<description>Cost Element </description>
			</attribute>

			<attribute name="jobCode">
			<label>Job Code </label>
			<shortLabel>Job Code </shortLabel>
			<maxLength>6</maxLength>
			<validationPattern><alphaNumeric /></validationPattern>
			<control>
				<text size="6" />
			</control>
			<summary>Job Code </summary>
			<description>Job Code </description>
			</attribute>

			<attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

		</attributes>
    </businessObject>

</dictionaryEntry>
