<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.kra.kim.bo.KraQualifiedRolePerson">

        <inquiry>
            <title>KRA Qualified Role Person</title>

            <inquirySections><inquirySection title="KraQualifiedRolePerson"><inquiryFields>
                <field attributeName="id" forceInquiry="true"/>
                <field attributeName="roleId"/>
                <field attributeName="kraPersonId"/>
                <field attributeName="unitNumber"/>
                <field attributeName="subUnitsAllowed"/>
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>KRA Qualified Role Person Lookup</title>
            <menubar>
                <![CDATA[<a href="index.jsp">Main</a>]]>
            </menubar>
            <instructions>Look up a KRA Qualified Role Person</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="id"/>
                    <sortAttribute attributeName="roleId"/>
                    <sortAttribute attributeName="kraPersonId"/>
                    <sortAttribute attributeName="unit.unitName"/>
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="id" required="false"/>
                <lookupField attributeName="roleId" required="false"/>
                <lookupField attributeName="kraPersonId" required="false"/>
                <lookupField attributeName="unitNumber" required="false"/>
                <lookupField attributeName="subUnitsAllowed" required="false"/>
            </lookupFields>

            <resultFields>
                <field attributeName="id" forceInquiry="true"/>
                <field attributeName="roleId" forceInquiry="false"/>
                <field attributeName="kraPersonId" forceInquiry="false"/>
                <field attributeName="unit.unitName" forceInquiry="false"/>
                <field attributeName="subUnitsAllowed" forceInquiry="false"/>
            </resultFields>
        </lookup>

        <titleAttribute>id</titleAttribute>
        <attributes>
            <attribute name="id">
                <label>Qualified Role Person Id </label>
                <shortLabel>Quali Role Pers Id </shortLabel>
                <maxLength>8</maxLength>
                <validationPattern><numeric /></validationPattern>
                <control>
                    <text size="8" />
                </control>
                <summary>Qualified Role Person Id </summary>
                <description>Qualified Role Person Id </description>
            </attribute>

            <attributeReference name="roleId" sourceClassName="org.kuali.kra.kim.bo.KimRole" sourceAttributeName="id">
                <label>KIM Role Id</label>
                <shortLabel>KIM Role Id</shortLabel>
                <summary>KIM Person Id </summary>
                <description>KIM Person Id </description>
            </attributeReference>

            <attribute name="kraPersonId">
                <label>KRA Person Id </label>
                <shortLabel>KRA Person Id </shortLabel>
                <maxLength>8</maxLength>
                <validationPattern><numeric /></validationPattern>
                <control>
                    <text size="8" />
                </control>
                <summary>KRA Person Id </summary>
                <description>KRA Person Id </description>
            </attribute>

            <attributeReference name="unitNumber" sourceClassName="org.kuali.kra.bo.Unit" sourceAttributeName="unitNumber" />

            <attributeReference name="subUnitsAllowed" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericBoolean" />

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />
        </attributes>
        
        <relationships>
            <relationship objectAttributeName="unit" >
                <primitiveAttribute sourceName="unitNumber" targetName="unitNumber" />
                <supportAttribute sourceName="unit.unitName" targetName="unitName" identifier="true" />
            </relationship>

            <relationship objectAttributeName="person" >
                <primitiveAttribute sourceName="kraPersonId" targetName="personId" />
            </relationship>
        </relationships>
    </businessObject>

</dictionaryEntry>
