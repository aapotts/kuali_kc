<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans" xmlns:p="http://www.springframework.org/schema/p" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="State" parent="State-parentBean"/>

  <bean abstract="true" id="State-parentBean" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kra.bo.State"/>
		<property name="objectLabel" value="State"/>
    <property name="inquiryDefinition">
      <ref bean="State-inquiryDefinition"/>
    </property>
    <property name="lookupDefinition">
      <ref bean="State-lookupDefinition"/>
    </property>
    <property name="titleAttribute" value="stateCode"/>
    <property name="attributes">
      <list>
        <ref bean="State-stateCode"/>
        <ref bean="State-description"/>
        <ref bean="State-countryCode"/>
        <ref bean="State-versionNumber"/>
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="State-stateCode" parent="State-stateCode-parentBean"/>

  <bean abstract="true" id="State-stateCode-parentBean" parent="AttributeDefinition">
    <property name="name" value="stateCode"/>
    <property name="forceUppercase" value="false"/>
    <property name="label" value="State Code"/>
    <property name="shortLabel" value="State Code"/>
    <property name="maxLength" value="15"/>
    <property name="validationPattern">
      <bean parent="AlphaNumericValidationPattern"/>
    </property>
    <property name="control">
      <bean p:size="17" parent="TextControlDefinition"/>
    </property>
    <property name="summary" value="State Code"/>
    <property name="description" value="State Code"/>
  </bean>
  <bean id="State-description" parent="State-description-parentBean"/>

  <bean abstract="true" id="State-description-parentBean" parent="AttributeDefinition">
    <property name="name" value="description"/>
    <property name="forceUppercase" value="false"/>
    <property name="label" value="Description"/>
    <property name="shortLabel" value="Description"/>
    <property name="maxLength" value="200"/>
    <property name="validationPattern"><bean p:allowWhitespace="true" parent="AnyCharacterValidationPattern"/></property>
    <property name="control">
      <bean p:size="10" parent="TextControlDefinition"/>
    </property>
    <property name="summary" value="Description"/>
    <property name="description" value="State description..."/>
  </bean>
  <bean id="State-countryCode" parent="State-countryCode-parentBean"/>

  <bean abstract="true" id="State-countryCode-parentBean" parent="Country-countryCode">
  </bean>
  <bean id="State-versionNumber" parent="State-versionNumber-parentBean"/>

  <bean abstract="true" id="State-versionNumber-parentBean" parent="AttributeReferenceDummy-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="State-inquiryDefinition" parent="State-inquiryDefinition-parentBean"/>

  <bean abstract="true" id="State-inquiryDefinition-parentBean" parent="InquiryDefinition">
    <property name="title" value="State Code"/>
    <property name="inquirySections">
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="State Code Code"/>
          <property name="numberOfColumns" value="1"/>
          <property name="inquiryFields">
            <list>
              <bean p:attributeName="countryCode" p:forceInquiry="true" parent="FieldDefinition"/>
              <bean p:attributeName="stateCode" p:forceInquiry="true" parent="FieldDefinition"/>
              <bean p:attributeName="description" parent="FieldDefinition"/>
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="State-lookupDefinition" parent="State-lookupDefinition-parentBean"/>

  <bean abstract="true" id="State-lookupDefinition-parentBean" parent="LookupDefinition">
    <property name="title" value="State Code Lookup"/>
    <property name="menubar" value="&lt;a href=&quot;index.jsp&quot;&gt;Main&lt;/a&gt;"/>

    <property name="defaultSort">
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields">
      <list>
        <bean p:attributeName="countryCode" p:forceLookup="true" parent="FieldDefinition"/>
        <bean p:attributeName="stateCode" p:forceLookup="true" parent="FieldDefinition"/>
        <bean p:attributeName="description" parent="FieldDefinition"/>
      </list>
    </property>
    <property name="resultFields">
      <list>
        <bean p:attributeName="countryCode" p:forceInquiry="true" parent="FieldDefinition"/>
        <bean p:attributeName="stateCode" p:forceInquiry="true" parent="FieldDefinition"/>
        <bean p:attributeName="description" parent="FieldDefinition"/>
      </list>
    </property>
  </bean>
</beans>