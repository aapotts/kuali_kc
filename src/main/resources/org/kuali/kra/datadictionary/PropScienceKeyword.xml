<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.kra.proposaldevelopment.bo.PropScienceKeyword">

        <inquiry>
            <title>Proposal Science Keyword </title>

            <inquirySections><inquirySection title="Proposal Science Keyword "><inquiryFields>
                <field attributeName="proposalNumber" forceInquiry="true" />
                <field attributeName="scienceKeywordCode" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Proposal Science Keyword Lookup</title>
			<menubar>
				<![CDATA[<a href="index.jsp">Main</a>]]>
			</menubar>
            <instructions>Look up Proposal Science Keyword</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="proposalNumber" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="proposalNumber" required="false" />
                <lookupField attributeName="scienceKeywordCode" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="proposalNumber" forceInquiry="true" />
                <field attributeName="scienceKeywordCode" forceInquiry="false"/>
            </resultFields>
        </lookup>

        <titleAttribute>proposalNumber</titleAttribute>

        <attributes>

            <attribute name="proposalNumber">
                <label>Proposal Number</label>
                <shortLabel>Proposal Number</shortLabel>

                <maxLength>12</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="12" />
                </control>

                <summary>Proposal Number</summary>
                <description>Proposal Number</description>
			</attribute>

            <attribute name="scienceKeywordCode" forceUppercase="false">
                <label>Science Keyword Code</label>
                <shortLabel>Science Keyword Code</shortLabel>

                <maxLength>3</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <text size="3" />
                </control>

                <summary>Science Keyword Code</summary>
                <description>Science Keyword Code</description>
			</attribute>

            <attribute name="selectKeyword" forceUppercase="false">
                <label>Select Keyword</label>
                <shortLabel>Select Keyword</shortLabel>

                <maxLength>3</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <control>
                    <checkbox />
                </control>

                <summary>Select Keyword</summary>
                <description>Select Keyword</description>
			</attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />
		</attributes>
    </businessObject>

</dictionaryEntry>