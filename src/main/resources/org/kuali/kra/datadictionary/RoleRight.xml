<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.kra.bo.RoleRight">

        <inquiry>
            <title>Role Rights</title>

            <inquirySections><inquirySection title="Role Rights"><inquiryFields>
            	<field attributeName="rightId" forceInquiry="true"/>
				<field attributeName="roleId" forceInquiry="true"/>
				<field attributeName="descendFlag"/>

            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Role Rights Lookup</title>
			<menubar>
				<![CDATA[<a href="index.jsp">Main</a>]]>
			</menubar>
            <instructions>Look up a Role Rights</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                	<sortAttribute attributeName="rightId"/>
					<sortAttribute attributeName="roleId"/>

                </sortAttributes>
            </defaultSort>

            <lookupFields>
            	<lookupField attributeName="rightId" required="false"/>
				<lookupField attributeName="roleId" required="false"/>
				<lookupField attributeName="descendFlag" required="false"/>

            </lookupFields>

            <resultFields>
            	<field attributeName="rightId" forceInquiry="true"/>
				<field attributeName="roleId" forceInquiry="true"/>
				<field attributeName="descendFlag" forceInquiry="false"/>

            </resultFields>
        </lookup>


        <attributes>

	        <attribute name="rightId">
			<label>Right Id </label>
			<shortLabel>Right Id </shortLabel>
			<maxLength>30</maxLength>
			<validationPattern><anyCharacter /></validationPattern>
			<control>
				<text size="30" />
			</control>
			<summary>Right Id </summary>
			<description>Right Id </description>
			</attribute>

			<attribute name="roleId">
			<label>Role Id </label>
			<shortLabel>Role Id </shortLabel>
			<maxLength>5</maxLength>
			<validationPattern><numeric /></validationPattern>
			<control>
				<text size="5" />
			</control>
			<summary>Role Id </summary>
			<description>Role Id </description>
			</attribute>

			<attribute name="descendFlag">
			<label>Descend Flag </label>
			<shortLabel>Descend Flag </shortLabel>
			<maxLength>1</maxLength>
			<validationPattern><anyCharacter /></validationPattern>
			<control>
				<text size="1" />
			</control>
			<summary>Descend Flag </summary>
			<description>Descend Flag </description>
			</attribute>

		</attributes>
    
    </businessObject>

</dictionaryEntry>
