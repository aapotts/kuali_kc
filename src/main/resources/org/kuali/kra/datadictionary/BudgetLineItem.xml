<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.kra.budget.bo.BudgetLineItem">

        <inquiry>
            <title>Budget Details</title>

            <inquirySections><inquirySection title="Budget Details"><inquiryFields>
            	<field attributeName="budgetPeriod" forceInquiry="true"/>
				<field attributeName="lineItemNumber" forceInquiry="true"/>
				<field attributeName="proposalNumber" forceInquiry="true"/>
				<field attributeName="versionNumber" forceInquiry="true"/>
				<field attributeName="applyInRateFlag"/>
				<field attributeName="basedOnLineItem"/>
				<field attributeName="budgetCategoryCode"/>
				<field attributeName="budgetJustification"/>
				<field attributeName="costElement"/>
				<field attributeName="costSharingAmount"/>
				<field attributeName="endDate"/>
				<field attributeName="lineItemCost"/>
				<field attributeName="lineItemDescription"/>
				<field attributeName="lineItemSequence"/>
				<field attributeName="onOffCampusFlag"/>
				<field attributeName="quantity"/>
				<field attributeName="startDate"/>
				<field attributeName="underrecoveryAmount"/>

            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Budget Details Lookup</title>
			<menubar>
				<![CDATA[<a href="index.jsp">Main</a>]]>
			</menubar>
            <instructions>Look up a Budget Details</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                	<sortAttribute attributeName="budgetPeriod"/>
					<sortAttribute attributeName="lineItemNumber"/>
					<sortAttribute attributeName="proposalNumber"/>
					<sortAttribute attributeName="versionNumber"/>

                </sortAttributes>
            </defaultSort>

            <lookupFields>
            	<lookupField attributeName="budgetPeriod" required="false"/>
				<lookupField attributeName="lineItemNumber" required="false"/>
				<lookupField attributeName="proposalNumber" required="false"/>
				<lookupField attributeName="versionNumber" required="false"/>
				<lookupField attributeName="applyInRateFlag" required="false"/>
				<lookupField attributeName="basedOnLineItem" required="false"/>
				<lookupField attributeName="budgetCategoryCode" required="false"/>
				<lookupField attributeName="budgetJustification" required="false"/>
				<lookupField attributeName="costElement" required="false"/>
				<lookupField attributeName="costSharingAmount" required="false"/>
				<lookupField attributeName="endDate" required="false"/>
				<lookupField attributeName="lineItemCost" required="false"/>
				<lookupField attributeName="lineItemDescription" required="false"/>
				<lookupField attributeName="lineItemSequence" required="false"/>
				<lookupField attributeName="onOffCampusFlag" required="false"/>
				<lookupField attributeName="quantity" required="false"/>
				<lookupField attributeName="startDate" required="false"/>
				<lookupField attributeName="underrecoveryAmount" required="false"/>

            </lookupFields>

            <resultFields>
            	<field attributeName="budgetPeriod" forceInquiry="true"/>
				<field attributeName="lineItemNumber" forceInquiry="true"/>
				<field attributeName="proposalNumber" forceInquiry="true"/>
				<field attributeName="versionNumber" forceInquiry="true"/>
				<field attributeName="applyInRateFlag" forceInquiry="false"/>
				<field attributeName="basedOnLineItem" forceInquiry="false"/>
				<field attributeName="budgetCategoryCode" forceInquiry="false"/>
				<field attributeName="budgetJustification" forceInquiry="false"/>
				<field attributeName="costElement" forceInquiry="false"/>
				<field attributeName="costSharingAmount" forceInquiry="false"/>
				<field attributeName="endDate" forceInquiry="false"/>
				<field attributeName="lineItemCost" forceInquiry="false"/>
				<field attributeName="lineItemDescription" forceInquiry="false"/>
				<field attributeName="lineItemSequence" forceInquiry="false"/>
				<field attributeName="onOffCampusFlag" forceInquiry="false"/>
				<field attributeName="quantity" forceInquiry="false"/>
				<field attributeName="startDate" forceInquiry="false"/>
				<field attributeName="underrecoveryAmount" forceInquiry="false"/>

            </resultFields>
        </lookup>

        <titleAttribute>budgetPeriod</titleAttribute>
		<!-- <titleAttribute>lineItemNumber</titleAttribute>
		<titleAttribute>proposalNumber</titleAttribute>
		<titleAttribute>versionNumber</titleAttribute> -->
        <attributes>

	        <attribute name="budgetPeriod">
			<label>Budget Period </label>
			<shortLabel>Budget Period </shortLabel>
			<maxLength>3</maxLength>
			<validationPattern><numeric /></validationPattern>
			<control>
				<hidden />
			</control>
			<summary>Budget Period </summary>
			<description>Budget Period </description>
			</attribute>

			<attribute name="lineItemNumber">
			<label>Line Item Number </label>
			<shortLabel>Line Item Number </shortLabel>
			<maxLength>3</maxLength>
			<validationPattern><numeric /></validationPattern>
			<control>
				<text size="3" />
			</control>
			<summary>Line Item Number </summary>
			<description>Line Item Number </description>
			</attribute>

			<attribute name="proposalNumber">
			<label>Proposal Number </label>
			<shortLabel>Proposal Number </shortLabel>
			<maxLength>12</maxLength>
			<validationPattern><alphaNumeric /></validationPattern>
			<control>
				<text size="12" />
			</control>
			<summary>Proposal Number </summary>
			<description>Proposal Number </description>
			</attribute>

			<attribute name="versionNumber">
			<label>Version Number </label>
			<shortLabel>Version Number </shortLabel>
			<maxLength>3</maxLength>
			<validationPattern><numeric /></validationPattern>
			<control>
				<text size="3" />
			</control>
			<summary>Version Number </summary>
			<description>Version Number </description>
			</attribute>

			<attribute name="applyInRateFlag">
			<label>Apply Inflation </label>
			<shortLabel>Apply Inflation </shortLabel>
			<maxLength>1</maxLength>
			<validationPattern><alphaNumeric /></validationPattern>
			<control>
				<checkbox />
			</control>
			<summary>Apply Inflation </summary>
			<description>Apply Inflation </description>
			</attribute>

			<attribute name="basedOnLineItem">
			<label>Based On Line Item </label>
			<shortLabel>Based On Line Item </shortLabel>
			<maxLength>3</maxLength>
			<validationPattern><numeric /></validationPattern>
			<control>
				<text size="3" />
			</control>
			<summary>Based On Line Item </summary>
			<description>Based On Line Item </description>
			</attribute>

			<attribute name="budgetCategoryCode">
			<label>Budget Category Code </label>
			<shortLabel>Budget Category Code </shortLabel>
			<maxLength>3</maxLength>
			<validationPattern><numeric /></validationPattern>
			<control>				
				<select valuesFinderClass="org.kuali.core.lookup.keyvalues.PersistableBusinessObjectValuesFinder" businessObjectClass="org.kuali.kra.budget.bo.BudgetCategory" keyAttribute="budgetCategoryCode" labelAttribute="description" includeKeyInLabel="false"/>
			</control>
			<summary>Budget Category Code </summary>
			<description>Budget Category Code </description>
			</attribute>

			<attribute name="budgetJustification">
			<label>Budget Justification </label>
			<shortLabel>Budget Justification </shortLabel>
			<maxLength>200</maxLength>
			<validationPattern><anyCharacter allowWhitespace="true"/></validationPattern>
			<control>
				<textarea rows="3" cols="30" />
			</control>
			<summary>Budget Justification </summary>
			<description>Budget Justification </description>
			</attribute>

			<attribute name="costElement">
			<label>Object Code Name </label>
			<shortLabel>Object Code Name </shortLabel>
			<maxLength>8</maxLength>						
			<validationPattern><alphaNumeric /></validationPattern>
			<required>true</required>
			<control>
				<select valuesFinderClass="org.kuali.kra.budget.lookup.keyvalue.CostElementValuesFinder" />
			</control>
			<summary>Object Code Name </summary>
			<description>Object Code Name </description>
			</attribute>

            <attributeReference name="costSharingAmount" sourceClassName="org.kuali.kra.bo.KraAttributeReferenceDummy" sourceAttributeName="genericAmount">
			<label>Cost Sharing </label>
			<shortLabel>Cost Sharing </shortLabel>
			<summary>Cost Sharing </summary>
			<description>Cost Sharing </description>
			</attributeReference>

            <attributeReference name="endDate" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericDate" >
                <label>End Date</label>
                <shortLabel>End Date</shortLabel>
				<required>true</required>
                <summary>End Date</summary>
                <description>End Date.</description>
      		</attributeReference>

            <attributeReference name="lineItemCost" sourceClassName="org.kuali.kra.bo.KraAttributeReferenceDummy" sourceAttributeName="genericAmount">
			<label>Total Base Cost </label>
			<shortLabel>Total Base Cost </shortLabel>
			<summary>Total Base Cost </summary>
			<description>Total Base Cost </description>
			</attributeReference>

			<attribute name="lineItemDescription">
			<label>Description </label>
			<shortLabel>Description </shortLabel>
			<maxLength>200</maxLength>
			<control>
					<textarea rows="3" cols="30" />
			</control>
			<summary>Description </summary>
			<description>Description </description>
			</attribute>

			<attribute name="lineItemSequence">
			<label>Line Item Sequence </label>
			<shortLabel>Line Item Sequence </shortLabel>
			<maxLength>3</maxLength>
			<validationPattern><numeric /></validationPattern>
			<control>
				<text size="3" />
			</control>
			<summary>Line Item Sequence </summary>
			<description>Line Item Sequence </description>
			</attribute>

			<attribute name="onOffCampusFlag">
			<label>On Campus </label>
			<shortLabel>On Campus </shortLabel>
			<maxLength>1</maxLength>
			<validationPattern><alphaNumeric /></validationPattern>
			<control>
				<checkbox />
			</control>
			<summary>On Campus </summary>
			<description>On Campus </description>
			</attribute>

			<attribute name="quantity">
			<label>Qty </label>
			<shortLabel>Qty </shortLabel>
			<maxLength>4</maxLength>
			<validationPattern><numeric /></validationPattern>
			<control>
				<text size="4" />
			</control>
			<summary>Qty </summary>
			<description>Qty </description>
			</attribute>

            <attributeReference name="startDate" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericDate" >
                <label>Start Date</label>
                <shortLabel>Start Date</shortLabel>
				<required>true</required>
                <summary>Start Date</summary>
                <description>Start Date.</description>
      		</attributeReference>

            <attributeReference name="underrecoveryAmount" sourceClassName="org.kuali.kra.bo.KraAttributeReferenceDummy" sourceAttributeName="genericAmount">
			<label>Unrecovered F&amp;A </label>
			<shortLabel>Unrecovered F&amp;A </shortLabel>
			<summary>Unrecovered F&amp;A </summary>
			<description>Unrecovered F&amp;A </description>
			</attributeReference>

		</attributes>
        <objectHelp parameterNamespace="KRA-B" parameterDetailType="D"  parameterName="budgetLineItemHelpUrl"/> 
    </businessObject>

</dictionaryEntry>
