--><beans xmlns="http://www.springframework.org/schema/beans" xmlns:p="http://www.springframework.org/schema/p" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.springframework.org/schema/beans   http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="AwardTemplate" parent="AwardTemplate-parentBean"/>
	<bean abstract="true" id="AwardTemplate-parentBean" parent="BusinessObjectEntry">
		<property name="businessObjectClass" value="org.kuali.kra.award.home.AwardTemplate"/>
		<property name="objectLabel" value="Sponsor Template"/>
		<property name="inquiryDefinition">
			<ref bean="AwardTemplate-inquiryDefinition"/>
		</property>
		<property name="lookupDefinition">
			<ref bean="AwardTemplate-lookupDefinition"/>
		</property><!--
		<property name="relationshipDefinition" >
			<ref bean="AwardTemplate-relationshipDefinition" />
		</property>
		--><property name="titleAttribute" value="templateCode"/>
		<property name="attributes">
			<list>
				<ref bean="AwardTemplate-templateCode"/>
				<ref bean="AwardTemplate-description"/>
				<ref bean="AwardTemplate-statusCode"/>
				<ref bean="AwardTemplate-primeSponsorCode"/>
				<ref bean="AwardTemplate-nonCompetingContPrpslDueCode"/>
				<ref bean="AwardTemplate-competingRenewalPrpslDueCode"/>
				<ref bean="AwardTemplate-basisOfPaymentCode"/>
				<ref bean="AwardTemplate-methodOfPaymentCode"/>
				<ref bean="AwardTemplate-paymentInvoiceFreqCode"/>
				<ref bean="AwardTemplate-invoiceNumberOfCopies"/>
				<ref bean="AwardTemplate-finalInvoiceDue"/>
				<ref bean="AwardTemplate-versionNumber"/>
			</list>
		</property>
	</bean>

	<!-- Attribute Definitions -->
	
	<bean id="AwardTemplate-templateCode" parent="AwardTemplate-templateCode-parentBean"/>
	<bean abstract="true" id="AwardTemplate-templateCode-parentBean" parent="AttributeDefinition">
		<property name="name" value="templateCode"/>
		<property name="forceUppercase" value="false"/>
		<property name="label" value="Sponsor Template Code"/>
		<property name="shortLabel" value="Sponsor Template Code"/>
		<property name="maxLength" value="5"/>
		<property name="validationPattern">
			<bean parent="AlphaNumericValidationPattern"/>
		</property>
		<property name="control">
			<bean p:size="10" parent="TextControlDefinition"/>
		</property>
		<property name="summary" value="Sponsor Template Code"/>
		<property name="description" value="Sponsor Template Code"/>
	</bean>

	<bean id="AwardTemplate-description" parent="AwardTemplate-description-parentBean"/>
	<bean abstract="true" id="AwardTemplate-description-parentBean" parent="AttributeDefinition">
		<property name="name" value="description"/>
		<property name="forceUppercase" value="false"/>
		<property name="label" value="Description"/>
		<property name="shortLabel" value="Description"/>
		<property name="maxLength" value="200"/>
		<property name="validationPattern">
			<bean p:allowWhitespace="true" parent="AnyCharacterValidationPattern"/>
        </property>
	    <property name="control">
	      <bean p:cols="40" p:rows="5" parent="TextareaControlDefinition"/>
	    </property>
		<property name="summary" value="Description"/>
		<property name="description" value="Description"/>
	</bean>

	<bean id="AwardTemplate-statusCode" parent="AwardTemplate-statusCode-parentBean"/>
	<bean abstract="true" id="AwardTemplate-statusCode-parentBean" parent="AttributeDefinition">
		<property name="name" value="statusCode"/>
		<property name="forceUppercase" value="false"/>
		<property name="label" value="Status Code"/>
		<property name="shortLabel" value="Status Code"/>
		<property name="maxLength" value="3"/>
		<property name="validationPattern">
			<bean parent="AlphaNumericValidationPattern"/>
		</property>
	    <property name="control">
	      <bean p:businessObjectClass="org.kuali.kra.award.home.AwardStatus" p:includeBlankRow="false" p:includeKeyInLabel="false" p:keyAttribute="statusCode" p:labelAttribute="description" p:valuesFinderClass="org.kuali.kra.lookup.keyvalue.ExtendedPersistableBusinessObjectValuesFinder" parent="SelectControlDefinition"/>
	    </property>
		<property name="summary" value="Status Code"/>
		<property name="description" value="Status Code"/>
	</bean>

	<bean id="AwardTemplate-primeSponsorCode" parent="AwardTemplate-primeSponsorCode-parentBean"/>
	<bean abstract="true" id="AwardTemplate-primeSponsorCode-parentBean" parent="AttributeDefinition">
		<property name="name" value="primeSponsorCode"/>
		<property name="forceUppercase" value="false"/>
		<property name="label" value="Prime Sponsor Code"/>
		<property name="shortLabel" value="Prime Sponsor Code"/>
		<property name="maxLength" value="6"/>
		<property name="validationPattern">
			<bean parent="AlphaNumericValidationPattern"/>
		</property>
		<property name="control">
			<bean p:size="10" parent="TextControlDefinition"/>
		</property>
		<property name="summary" value="Prime Sponsor Code"/>
		<property name="description" value="Prime Sponsor Code"/>
	</bean>

	<bean id="AwardTemplate-nonCompetingContPrpslDueCode" parent="AwardTemplate-nonCompetingContPrpslDueCode-parentBean"/>
	<bean abstract="true" id="AwardTemplate-nonCompetingContPrpslDueCode-parentBean" parent="AttributeDefinition">
		<property name="name" value="nonCompetingContPrpslDueCode"/>
		<property name="forceUppercase" value="false"/>
		<property name="label" value="Non Competing Cont Prpsl Due"/>
		<property name="shortLabel" value="Non Competing Cont Prpsl Due"/>
		<property name="maxLength" value="3"/>
		<property name="validationPattern">
			<bean parent="AlphaNumericValidationPattern"/>
		</property>
	    <property name="control">
	      <bean p:businessObjectClass="org.kuali.kra.award.paymentreports.Frequency" p:includeKeyInLabel="false" p:keyAttribute="frequencyCode" p:labelAttribute="description" p:valuesFinderClass="org.kuali.kra.award.lookup.keyvalue.AwardProposalDueValueFinder" parent="SelectControlDefinition"/>
	    </property>
		<property name="summary" value="Non Competing Cont Prpsl Due"/>
		<property name="description" value="Non Competing Cont Prpsl Due"/>
	</bean>

	<bean id="AwardTemplate-competingRenewalPrpslDueCode" parent="AwardTemplate-competingRenewalPrpslDueCode-parentBean"/>
	<bean abstract="true" id="AwardTemplate-competingRenewalPrpslDueCode-parentBean" parent="AttributeDefinition">
		<property name="name" value="competingRenewalPrpslDueCode"/>
		<property name="forceUppercase" value="false"/>
		<property name="label" value="Competing Renewal Prpsl Due"/>
		<property name="shortLabel" value="Competing Renewal Prpsl Due"/>
		<property name="maxLength" value="3"/>
		<property name="validationPattern">
			<bean parent="AlphaNumericValidationPattern"/>
		</property>
	    <property name="control">
	      <bean p:businessObjectClass="org.kuali.kra.award.paymentreports.Frequency" p:includeKeyInLabel="false" p:keyAttribute="frequencyCode" p:labelAttribute="description" p:valuesFinderClass="org.kuali.kra.award.lookup.keyvalue.AwardProposalDueValueFinder" parent="SelectControlDefinition"/>
	    </property>
		<property name="summary" value="Competing Renewal Prpsl Due"/>
		<property name="description" value="Competing Renewal Prpsl Due"/>
	</bean>

	<bean id="AwardTemplate-basisOfPaymentCode" parent="AwardTemplate-basisOfPaymentCode-parentBean"/>
	<bean abstract="true" id="AwardTemplate-basisOfPaymentCode-parentBean" parent="AttributeDefinition">
		<property name="name" value="basisOfPaymentCode"/>
		<property name="forceUppercase" value="false"/>
		<property name="label" value="Basis Of Payment Code"/>
		<property name="shortLabel" value="Basis Of Payment Code"/>
		<property name="maxLength" value="3"/>
		<property name="validationPattern">
			<bean parent="AlphaNumericValidationPattern"/>
		</property>
	    <property name="control">
	      <bean p:businessObjectClass="org.kuali.kra.award.home.AwardBasisOfPayment" p:includeBlankRow="false" p:includeKeyInLabel="false" p:keyAttribute="basisOfPaymentCode" p:labelAttribute="description" p:valuesFinderClass="org.kuali.kra.lookup.keyvalue.ExtendedPersistableBusinessObjectValuesFinder" parent="SelectControlDefinition"/>
	    </property>
		<property name="summary" value="Basis Of Payment Code"/>
		<property name="description" value="Basis Of Payment Code"/>
	</bean>

	<bean id="AwardTemplate-methodOfPaymentCode" parent="AwardTemplate-methodOfPaymentCode-parentBean"/>
	<bean abstract="true" id="AwardTemplate-methodOfPaymentCode-parentBean" parent="AttributeDefinition">
		<property name="name" value="methodOfPaymentCode"/>
		<property name="forceUppercase" value="false"/>
		<property name="label" value="Method Of Payment Code"/>
		<property name="shortLabel" value="Method Of Payment Code"/>
		<property name="maxLength" value="3"/>
		<property name="validationPattern">
			<bean parent="AlphaNumericValidationPattern"/>
		</property>
	    <property name="control">
	      <bean p:businessObjectClass="org.kuali.kra.award.home.AwardMethodOfPayment" p:includeBlankRow="false" p:includeKeyInLabel="false" p:keyAttribute="methodOfPaymentCode" p:labelAttribute="description" p:valuesFinderClass="org.kuali.kra.lookup.keyvalue.ExtendedPersistableBusinessObjectValuesFinder" parent="SelectControlDefinition"/>
	    </property>
		<property name="summary" value="Method Of Payment Code"/>
		<property name="description" value="Method Of Payment Code"/>
	</bean>

	<bean id="AwardTemplate-paymentInvoiceFreqCode" parent="AwardTemplate-paymentInvoiceFreqCode-parentBean"/>
	<bean abstract="true" id="AwardTemplate-paymentInvoiceFreqCode-parentBean" parent="AttributeDefinition">
		<property name="name" value="paymentInvoiceFreqCode"/>
		<property name="forceUppercase" value="false"/>
		<property name="label" value="Payment Invoice Freq Code"/>
		<property name="shortLabel" value="Payment Invoice Freq Code"/>
		<property name="maxLength" value="3"/>
		<property name="validationPattern">
			<bean parent="AlphaNumericValidationPattern"/>
		</property>
	    <property name="control">
	      <bean p:businessObjectClass="org.kuali.kra.award.paymentreports.Frequency" p:includeKeyInLabel="false" p:keyAttribute="frequencyCode" p:labelAttribute="description" p:valuesFinderClass="org.kuali.kra.award.lookup.keyvalue.PaymentInvoiceFrequencyValueFinder" parent="SelectControlDefinition"/>
	    </property>
		<property name="summary" value="Payment Invoice Freq Code"/>
		<property name="description" value="Payment Invoice Freq Code"/>
	</bean>

	<bean id="AwardTemplate-invoiceNumberOfCopies" parent="AwardTemplate-invoiceNumberOfCopies-parentBean"/>
	<bean abstract="true" id="AwardTemplate-invoiceNumberOfCopies-parentBean" parent="AttributeDefinition">
		<property name="name" value="invoiceNumberOfCopies"/>
		<property name="forceUppercase" value="false"/>
		<property name="label" value="Invoice Number Of Copies"/>
		<property name="shortLabel" value="Invoice Number Of Copies"/>
		<property name="maxLength" value="22"/>
		<property name="validationPattern">
			<bean parent="NumericValidationPattern"/>
		</property>
		<property name="control">
			<bean p:size="10" parent="TextControlDefinition"/>
		</property>
		<property name="summary" value="Invoice Number Of Copies"/>
		<property name="description" value="Invoice Number Of Copies"/>
	</bean>

	<bean id="AwardTemplate-finalInvoiceDue" parent="AwardTemplate-finalInvoiceDue-parentBean"/>
	<bean abstract="true" id="AwardTemplate-finalInvoiceDue-parentBean" parent="AttributeDefinition">
		<property name="name" value="finalInvoiceDue"/>
		<property name="forceUppercase" value="false"/>
		<property name="label" value="Final Invoice Due"/>
		<property name="shortLabel" value="Final Invoice Due"/>
		<property name="maxLength" value="22"/>
		<property name="validationPattern">
			<bean parent="NumericValidationPattern"/>
		</property>
		<property name="control">
			<bean p:size="10" parent="TextControlDefinition"/>
		</property>
		<property name="summary" value="Final Invoice Due"/>
		<property name="description" value="Final Invoice Due"/>
	</bean>

	<bean id="AwardTemplate-versionNumber" parent="AwardTemplate-versionNumber-parentBean"/>
	<bean abstract="true" id="AwardTemplate-versionNumber-parentBean" parent="AttributeReferenceDummy-versionNumber">
	</bean>

	<!-- Business Object Inquiry Definition -->
	<bean id="AwardTemplate-inquiryDefinition" parent="AwardTemplate-inquiryDefinition-parentBean"/>
	<bean abstract="true" id="AwardTemplate-inquiryDefinition-parentBean" parent="InquiryDefinition">
		<property name="title" value="SponsorTemplate Code"/>
		<property name="inquirySections">
			<list>
				<bean parent="InquirySectionDefinition">
					<property name="title" value="SponsorTemplate Code"/>
					<property name="numberOfColumns" value="1"/>
					<property name="inquiryFields">
						<list>
							<bean p:attributeName="templateCode" p:forceInquiry="true" parent="FieldDefinition"/>
							<bean p:attributeName="description" parent="FieldDefinition"/>
							<bean p:attributeName="statusCode" parent="FieldDefinition"/>
							<bean p:attributeName="primeSponsorCode" parent="FieldDefinition"/>
							<bean p:attributeName="nonCompetingContPrpslDueCode" parent="FieldDefinition"/>
							<bean p:attributeName="competingRenewalPrpslDueCode" parent="FieldDefinition"/>
							<bean p:attributeName="basisOfPaymentCode" parent="FieldDefinition"/>
							<bean p:attributeName="methodOfPaymentCode" parent="FieldDefinition"/>
							<bean p:attributeName="paymentInvoiceFreqCode" parent="FieldDefinition"/>
							<bean p:attributeName="invoiceNumberOfCopies" parent="FieldDefinition"/>
							<bean p:attributeName="finalInvoiceDue" parent="FieldDefinition"/>
						</list>
					</property>
				</bean>
			</list>
		</property>
	</bean>

	<!-- Business Object Lookup Definition -->
	<bean id="AwardTemplate-lookupDefinition" parent="AwardTemplate-lookupDefinition-parentBean"/>
	<bean abstract="true" id="AwardTemplate-lookupDefinition-parentBean" parent="LookupDefinition">
		<property name="title" value="Sponsor Template Lookup"/>
		<property name="menubar" value="&lt;a href=&quot;index.jsp&quot;&gt;Main&lt;/a&gt;"/>

		<property name="defaultSort">
			<bean parent="SortDefinition">
		</bean>
		</property>
		<property name="lookupFields">
			<list>
				<bean p:attributeName="templateCode" p:forceInquiry="true" parent="FieldDefinition"/>
				<bean p:attributeName="description" parent="FieldDefinition"/>
				<bean p:attributeName="statusCode" parent="FieldDefinition"/>
				<bean p:attributeName="primeSponsorCode" parent="FieldDefinition"/>
				<bean p:attributeName="nonCompetingContPrpslDueCode" parent="FieldDefinition"/>
				<bean p:attributeName="competingRenewalPrpslDueCode" parent="FieldDefinition"/>
				<bean p:attributeName="basisOfPaymentCode" parent="FieldDefinition"/>
				<bean p:attributeName="methodOfPaymentCode" parent="FieldDefinition"/>
				<bean p:attributeName="paymentInvoiceFreqCode" parent="FieldDefinition"/>
				<bean p:attributeName="invoiceNumberOfCopies" parent="FieldDefinition"/>
				<bean p:attributeName="finalInvoiceDue" parent="FieldDefinition"/>
			</list>
		</property>
		<property name="resultFields">
			<list>
				<bean p:attributeName="templateCode" p:forceInquiry="true" parent="FieldDefinition"/>
				<bean p:attributeName="description" parent="FieldDefinition"/>
				<bean p:attributeName="statusCode" parent="FieldDefinition"/>
				<bean p:attributeName="primeSponsorCode" parent="FieldDefinition"/>
				<bean p:attributeName="nonCompetingContPrpslDueCode" parent="FieldDefinition"/>
				<bean p:attributeName="competingRenewalPrpslDueCode" parent="FieldDefinition"/>
				<bean p:attributeName="basisOfPaymentCode" parent="FieldDefinition"/>
				<bean p:attributeName="methodOfPaymentCode" parent="FieldDefinition"/>
				<bean p:attributeName="paymentInvoiceFreqCode" parent="FieldDefinition"/>
				<bean p:attributeName="invoiceNumberOfCopies" parent="FieldDefinition"/>
				<bean p:attributeName="finalInvoiceDue" parent="FieldDefinition"/>
			</list>
		</property>
	</bean>
	<!-- Business Object Lookup Definition --><!--
	<bean id="AwardTemplate-relationshipDefinition" parent="AwardTemplate-relationshipDefinition-parentBean" />
	<bean id="AwardTemplate-relationshipDefinition-parentBean" abstract="true" parent="RelationshipDefinition">
		<property name="resultFields" >
		</property>
	</bean>-->
</beans>