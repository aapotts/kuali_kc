<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.kra.budget.bo.BudgetCategoryMapping">

        <inquiry>
            <title>Budget Category Mapping</title>

            <inquirySections><inquirySection title="Budget Category Mapping"><inquiryFields>
            	<field attributeName="budgetCategoryCode" forceInquiry="true"/>
				<field attributeName="mappingName" forceInquiry="true"/>
				<field attributeName="targetCategoryCode" forceInquiry="true"/>

            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Budget Category Mapping Lookup</title>
			<menubar>
				<![CDATA[<a href="index.jsp>Main</a>]]>
			</menubar>
            <instructions>Look up a Budget Category Mapping</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                	<sortAttribute attributeName="budgetCategoryCode"/>
					<sortAttribute attributeName="mappingName"/>
					<sortAttribute attributeName="targetCategoryCode"/>

                </sortAttributes>
            </defaultSort>

            <lookupFields>
            	<lookupField attributeName="budgetCategoryCode" required="false"/>
				<lookupField attributeName="mappingName" required="false"/>
				<lookupField attributeName="targetCategoryCode" required="false"/>

            </lookupFields>

            <resultFields>
            	<field attributeName="budgetCategoryCode" forceInquiry="true"/>
				<field attributeName="mappingName" forceInquiry="true"/>
				<field attributeName="targetCategoryCode" forceInquiry="true"/>

            </resultFields>
        </lookup>

        <!-- <titleAttribute>coeusCategoryCode</titleAttribute>
		<titleAttribute>mappingName</titleAttribute> -->
		<titleAttribute>targetCategoryCode</titleAttribute>
        <attributes>

	        <attribute name="budgetCategoryCode">
			<label>Coeus Category Code </label>
			<shortLabel>Coeus Category Code </shortLabel>
			<maxLength>3</maxLength>
			<validationPattern><numeric /></validationPattern>
			<control>
				<text size="3" />
			</control>
			<summary>Coeus Category Code </summary>
			<description>Coeus Category Code </description>
			</attribute>

			<attribute name="mappingName">
			<label>Mapping Name </label>
			<shortLabel>Mapping Name </shortLabel>
			<maxLength>100</maxLength>
			<validationPattern><anyCharacter allowWhitespace="true"/></validationPattern>
			<control>
				<text size="40" />
			</control>
			<summary>Mapping Name </summary>
			<description>Mapping Name </description>
			</attribute>

			<attribute name="targetCategoryCode">
			<label>Target Category Code </label>
			<shortLabel>Target Category Code </shortLabel>
			<maxLength>15</maxLength>
			<validationPattern><alphaNumeric /></validationPattern>
			<control>
				<text size="15" />
			</control>
			<summary>Target Category Code </summary>
			<description>Target Category Code </description>
			</attribute>
            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

		</attributes>
    </businessObject>

</dictionaryEntry>
