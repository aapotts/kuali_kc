<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.kra.proposaldevelopment.bo.ProposalUserRoles">

        <inquiry>
            <title>Prop User Roles</title>

            <inquirySections><inquirySection title="Prop User Roles"><inquiryFields>
            	<field attributeName="proposalNumber" forceInquiry="true"/>
				<field attributeName="roleId" forceInquiry="true"/>
				<field attributeName="userId" forceInquiry="true"/>

            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Prop User Roles Lookup</title>
			<menubar>
				<![CDATA[<a href="index.jsp">Main</a>]]>
			</menubar>
            <instructions>Look up a Prop User Roles</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                	<sortAttribute attributeName="proposalNumber"/>
					<sortAttribute attributeName="roleId"/>
					<sortAttribute attributeName="userId"/>

                </sortAttributes>
            </defaultSort>

            <lookupFields>
            	<lookupField attributeName="proposalNumber" required="false"/>
				<lookupField attributeName="roleId" required="false"/>
				<lookupField attributeName="userId" required="false"/>

            </lookupFields>

            <resultFields>
            	<field attributeName="proposalNumber" forceInquiry="true"/>
				<field attributeName="roleId" forceInquiry="true"/>
				<field attributeName="userId" forceInquiry="true"/>

            </resultFields>
        </lookup>

		<titleAttribute>userId</titleAttribute>
        <attributes>

	        <attribute name="proposalNumber">
			<label>Proposal Number </label>
			<shortLabel>Proposal Number </shortLabel>
			<maxLength>12</maxLength>
			<validationPattern><numeric /></validationPattern>
			<control>
				<text size="12" />
			</control>
			<summary>Proposal Number </summary>
			<description>Proposal Number </description>
			</attribute>

			<attribute name="roleId">
			<label>Role Id </label>
			<shortLabel>Role Id </shortLabel>
			<maxLength>5</maxLength>
			<validationPattern><numeric /></validationPattern>
			<control>
				<text size="5" />
			</control>
			<summary>Role Id </summary>
			<description>Role Id </description>
			</attribute>

			<attribute name="userId">
			<label>User Id </label>
			<shortLabel>User Id </shortLabel>
			<maxLength>10</maxLength>
			<validationPattern><anyCharacter/></validationPattern>
			<control>
				<text size="10" />
			</control>
			<summary>User Id </summary>
			<description>User Id </description>
			</attribute>

		</attributes>
    </businessObject>

</dictionaryEntry>
