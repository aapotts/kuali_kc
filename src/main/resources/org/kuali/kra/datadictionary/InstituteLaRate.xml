<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.kra.bo.InstituteLaRate">

        <inquiry>
            <title>Institute La Rates</title>

            <inquirySections><inquirySection title="Institute La Rates"><inquiryFields>
            	<field attributeName="fiscalYear" forceInquiry="true"/>
				<field attributeName="onOffCampusFlag" forceInquiry="true"/>
				<field attributeName="rateClassCode" forceInquiry="true"/>
				<field attributeName="rateTypeCode" forceInquiry="true"/>
				<field attributeName="startDate" forceInquiry="true"/>
				<field attributeName="unitNumber" forceInquiry="true"/>
				<field attributeName="instituteRate"/>
				<field attributeName="active"/>

            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <lookupableID>onOffCampusLookupable</lookupableID>
        
            <title>Institute La Rates Lookup</title>
			<menubar>
				<![CDATA[<a href="/kra-dev/">Main</a>]]>
			</menubar>
            <instructions>Look up a Institute La Rates</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                	<sortAttribute attributeName="fiscalYear"/>
					<sortAttribute attributeName="onOffCampusFlag"/>
					<sortAttribute attributeName="rateClassCode"/>
					<sortAttribute attributeName="rateTypeCode"/>
					<sortAttribute attributeName="startDate"/>
					<sortAttribute attributeName="unitNumber"/>

                </sortAttributes>
            </defaultSort>

            <lookupFields>
            	<lookupField attributeName="fiscalYear" required="false"/>
				<lookupField attributeName="onOffCampusFlag" required="false"/>
				<lookupField attributeName="rateClassCode" required="false"/>
				<lookupField attributeName="rateTypeCode" required="false"/>
				<lookupField attributeName="startDate" required="false"/>
				<lookupField attributeName="unitNumber" required="false"/>
				<lookupField attributeName="instituteRate" required="false"/>
				<lookupField attributeName="active" required="false"/>

            </lookupFields>

            <resultFields>
            	<field attributeName="fiscalYear" forceInquiry="true"/>
				<field attributeName="onOffCampusFlag" forceInquiry="true"/>
				<field attributeName="rateClassCode" forceInquiry="true"/>
            	<field attributeName="rateClass.description" forceInquiry="true"/>
				<field attributeName="rateTypeCode" forceInquiry="true"/>
            	<field attributeName="rateType.description" forceInquiry="true"/>
				<field attributeName="startDate" forceInquiry="true"/>
				<field attributeName="unitNumber" forceInquiry="true"/>
				<field attributeName="instituteRate" forceInquiry="false"/>
				<field attributeName="active" forceInquiry="false"/>

            </resultFields>
        </lookup>

		<titleAttribute>unitNumber</titleAttribute>
        <attributes>

	        <attribute name="fiscalYear">
			<label>Fiscal Year </label>
			<shortLabel>Fiscal Year </shortLabel>
			<maxLength>4</maxLength>
			<validationPattern><alphaNumeric/></validationPattern>
			<control>
				<text size="4" />
			</control>
			<summary>Fiscal Year </summary>
			<description>Fiscal Year </description>
			</attribute>

			<attribute name="onOffCampusFlag">
			<label>On/Off Campus Flag </label>
			<shortLabel>On/Off Campus Flag </shortLabel>
			<maxLength>1</maxLength>
			<validationPattern><anyCharacter/></validationPattern>
			<control>
				<checkbox/>
					<!-- <radio valuesFinderClass="org.kuali.kra.budget.lookup.keyvalue.OnOffCampusFlagValuesFinder" /> -->
			</control>
			<summary>On Off Campus Flag </summary>
			<description>On Off Campus Flag </description>
			</attribute>

           <attributeReference name="active" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericBoolean">
                <label>Active</label>
                <shortLabel>Active </shortLabel>
                <summary>Active</summary>
                <description>Active</description>
           </attributeReference>

			<attribute name="rateClassCode">
			<label>Rate Class Code </label>
			<shortLabel>Rate Class Code </shortLabel>
			<maxLength>3</maxLength>
			<validationPattern><numeric /></validationPattern>
			<control>
				<text size="3" />
			</control>
			<summary>Rate Class Code </summary>
			<description>Rate Class Code </description>
			</attribute>

			<attribute name="rateTypeCode">
			<label>Rate Type Code </label>
			<shortLabel>Rate Type Code </shortLabel>
			<maxLength>3</maxLength>
			<validationPattern><numeric /></validationPattern>
			<control>
				<text size="3" />
			</control>
			<summary>Rate Type Code </summary>
			<description>Rate Type Code </description>
			</attribute>

			<attribute name="startDate">
			<label>Start Date </label>
			<shortLabel>Start Date </shortLabel>
			<maxLength>10</maxLength>
			<validationPattern><date /></validationPattern>
			<control>
				<text size="10" datePicker="true" />
			</control>
			<summary>Start Date </summary>
			<description>Start Date </description>
			</attribute>

			<attribute name="unitNumber">
			<label>Unit Number </label>
			<shortLabel>Unit Number </shortLabel>
			<maxLength>8</maxLength>
			<validationPattern><anyCharacter/></validationPattern>
			<control>
				<text size="8" />
			</control>
			<summary>Unit Number </summary>
			<description>Unit Number </description>
			</attribute>

			<attribute name="instituteRate">
			<label>Rate </label>
			<shortLabel>Rate </shortLabel>
			<maxLength>5</maxLength>
			<validationPattern><fixedPoint allowNegative="false" precision="5" scale="2"/></validationPattern>
			<control>
				<text size="5" />
			</control>
			<summary>Rate </summary>
			<description>Rate </description>
			</attribute>
			<attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />
            <attributeReference name="rateClass.description" sourceClassName="org.kuali.kra.budget.bo.RateClass" sourceAttributeName="description" />
            <attributeReference name="rateType.description" sourceClassName="org.kuali.kra.budget.bo.RateType" sourceAttributeName="description" />

		</attributes>
    </businessObject>

</dictionaryEntry>
