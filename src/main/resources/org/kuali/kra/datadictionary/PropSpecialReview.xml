<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "dataDictionary-1_0.dtd">
<dictionaryEntry>

    <businessObject businessObjectClass="org.kuali.kra.proposaldevelopment.bo.PropSpecialReview" >

        <inquiry>
            <title>Prop Special Review</title>

            <inquirySections><inquirySection title="Prop Special Review"><inquiryFields>
            	<field attributeName="proposalNumber" forceInquiry="true"/>
				<field attributeName="specialReviewNumber" forceInquiry="true"/>
				<field attributeName="applicationDate"/>
				<field attributeName="approvalDate"/>
				<field attributeName="approvalTypeCode"/>
				<field attributeName="comments"/>
				<field attributeName="protocolNumber"/>
				<field attributeName="specialReviewCode"/>

            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Prop Special Review Lookup</title>
			<menubar>
				<![CDATA[<a href="index.jsp">Main</a>]]>
			</menubar>
            <instructions>Look up a Prop Special Review</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                	<sortAttribute attributeName="proposalNumber"/>
					<sortAttribute attributeName="specialReviewNumber"/>

                </sortAttributes>
            </defaultSort>

            <lookupFields>
            	<lookupField attributeName="proposalNumber" required="false"/>
				<lookupField attributeName="specialReviewNumber" required="false"/>
				<lookupField attributeName="applicationDate" required="false"/>
				<lookupField attributeName="approvalDate" required="false"/>
				<lookupField attributeName="approvalTypeCode" required="false"/>
				<lookupField attributeName="comments" required="false"/>
				<lookupField attributeName="protocolNumber" required="false"/>
				<lookupField attributeName="specialReviewCode" required="false"/>

            </lookupFields>

            <resultFields>
            	<field attributeName="proposalNumber" forceInquiry="true"/>
				<field attributeName="specialReviewNumber" forceInquiry="true"/>
				<field attributeName="applicationDate" forceInquiry="false"/>
				<field attributeName="approvalDate" forceInquiry="false"/>
				<field attributeName="approvalTypeCode" forceInquiry="false"/>
				<field attributeName="comments" forceInquiry="false"/>
				<field attributeName="protocolNumber" forceInquiry="false"/>
				<field attributeName="specialReviewCode" forceInquiry="false"/>

            </resultFields>
        </lookup>

		<titleAttribute>specialReviewNumber</titleAttribute>
        <attributes>

	        <attribute name="proposalNumber">
			<label>Proposal Number </label>
			<shortLabel>Proposal Number </shortLabel>
			<maxLength>12</maxLength>
			<validationPattern><numeric /></validationPattern>
			<control>
				<hidden />
			</control>
			<summary>Proposal Number </summary>
			<description>Proposal Number </description>
			</attribute>

			<attribute name="specialReviewNumber">
			<label>Special Review Number </label>
			<shortLabel>Special Review Number </shortLabel>
			<maxLength>3</maxLength>
			<validationPattern><numeric /></validationPattern>
			<control>
				<text size="3" />
			</control>
			<summary>Special Review Number </summary>
			<description>Special Review Number </description>
			</attribute>

            <attributeReference name="applicationDate" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericDate" >
                <label>Application Date</label>
                <shortLabel>Application Date</shortLabel>

 				<required>false</required>

                <summary>The date the PI submitted a protocol/request for permission to the relevant institutional committee/person</summary>
                <description>Application Date</description>
      		</attributeReference>

            <attributeReference name="approvalDate" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericDate" >
                <label>Approval Date</label>
                <shortLabel>Approval Date</shortLabel>

 				<required>false</required>

                <summary>The date the protocol/request for permission was approved by the relevant institutional committee/person</summary>
                <description>Approval Date</description>
      		</attributeReference>


			<attribute name="approvalTypeCode">
			<label>Approval Status </label>
			<shortLabel>Approval Type Code </shortLabel>
			<maxLength>3</maxLength>
			<validationPattern><alphaNumeric /></validationPattern>
			<required>true</required>
			<control>
                    <select valuesFinderClass="org.kuali.kra.proposaldevelopment.lookup.keyvalue.SpecialReviewApprovalTypeValuesFinder" />
			</control>
			<summary>Approval Type Code </summary>
			<description>Approval Type Code </description>
			</attribute>

			<attribute name="comments">
			<label>Comments </label>
			<shortLabel>Comments </shortLabel>
			<maxLength>800</maxLength>
			<validationPattern><anyCharacter  allowWhitespace="true" /></validationPattern>
			<control>
				<textarea cols="40" rows="3"  />
			</control>
			<summary>Comments </summary>
			<description>Comments </description>
			</attribute>

			<attribute name="protocolNumber">
			<label>Protocol # </label>
			<shortLabel>Protocol # </shortLabel>
			<maxLength>20</maxLength>
			<validationPattern><alphaNumeric /></validationPattern>
			<control>
				<text size="20" />
			</control>
			<summary>Protocol Number </summary>
			<description>Protocol Number </description>
			</attribute>

			<attribute name="specialReviewCode">
			<label>Type </label>
			<shortLabel>Special Review Code </shortLabel>
			<maxLength>3</maxLength>
			<validationPattern><alphaNumeric /></validationPattern>
			<required>true</required>
			<control>
                    <select valuesFinderClass="org.kuali.kra.proposaldevelopment.lookup.keyvalue.SpecialReviewValuesFinder" />
			</control>
			<summary>Special Review Code </summary>
			<description>Special Review Code </description>
			</attribute>

		</attributes>
    </businessObject>

</dictionaryEntry>