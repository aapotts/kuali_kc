<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="DevelopmentProposal" parent="DevelopmentProposal-parentBean" />
	<bean id="DevelopmentProposal-parentBean" abstract="true" parent="BusinessObjectEntry">
		<property name="businessObjectClass" value="org.kuali.kra.proposaldevelopment.bo.DevelopmentProposal" />
		<!--
		<property name="inquiryDefinition" >
			<ref bean="DevelopmentProposal-inquiryDefinition" />
		</property>
		<property name="lookupDefinition" >
			<ref bean="DevelopmentProposal-lookupDefinition" />
		</property>
		-->
		<property name="titleAttribute" value="proposalNumber" />



	    <property name="attributes">
	      <list>
	        <ref bean="DevelopmentProposal-proposalNumber"/>
	        <ref bean="DevelopmentProposal-title"/>
	        <ref bean="DevelopmentProposal-sponsorCode"/>
	        <ref bean="DevelopmentProposal-requestedStartDateInitial"/>
	        <ref bean="DevelopmentProposal-requestedEndDateInitial"/>
	        <ref bean="DevelopmentProposal-activityTypeCode"/>
	        <ref bean="DevelopmentProposal-ownedByUnitNumber"/>
	        <ref bean="DevelopmentProposal-proposalTypeCode"/>
	        <ref bean="DevelopmentProposal-currentAwardNumber"/>
	        <ref bean="DevelopmentProposal-continuedFrom"/>
	        <ref bean="DevelopmentProposal-deadlineDate"/>
	        <ref bean="DevelopmentProposal-deadlineType"/>
	        <ref bean="DevelopmentProposal-primeSponsorCode"/>
	        <ref bean="DevelopmentProposal-nsfCode"/>
	        <ref bean="DevelopmentProposal-agencyDivisionCode"/>
	        <ref bean="DevelopmentProposal-programAnnouncementTitle"/>
	        <ref bean="DevelopmentProposal-noticeOfOpportunityCode"/>
	        <ref bean="DevelopmentProposal-cfdaNumber"/>
	        <ref bean="DevelopmentProposal-programAnnouncementNumber"/>
	        <ref bean="DevelopmentProposal-sponsorProposalNumber"/>
	        <ref bean="DevelopmentProposal-subcontracts"/>
	        <ref bean="DevelopmentProposal-agencyProgramCode"/>
	        <ref bean="DevelopmentProposal-mailBy"/>
	        <ref bean="DevelopmentProposal-mailType"/>
	        <ref bean="DevelopmentProposal-mailAccountNumber"/>
	        <ref bean="DevelopmentProposal-numberOfCopies"/>
	        <ref bean="DevelopmentProposal-mailingAddressId"/>
	        <ref bean="DevelopmentProposal-mailDescription"/>
	        <ref bean="DevelopmentProposal-budgetStatus"/>
	        <ref bean="DevelopmentProposal-postSubmissionStatusCode"/>
	        <ref bean="DevelopmentProposal-versionNumber"/>
	      </list>
	    </property>
	</bean>


<!-- Attribute Definitions -->

  <bean id="DevelopmentProposal-proposalNumber" parent="DevelopmentProposal-proposalNumber-parentBean"/>

  <bean id="DevelopmentProposal-proposalNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="proposalNumber"/>
    <property name="forceUppercase" value="false"/>
    <property name="label" value="Proposal ID"/>
    <property name="shortLabel" value="Proposal ID"/>
    <property name="maxLength" value="12"/>
    <property name="validationPattern">
      <bean parent="NumericValidationPattern"/>
    </property>
    <property name="required" value="true"/>
    <property name="control">
      <bean parent="HiddenControlDefinition"/>
    </property>
    <property name="summary" value="Proposal Number"/>
    <property name="description" value="The unique proposal number identifying a proposal. This is a system generated, sequential number."/>
  </bean>
  <bean id="DevelopmentProposal-title" parent="DevelopmentProposal-title-parentBean"/>

  <bean id="DevelopmentProposal-title-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="title"/>
    <property name="forceUppercase" value="false"/>
    <property name="label" value="Project Title"/>
    <property name="shortLabel" value="Title"/>
    <property name="maxLength" value="150"/>
    <property name="validationPattern">
      <bean parent="AnyCharacterValidationPattern" p:allowWhitespace="true"/>
    </property>
    <property name="required" value="true"/>
    <property name="control">
      <bean parent="TextareaControlDefinition" p:rows="3" p:cols="80"/>
    </property>
    <property name="summary" value="The title of the propsal."/>
    <property name="description" value="The proposed title of the project."/>
  </bean>
  <bean id="DevelopmentProposal-sponsorCode" parent="DevelopmentProposal-sponsorCode-parentBean"/>

  <bean id="DevelopmentProposal-sponsorCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="sponsorCode"/>
    <property name="forceUppercase" value="false"/>
    <property name="label" value="Sponsor ID"/>
    <property name="shortLabel" value="Sponsor"/>
    <property name="maxLength" value="6"/>
    <property name="validationPattern">
      <bean parent="AnyCharacterValidationPattern"/>
    </property>
    <property name="required" value="true"/>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="12"/>
    </property>
    <property name="summary" value="Agency/Sponsor #"/>
    <property name="description" value="The identification number of the organization or agency that is providing support for the sponsored project."/>
  </bean>
  <bean id="DevelopmentProposal-requestedStartDateInitial" parent="DevelopmentProposal-requestedStartDateInitial-parentBean"/>

  <bean id="DevelopmentProposal-requestedStartDateInitial-parentBean" abstract="true" parent="KraAttributeReferenceDummy-genericDate">
    <property name="name" value="requestedStartDateInitial"/>
    <property name="label" value="Project Start Date"/>
    <property name="shortLabel" value="Start Dt"/>
    <property name="required" value="true"/>
    <property name="summary" value="The Start Date of the Proposal"/>
    <property name="description" value="The date the project period will begin."/>
  </bean>
  <bean id="DevelopmentProposal-requestedEndDateInitial" parent="DevelopmentProposal-requestedEndDateInitial-parentBean"/>

  <bean id="DevelopmentProposal-requestedEndDateInitial-parentBean" abstract="true" parent="KraAttributeReferenceDummy-genericDate">
    <property name="name" value="requestedEndDateInitial"/>
    <property name="label" value="Project End Date"/>
    <property name="shortLabel" value="End Dt"/>
    <property name="required" value="true"/>
    <property name="summary" value="The End Date of the Proposal"/>
    <property name="description" value="The date the project period will terminate. Use the date format (mm/dd/yyyy)"/>
  </bean>
  <bean id="DevelopmentProposal-activityTypeCode" parent="DevelopmentProposal-activityTypeCode-parentBean"/>

  <bean id="DevelopmentProposal-activityTypeCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="activityTypeCode"/>
    <property name="forceUppercase" value="false"/>
    <property name="label" value="Activity Type"/>
    <property name="shortLabel" value="Activity Type"/>
    <property name="maxLength" value="3"/>
    <property name="validationPattern">
      <bean parent="AlphaNumericValidationPattern"/>
    </property>
    <property name="required" value="true"/>
    <property name="control">
      <bean parent="SelectControlDefinition" p:businessObjectClass="org.kuali.kra.proposaldevelopment.bo.ActivityType" p:valuesFinderClass="org.kuali.kra.lookup.keyvalue.ExtendedPersistableBusinessObjectValuesFinder" p:includeKeyInLabel="false" p:keyAttribute="activityTypeCode" p:labelAttribute="description"/>
    </property>
    <property name="summary" value="Activity Type summary"/>
    <property name="description" value="The type of activity proposed, e.g. organized research, instruction, etc."/>
  </bean>
  <bean id="DevelopmentProposal-ownedByUnitNumber" parent="DevelopmentProposal-ownedByUnitNumber-parentBean"/>

  <bean id="DevelopmentProposal-ownedByUnitNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="ownedByUnitNumber"/>
    <property name="forceUppercase" value="false"/>
    <property name="label" value="Lead Unit"/>
    <property name="shortLabel" value="Lead Unit"/>
    <property name="maxLength" value="8"/>
    <property name="validationPattern">
      <bean parent="AnyCharacterValidationPattern"/>
    </property>
    <property name="required" value="true"/>
    <property name="control">
      <bean parent="SelectControlDefinition" p:valuesFinderClass="org.kuali.kra.proposaldevelopment.lookup.keyvalue.LeadUnitValuesFinder" p:includeKeyInLabel="false"/>
    </property>
    <property name="summary" value="Lead Unit (Owned by Unit)"/>
    <property name="description" value="The proposal's fixed home unit that will be used for routing and business rule purposes. AKA Owned by Unit."/>
  </bean>
  <bean id="DevelopmentProposal-proposalTypeCode" parent="DevelopmentProposal-proposalTypeCode-parentBean"/>

  <bean id="DevelopmentProposal-proposalTypeCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="proposalTypeCode"/>
    <property name="forceUppercase" value="false"/>
    <property name="label" value="Proposal Type"/>
    <property name="shortLabel" value="Proposal Type"/>
    <property name="maxLength" value="3"/>
    <property name="validationPattern">
      <bean parent="AlphaNumericValidationPattern"/>
    </property>
    <property name="required" value="true"/>
    <property name="control">
      <bean parent="SelectControlDefinition" p:businessObjectClass="org.kuali.kra.proposaldevelopment.bo.ProposalType" p:valuesFinderClass="org.kuali.kra.lookup.keyvalue.ExtendedPersistableBusinessObjectValuesFinder" p:includeKeyInLabel="false" p:keyAttribute="proposalTypeCode" p:labelAttribute="description"/>
    </property>
    <property name="summary" value="The Types that the Proposal Could be."/>
    <property name="description" value="The type of the proposal."/>
  </bean>
  <bean id="DevelopmentProposal-currentAwardNumber" parent="DevelopmentProposal-currentAwardNumber-parentBean"/>

  <bean id="DevelopmentProposal-currentAwardNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="currentAwardNumber"/>
    <property name="forceUppercase" value="false"/>
    <property name="label" value="Award ID"/>
    <property name="shortLabel" value="Award ID"/>
    <property name="maxLength" value="10"/>
    <property name="validationPattern">
      <bean parent="AnyCharacterValidationPattern"/>
    </property>
    <property name="required" value="false"/>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="20"/>
    </property>
    <property name="summary" value="The Sponsor Award Number for this Proposal."/>
    <property name="description" value="A unique institutionally assigned number of a previously funded application."/>
  </bean>
  <bean id="DevelopmentProposal-continuedFrom" parent="DevelopmentProposal-continuedFrom-parentBean"/>

  <bean id="DevelopmentProposal-continuedFrom-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="continuedFrom"/>
    <property name="forceUppercase" value="false"/>
    <property name="label" value="Original Proposal ID"/>
    <property name="shortLabel" value="Original Proposal ID"/>
    <property name="maxLength" value="8"/>
    <property name="validationPattern">
      <bean parent="AlphaNumericValidationPattern"/>
    </property>
    <property name="required" value="false"/>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="20"/>
    </property>
    <property name="summary" value="The Original Proposal ID for this Proposal."/>
    <property name="description" value="A unique institutionally assigned proposal number of a previously submitted proposal. This is applicable when submitting a revised (changed/corrected) application."/>
  </bean>
  <bean id="DevelopmentProposal-deadlineDate" parent="DevelopmentProposal-deadlineDate-parentBean"/>

  <bean id="DevelopmentProposal-deadlineDate-parentBean" abstract="true" parent="AttributeReferenceDummy-genericDate">
    <property name="name" value="deadlineDate"/>
    <property name="label" value="Sponsor Deadline Date"/>
    <property name="shortLabel" value="Sponsor Deadline Date"/>
    <property name="required" value="false"/>
    <property name="summary" value="The Deadline Date of the Proposal"/>
    <property name="description" value="The date in which the proposal is due to the sponsoring agency.  Additional deadlines may be set by the office authorized to submit proposals on behalf of the institution (e.g. sponsored programs, etc.)."/>
  </bean>
  <bean id="DevelopmentProposal-deadlineType" parent="DevelopmentProposal-deadlineType-parentBean"/>

  <bean id="DevelopmentProposal-deadlineType-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="deadlineType"/>
    <property name="forceUppercase" value="false"/>
    <property name="label" value="Sponsor Deadline Type"/>
    <property name="shortLabel" value="Sponsor Deadline Type"/>
    <property name="maxLength" value="3"/>
    <property name="validationPattern">
      <bean parent="AnyCharacterValidationPattern"/>
    </property>
    <property name="required" value="false"/>
    <property name="control">
      <bean parent="SelectControlDefinition" p:businessObjectClass="org.kuali.kra.proposaldevelopment.bo.DeadlineType" p:valuesFinderClass="org.kuali.kra.lookup.keyvalue.ExtendedPersistableBusinessObjectValuesFinder" p:includeKeyInLabel="false" p:keyAttribute="deadlineTypeCode" p:labelAttribute="description"/>
    </property>
    <property name="summary" value="The Deadline Type for this Proposal."/>
    <property name="description" value="Indicates the type of proposal deadline."/>
  </bean>
  <bean id="DevelopmentProposal-primeSponsorCode" parent="DevelopmentProposal-primeSponsorCode-parentBean"/>

  <bean id="DevelopmentProposal-primeSponsorCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="primeSponsorCode"/>
    <property name="forceUppercase" value="false"/>
    <property name="label" value="Prime Sponsor ID"/>
    <property name="shortLabel" value="Prime Sponsor ID"/>
    <property name="maxLength" value="6"/>
    <property name="validationPattern">
      <bean parent="AnyCharacterValidationPattern"/>
    </property>
    <property name="required" value="false"/>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="20"/>
    </property>
    <property name="summary" value="The Prime Sponsor for this Proposal."/>
    <property name="description" value="The sponsor that provides the original source of project funding."/>
  </bean>
  <bean id="DevelopmentProposal-nsfCode" parent="DevelopmentProposal-nsfCode-parentBean"/>

  <bean id="DevelopmentProposal-nsfCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="nsfCode"/>
    <property name="forceUppercase" value="false"/>
    <property name="label" value="NSF Science Code"/>
    <property name="shortLabel" value="NSF Science Code"/>
    <property name="maxLength" value="15"/>
    <property name="validationPattern">
      <bean parent="AnyCharacterValidationPattern"/>
    </property>
    <property name="required" value="false"/>
    <property name="control">
      <bean parent="SelectControlDefinition" p:businessObjectClass="org.kuali.kra.bo.NsfCode" p:valuesFinderClass="org.kuali.kra.lookup.keyvalue.ExtendedPersistableBusinessObjectValuesFinder" p:includeKeyInLabel="false" p:keyAttribute="nsfCode" p:labelAttribute="description"/>
    </property>
    <property name="summary" value="The NSF Science Code for this Proposal."/>
    <property name="description" value="A number used to indicate the type of research activity as defined by the National Science Foundation. NSF codes are not specific for NSF proposals and/or awards."/>
  </bean>
  <bean id="DevelopmentProposal-agencyDivisionCode" parent="DevelopmentProposal-agencyDivisionCode-parentBean"/>

  <bean id="DevelopmentProposal-agencyDivisionCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="agencyDivisionCode"/>
    <property name="forceUppercase" value="false"/>
    <property name="label" value="Sponsor Div Code"/>
    <property name="shortLabel" value="Sponsor Div Code"/>
    <property name="maxLength" value="50"/>
    <property name="validationPattern">
      <bean parent="AnyCharacterValidationPattern" p:allowWhitespace="true"/>
    </property>
    <property name="required" value="false"/>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="50"/>
    </property>
    <property name="summary" value="The Sponsor Division Code for this Proposal."/>
    <property name="description" value="A code that is unique to each NSF division.  Currently specific to the NSF only."/>
  </bean>
  <bean id="DevelopmentProposal-programAnnouncementTitle" parent="DevelopmentProposal-programAnnouncementTitle-parentBean"/>

  <bean id="DevelopmentProposal-programAnnouncementTitle-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="programAnnouncementTitle"/>
    <property name="forceUppercase" value="false"/>
    <property name="label" value="Opportunity Title"/>
    <property name="shortLabel" value="Opportunity Title"/>
    <property name="maxLength" value="255"/>
    <property name="validationPattern">
      <bean parent="AnyCharacterValidationPattern" p:allowWhitespace="true"/>
    </property>
    <property name="required" value="false"/>
    <property name="control">
      <bean parent="TextareaControlDefinition" p:rows="3" p:cols="80"/>
    </property>
    <property name="summary" value="The Opportunity Title for this Proposal."/>
    <property name="description" value="The title of a publicly available document, announcing a federal agency's intentions to award discretionary grants or cooperative agreements, usually as a result of competition for funds.  AKA  Funding opportunity announcements, notices of funding availability, or solicitations."/>
  </bean>
  <bean id="DevelopmentProposal-noticeOfOpportunityCode" parent="DevelopmentProposal-noticeOfOpportunityCode-parentBean"/>

  <bean id="DevelopmentProposal-noticeOfOpportunityCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="noticeOfOpportunityCode"/>
    <property name="forceUppercase" value="false"/>
    <property name="label" value="Notice of Opportunity"/>
    <property name="shortLabel" value="Notice of Opportunity"/>
    <property name="maxLength" value="3"/>
    <property name="validationPattern">
      <bean parent="AnyCharacterValidationPattern"/>
    </property>
    <property name="required" value="false"/>
    <property name="control">
      <bean parent="SelectControlDefinition" p:valuesFinderClass="org.kuali.kra.proposaldevelopment.lookup.keyvalue.NoticeOfOpportunityValuesFinder" p:includeKeyInLabel="false"/>
    </property>
    <property name="summary" value="Notice of Opportunity for this Proposal."/>
    <property name="description" value="Indicates if the proposal was in response to a solicitation, the type of solicitation or if it is unsolicited."/>
  </bean>
  <bean id="DevelopmentProposal-cfdaNumber" parent="DevelopmentProposal-cfdaNumber-parentBean"/>

  <bean id="DevelopmentProposal-cfdaNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="cfdaNumber"/>
    <property name="forceUppercase" value="false"/>
    <property name="label" value="CFDA Number"/>
    <property name="shortLabel" value="CFDA Number"/>
    <property name="maxLength" value="7"/>
    <property name="validationPattern">
      <bean parent="RegexValidationPattern" p:pattern="[0-9]{2}.[0-9]{3}[A-Za-z]" p:validationErrorMessageKey="error.invalidFormat.noValue"/>
    </property>
    <property name="required" value="false"/>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="20"/>
    </property>
    <property name="summary" value="The CFDA Number for this Proposal."/>
    <property name="description" value="A unique identifier for the sponsor and the funding opportunity announcement.  AKA &quot;Catalogue of Federal Domestic Assistance Number.&quot; The format for this CFDA Number is XX.XXX"/>
  </bean>
  <bean id="DevelopmentProposal-programAnnouncementNumber" parent="DevelopmentProposal-programAnnouncementNumber-parentBean"/>

  <bean id="DevelopmentProposal-programAnnouncementNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="programAnnouncementNumber"/>
    <property name="forceUppercase" value="false"/>
    <property name="label" value="Opportunity ID"/>
    <property name="shortLabel" value="Opportunity ID"/>
    <property name="maxLength" value="50"/>
    <property name="validationPattern">
      <bean parent="AnyCharacterValidationPattern" p:allowWhitespace="true"/>
    </property>
    <property name="required" value="false"/>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="20"/>
    </property>
    <property name="summary" value="The Opportunity ID for this Proposal."/>
    <property name="description" value="A unique identifier associated with each sponsor's funding opportunity announcement. AKA &quot;Funding Opportunity Announcement number&quot; or &quot;FOA number&quot;"/>
  </bean>
  <bean id="DevelopmentProposal-sponsorProposalNumber" parent="DevelopmentProposal-sponsorProposalNumber-parentBean"/>

  <bean id="DevelopmentProposal-sponsorProposalNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="sponsorProposalNumber"/>
    <property name="forceUppercase" value="false"/>
    <property name="label" value="Sponsor Proposal ID"/>
    <property name="shortLabel" value="Sponsor Proposal ID"/>
    <property name="maxLength" value="70"/>
    <property name="validationPattern">
      <bean parent="AnyCharacterValidationPattern" p:allowWhitespace="true"/>
    </property>
    <property name="required" value="false"/>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="20"/>
    </property>
    <property name="summary" value="The Sponsor ID for this Proposal."/>
    <property name="description" value="The unique identifier assigned by the sponsor when the proposal was submitted."/>
  </bean>
  <bean id="DevelopmentProposal-subcontracts" parent="DevelopmentProposal-subcontracts-parentBean"/>

  <bean id="DevelopmentProposal-subcontracts-parentBean" abstract="true" parent="AttributeReferenceDummy-genericBoolean">
    <property name="name" value="subcontracts"/>
    <property name="label" value="Does this proposal include subaward(s)?"/>
    <property name="shortLabel" value="Subaward Flag"/>
    <property name="summary" value="Subaward Flag"/>
    <property name="description" value="The proposal will include a sub award with another institution."/>
  </bean>
  <bean id="DevelopmentProposal-agencyProgramCode" parent="DevelopmentProposal-agencyProgramCode-parentBean"/>

  <bean id="DevelopmentProposal-agencyProgramCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="agencyProgramCode"/>
    <property name="forceUppercase" value="false"/>
    <property name="label" value="Sponsor Program Code"/>
    <property name="shortLabel" value="Sponsor Program Code"/>
    <property name="maxLength" value="50"/>
    <property name="validationPattern">
      <bean parent="AnyCharacterValidationPattern" p:allowWhitespace="true"/>
    </property>
    <property name="required" value="false"/>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="50"/>
    </property>
    <property name="summary" value="The Sponsor Program Code for this Proposal."/>
    <property name="description" value="A code that is unique to each NSF program.  Currently specific to the NSF only."/>
  </bean>
  <bean id="DevelopmentProposal-mailBy" parent="DevelopmentProposal-mailBy-parentBean"/>

  <bean id="DevelopmentProposal-mailBy-parentBean" abstract="true" parent="MailBy-mailByCode">
    <property name="name" value="mailBy"/>
    <property name="control">
      <bean parent="SelectControlDefinition" p:businessObjectClass="org.kuali.kra.proposaldevelopment.bo.MailBy" p:valuesFinderClass="org.kuali.kra.lookup.keyvalue.ExtendedPersistableBusinessObjectValuesFinder" p:includeKeyInLabel="false" p:keyAttribute="mailByCode" p:labelAttribute="description"/>
    </property>
    <property name="description" value="This field determines whether the Sponsored Programs Office or the department will be mailing the proposal to the agency/sponsor."/>
  </bean>
  <bean id="DevelopmentProposal-mailType" parent="DevelopmentProposal-mailType-parentBean"/>

  <bean id="DevelopmentProposal-mailType-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="mailType"/>
    <property name="forceUppercase" value="false"/>
    <property name="label" value="Type"/>
    <property name="shortLabel" value="Type"/>
    <property name="maxLength" value="1"/>
    <property name="validationPattern">
      <bean parent="AnyCharacterValidationPattern"/>
    </property>
    <property name="required" value="false"/>
    <property name="control">
      <bean parent="SelectControlDefinition" p:businessObjectClass="org.kuali.kra.proposaldevelopment.bo.MailType" p:valuesFinderClass="org.kuali.kra.lookup.keyvalue.ExtendedPersistableBusinessObjectValuesFinder" p:includeKeyInLabel="false" p:keyAttribute="mailType" p:labelAttribute="description"/>
    </property>
    <property name="summary" value="Mail Type"/>
    <property name="description" value="The method by which the proposal will be delivered to the agency/sponsor."/>
  </bean>
  <bean id="DevelopmentProposal-mailAccountNumber" parent="DevelopmentProposal-mailAccountNumber-parentBean"/>

  <bean id="DevelopmentProposal-mailAccountNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="mailAccountNumber"/>
    <property name="forceUppercase" value="false"/>
    <property name="label" value="Mail Account ID"/>
    <property name="shortLabel" value="Mail Account ID"/>
    <property name="maxLength" value="7"/>
    <property name="validationPattern">
      <bean parent="AnyCharacterValidationPattern" p:allowWhitespace="true"/>
    </property>
    <property name="required" value="false"/>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="10"/>
    </property>
    <property name="summary" value="Mail Account ID"/>
    <property name="description" value="The internal school account number that should be charged the mailing costs."/>
  </bean>
  <bean id="DevelopmentProposal-numberOfCopies" parent="DevelopmentProposal-numberOfCopies-parentBean"/>

  <bean id="DevelopmentProposal-numberOfCopies-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="numberOfCopies"/>
    <property name="forceUppercase" value="false"/>
    <property name="label" value="Number of Copies"/>
    <property name="maxLength" value="2"/>
    <property name="validationPattern">
      <bean parent="NumericValidationPattern"/>
    </property>
    <property name="required" value="false"/>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="3"/>
    </property>
    <property name="summary" value="Number of Copies"/>
    <property name="description" value="The number of paper copies of the proposal that should be mailed to the agency/sponsor."/>
  </bean>
  <bean id="DevelopmentProposal-mailingAddressId" parent="DevelopmentProposal-mailingAddressId-parentBean"/>

  <bean id="DevelopmentProposal-mailingAddressId-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="mailingAddressId"/>
    <property name="forceUppercase" value="false"/>
    <property name="label" value="Mailing Name &amp; Address"/>
