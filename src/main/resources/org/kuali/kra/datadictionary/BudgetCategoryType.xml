<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.kra.budget.bo.BudgetCategoryType">

        <inquiry>
            <title>Budget Category Type</title>

            <inquirySections><inquirySection title="Budget Category Type"><inquiryFields>
                <field attributeName="budgetCategoryTypeCode" forceInquiry="true" />
                <field attributeName="description" />
                <field attributeName="sortId" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Budget Category Type Lookup</title>
			<menubar>
				<![CDATA[<a href="index.jsp">Main</a>]]>
			</menubar>
            <instructions>Look up a Budget Category Type</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="budgetCategoryTypeCode" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="budgetCategoryTypeCode" required="false" />
                <lookupField attributeName="description" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="budgetCategoryTypeCode" forceInquiry="true" />
                <field attributeName="description" forceInquiry="false"/>
                <field attributeName="sortId" forceInquiry="false"/>
            </resultFields>
        </lookup>

        <titleAttribute>budgetCategoryTypeCode</titleAttribute>

        <attributes>

            <attribute name="budgetCategoryTypeCode">
                <label>Budget Category Type</label>
                <shortLabel>Budget Category Type</shortLabel>

                <maxLength>3</maxLength>
                <validationPattern><alphaNumeric /></validationPattern>

                <control>
                    <text size="4" />
                </control>

                <summary>Budget Category Type</summary>
                <description>Budget Category Type</description>
			</attribute>
			
            <attribute name="sortId">
                <label>Sort Id</label>
                <shortLabel>Sort Id</shortLabel>

                <maxLength>2</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="3" />
                </control>

                <summary>Sort Id</summary>
                <description>This sort id is used for sorting budget category.</description>
			</attribute>

            <attributeReference name="description" sourceClassName="org.kuali.kra.bo.KraAttributeReferenceDummy" sourceAttributeName="description" />

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

		</attributes>
    </businessObject>

</dictionaryEntry>
