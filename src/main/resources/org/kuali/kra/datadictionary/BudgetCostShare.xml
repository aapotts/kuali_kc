<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "dataDictionary-1_0.dtd">
<dictionaryEntry>

    <businessObject businessObjectClass="org.kuali.kra.budget.bo.BudgetCostShare">
    
		<titleAttribute>budgetCostShare</titleAttribute>
        <attributes>
