  <bean id="Award" parent="Award-parentBean" />

  <bean id="Award-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kra.award.bo.Award" />
    
    <property name="lookupDefinition" >
      <ref bean="Award-lookupDefinition" />
    </property>
    
    <property name="titleAttribute" value="awardComment" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KRA-A"
            p:parameterName="awardHelpUrl"
            p:parameterDetailType="D" />
    </property>
    <property name="attributes" >
