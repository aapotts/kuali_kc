<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="Award" parent="Award-parentBean" />

  <bean id="Award-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kra.award.bo.Award" />
    <property name="titleAttribute" value="awardComment" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KRA-A"
            p:parameterName="awardHelpUrl"
            p:parameterDetailType="D" />
    </property>
    <property name="attributes" >
      <list>
        <ref bean="Award-preAwardAuthorizedAmount" />
        <ref bean="Award-preAwardEffectiveDate" />
        <ref bean="Award-preAwardInstitutionalAuthorizedAmount" />
        <ref bean="Award-preAwardInstitutionalEffectiveDate" />
        <ref bean="Award-sequenceNumber" />
      </list>
    </property>
  </bean>
  
  <bean id="Award-preAwardAuthorizedAmount" parent="Award-preAwardAuthorizedAmount-parentBean" />

  <bean id="Award-preAwardAuthorizedAmount-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="preAwardAuthorizedAmount" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Amount" />
    <property name="shortLabel" value="Amount" />
    <property name="maxLength" value="12" />
    <property name="validationPattern" >
      <bean parent="FixedPointValidationPattern"
            p:precision="12"
            p:scale="2" />
    </property>
    <property name="required" value="false" />
    <property name="control" >
      <bean parent="CurrencyControlDefinition"
            p:formattedMaxLength="22"
            p:size="16" />
    </property>
    <property name="summary" value="Pre Award Sponsor Authorized Amount" />
    <property name="description" value="Pre Award Sponsor Authorized Amount" />
  </bean>
  
  <bean id="Award-preAwardEffectiveDate" parent="Award-preAwardEffectiveDate-parentBean" />

  <bean id="Award-preAwardEffectiveDate-parentBean" abstract="true" parent="AttributeReferenceDummy-genericDate">
    <property name="name" value="preAwardEffectiveDate" />
    <property name="label" value="Pre Award Sponsor Effective Date" />
    <property name="shortLabel" value="Pre Award Sponsor Effective Date" />
    <property name="required" value="false" />
    <property name="summary" value="Pre Award Sponsor Effective Date" />
    <property name="description" value="Pre Award Sponsor Effective Date" />
  </bean>
  
  <bean id="Award-preAwardInstitutionalAuthorizedAmount" parent="Award-preAwardInstitutionalAuthorizedAmount-parentBean" />

  <bean id="Award-preAwardInstitutionalAuthorizedAmount-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="preAwardInstitutionalAuthorizedAmount" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Amount" />
    <property name="shortLabel" value="Amount" />
    <property name="maxLength" value="12" />
    <property name="validationPattern" >
      <bean parent="FixedPointValidationPattern"
            p:precision="12"
            p:scale="2" />
    </property>
    <property name="required" value="false" />
    <property name="control" >
      <bean parent="CurrencyControlDefinition"
            p:formattedMaxLength="22"
            p:size="16" />
    </property>
    <property name="summary" value="Pre Award Institutional Authorized Amount" />
    <property name="description" value="Pre Award Institutional Authorized Amount" />
  </bean>
  
  <bean id="Award-preAwardInstitutionalEffectiveDate" parent="Award-preAwardInstitutionalEffectiveDate-parentBean" />

  <bean id="Award-preAwardInstitutionalEffectiveDate-parentBean" abstract="true" parent="AttributeReferenceDummy-genericDate">
    <property name="name" value="preAwardInstitutionalEffectiveDate" />
    <property name="label" value="Pre Award Institutional Effective Date" />
    <property name="shortLabel" value="Pre Award Institutional Effective Date" />
    <property name="required" value="false" />
    <property name="summary" value="Pre Award Institutional Effective Date" />
    <property name="description" value="Pre Award Institutional Effective Date" />
  </bean>
  
  
   <bean id="Award-sequenceNumber" parent="Award-sequenceNumber-parentBean" />

  <bean id="Award-sequenceNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="sequenceNumber" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Sequence Number" />
    <property name="shortLabel" value="Sequence Number" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
    <property name="summary" value="Sequence Number" />
    <property name="description" value="Sequence Number" />
  </bean>
  
  </beans>
  