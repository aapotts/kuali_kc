    <property name="title" value="Award Lookup" />
    <property name="lookupableID" value="sponsorLookupable" />
    <property name="menubar" value="&lt;a href=&quot;index.jsp&quot;&gt;Main&lt;/a&gt;" />
    <property name="instructions" value="Look up an Award" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="awardId" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="awardId"
              p:forceInquiry="true" />
        <bean parent="FieldDefinition"
              p:attributeName="title" />
        <bean parent="FieldDefinition"
              p:attributeName="awardNumber" />              
        <bean parent="FieldDefinition"
              p:attributeName="sponsorName" />
