		</property>
		<property name="summary" value="Membership Id" />
		<property name="description" value="Membership Id" />
	</bean>

	<bean id="CommitteeMembership-sequenceNumber" parent="CommitteeMembership-sequenceNumber-parentBean" />
	<bean id="CommitteeMembership-sequenceNumber-parentBean" abstract="true" parent="AttributeDefinition" >
		<property name="name" value="sequenceNumber" />
		<property name="forceUppercase" value="false" />
		<property name="label" value="Sequence Number" />
		<property name="shortLabel" value="Sequence Number" />
