	<bean id="CommitteeMembership-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber" />

	<!-- Business Object Inquiry Definition -->
	<bean id="CommitteeMembership-inquiryDefinition" parent="CommitteeMembership-inquiryDefinition-parentBean" />
	<bean id="CommitteeMembership-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
		<property name="title" value="Committee Memberships Id" />
		<property name="inquirySections" >
			<list>
				<bean parent="InquirySectionDefinition">
					<property name="title" value="Committee Membership Id" />
					<property name="numberOfColumns" value="1" />
					<property name="inquiryFields" >
						<list>
							<bean parent="FieldDefinition"
								p:attributeName="committeeMembershipsId" 
								p:forceInquiry="true" />
							<bean parent="FieldDefinition"
								p:attributeName="committeeId" />
							<bean parent="FieldDefinition"
								p:attributeName="personId" />
                            <bean parent="FieldDefinition"
                                p:attributeName="rolodexId" />
                            <bean parent="FieldDefinition"
                                p:attributeName="personName" />
							<bean parent="FieldDefinition"
								p:attributeName="membershipId" />
							<bean parent="FieldDefinition"
								p:attributeName="sequenceNumber" />
							<bean parent="FieldDefinition"
								p:attributeName="paidMember" />
							<bean parent="FieldDefinition"
								p:attributeName="termStartDate" />
							<bean parent="FieldDefinition"
								p:attributeName="termEndDate" />
							<bean parent="FieldDefinition"
								p:attributeName="membershipTypeCode" />
							<bean parent="FieldDefinition"
								p:attributeName="comments" />
                            <bean parent="FieldDefinition"
                                p:attributeName="contactNotes" />
                            <bean parent="FieldDefinition"
                                p:attributeName="trainingNotes" />
						</list>
					</property>
				</bean>
			</list>
		</property>
	</bean>

	<!-- Business Object Lookup Definition -->
	<bean id="CommitteeMembership-lookupDefinition" parent="CommitteeMembership-lookupDefinition-parentBean" />
	<bean id="CommitteeMembership-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
		<property name="title" value="Committee Membership Lookup" />
		<property name="menubar" value="&lt;a href=&quot;index.jsp&quot;&gt;Main&lt;/a&gt;" />
		<property name="instructions" value="Look up a Comm Memberships" />
		<property name="defaultSort" >
			<bean parent="SortDefinition" >
		</bean>
		</property>
		<property name="lookupFields" >
			<list>
				<bean parent="FieldDefinition"
					p:attributeName="committeeMembershipsId" 
					p:forceInquiry="true" />
				<bean parent="FieldDefinition"
					p:attributeName="committeeId" />
				<bean parent="FieldDefinition"
					p:attributeName="personId" />
                <bean parent="FieldDefinition"
                    p:attributeName="rolodexId" />
                <bean parent="FieldDefinition"
                    p:attributeName="personName" />
				<bean parent="FieldDefinition"
					p:attributeName="membershipId" />
				<bean parent="FieldDefinition"
					p:attributeName="sequenceNumber" />
				<bean parent="FieldDefinition"
					p:attributeName="paidMember" />
				<bean parent="FieldDefinition"
					p:attributeName="termStartDate" />
				<bean parent="FieldDefinition"
					p:attributeName="termEndDate" />
				<bean parent="FieldDefinition"
					p:attributeName="membershipTypeCode" />
				<bean parent="FieldDefinition"
					p:attributeName="comments" />
                <bean parent="FieldDefinition"
                    p:attributeName="contactNotes" />
                <bean parent="FieldDefinition"
                    p:attributeName="trainingNotes" />
			</list>
		</property>
		<property name="resultFields" >
			<list>
				<bean parent="FieldDefinition"
					p:attributeName="committeeMembershipsId" 
					p:forceInquiry="true" />
				<bean parent="FieldDefinition"
					p:attributeName="committeeId" />
				<bean parent="FieldDefinition"
					p:attributeName="personId" />
                <bean parent="FieldDefinition"
                    p:attributeName="rolodexId" />
                <bean parent="FieldDefinition"
                    p:attributeName="personName" />
				<bean parent="FieldDefinition"
					p:attributeName="membershipId" />
				<bean parent="FieldDefinition"
					p:attributeName="sequenceNumber" />
				<bean parent="FieldDefinition"
					p:attributeName="paidMember" />
				<bean parent="FieldDefinition"
					p:attributeName="termStartDate" />
				<bean parent="FieldDefinition"
					p:attributeName="termEndDate" />
				<bean parent="FieldDefinition"
					p:attributeName="membershipTypeCode" />
				<bean parent="FieldDefinition"
					p:attributeName="comments" />
                <bean parent="FieldDefinition"
                    p:attributeName="contactNotes" />
                <bean parent="FieldDefinition"
                    p:attributeName="trainingNotes" />
			</list>
		</property>
