<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="CommitteeMembership" parent="CommitteeMembership-parentBean" />
	<bean id="CommitteeMembership-parentBean" abstract="true" parent="BusinessObjectEntry">
		<property name="businessObjectClass" value="org.kuali.kra.committee.bo.CommitteeMembership" />
		<property name="inquiryDefinition" >
			<ref bean="CommitteeMembership-inquiryDefinition" />
		</property>
		<property name="lookupDefinition" >
			<ref bean="CommitteeMembership-lookupDefinition" />
		</property>
		<property name="titleAttribute" value="committeeMembershipsId" />
		<property name="attributes" >
			<list>
				<ref bean="CommitteeMembership-committeeMembershipsId" />
				<ref bean="CommitteeMembership-committeeId" />
				<ref bean="CommitteeMembership-personId" />
                <ref bean="CommitteeMembership-rolodexId" />
                <ref bean="CommitteeMembership-personName" />
				<ref bean="CommitteeMembership-membershipId" />
				<ref bean="CommitteeMembership-sequenceNumber" />
				<ref bean="CommitteeMembership-paidMember" />
				<ref bean="CommitteeMembership-termStartDate" />
				<ref bean="CommitteeMembership-termEndDate" />
				<ref bean="CommitteeMembership-membershipTypeCode" />
				<ref bean="CommitteeMembership-comments" />
<!--				<ref bean="CommitteeMembership-contactNotes" />-->
<!--				<ref bean="CommitteeMembership-trainingNotes" />-->
				<ref bean="CommitteeMembership-versionNumber" />
			</list>
		</property>
	</bean>

	<!-- Attribute Definitions -->
	
	<bean id="CommitteeMembership-committeeMembershipsId" parent="CommitteeMembership-committeeMembershipsId-parentBean" />
	<bean id="CommitteeMembership-committeeMembershipsId-parentBean" abstract="true" parent="AttributeDefinition" >
		<property name="name" value="committeeMembershipsId" />
		<property name="forceUppercase" value="false" />
		<property name="label" value="Committee Memberships Id" />
		<property name="shortLabel" value="Committee Memberships Id" />
		<property name="maxLength" value="22" />
		<property name="validationPattern" >
			<bean parent="NumericValidationPattern" />
		</property>
		<property name="control" >
			<bean parent="TextControlDefinition"
				p:size="10" />
		</property>
		<property name="summary" value="Committee Memberships Id" />
		<property name="description" value="Committee Memberships Id" />
	</bean>

	<bean id="CommitteeMembership-committeeId" parent="CommitteeMembership-committeeId-parentBean" />
	<bean id="CommitteeMembership-committeeId-parentBean" abstract="true" parent="AttributeDefinition" >
