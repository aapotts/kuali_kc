<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.kra.budget.bo.BudgetPerson">

        <inquiry>
            <title>Budget Persons</title>

            <inquirySections><inquirySection title="Budget Persons"><inquiryFields>
            	<field attributeName="effectiveDate" forceInquiry="true"/>
				<field attributeName="jobCode" forceInquiry="true"/>
				<field attributeName="nonEmployeeFlag" forceInquiry="true"/>
				<field attributeName="personId" forceInquiry="true"/>
				<field attributeName="proposalNumber" forceInquiry="true"/>
				<field attributeName="versionNumber" forceInquiry="true"/>
				<field attributeName="appointmentType"/>
				<field attributeName="calculationBase"/>
				<field attributeName="personName"/>

            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Budget Persons Lookup</title>
			<menubar>
				<![CDATA[<a href="index.jsp">Main</a>]]>
			</menubar>
            <instructions>Look up a Budget Persons</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                	<sortAttribute attributeName="effectiveDate"/>
					<sortAttribute attributeName="jobCode"/>
					<sortAttribute attributeName="nonEmployeeFlag"/>
					<sortAttribute attributeName="personId"/>
					<sortAttribute attributeName="proposalNumber"/>
					<sortAttribute attributeName="versionNumber"/>

                </sortAttributes>
            </defaultSort>

            <lookupFields>
            	<lookupField attributeName="effectiveDate" required="false"/>
				<lookupField attributeName="jobCode" required="false"/>
				<lookupField attributeName="nonEmployeeFlag" required="false"/>
				<lookupField attributeName="personId" required="false"/>
				<lookupField attributeName="proposalNumber" required="false"/>
				<lookupField attributeName="versionNumber" required="false"/>
				<lookupField attributeName="appointmentType" required="false"/>
				<lookupField attributeName="calculationBase" required="false"/>
				<lookupField attributeName="personName" required="false"/>

            </lookupFields>

            <resultFields>
            	<field attributeName="effectiveDate" forceInquiry="true"/>
				<field attributeName="jobCode" forceInquiry="true"/>
				<field attributeName="nonEmployeeFlag" forceInquiry="true"/>
				<field attributeName="personId" forceInquiry="true"/>
				<field attributeName="proposalNumber" forceInquiry="true"/>
				<field attributeName="versionNumber" forceInquiry="true"/>
				<field attributeName="appointmentType" forceInquiry="false"/>
				<field attributeName="calculationBase" forceInquiry="false"/>
				<field attributeName="personName" forceInquiry="false"/>

            </resultFields>
        </lookup>

		<titleAttribute>personId</titleAttribute>
        <attributes>

            <attributeReference name="effectiveDate" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericDate" >
                <label>Salary Effective Date</label>
                <shortLabel>Salary Effective Date</shortLabel>

 				<required>true</required>

                <summary>Salary Effective Date</summary>
                <description>Salary Effective Date.</description>
      		</attributeReference>


			<attribute name="jobCode">
			<label>Job Code </label>
			<shortLabel>Job Code </shortLabel>
			<maxLength>6</maxLength>
			<validationPattern><alphaNumeric /></validationPattern>
			<control>
				<text size="6" />
			</control>
			<summary>Job Code </summary>
			<description>Job Code </description>
			</attribute>

			<attribute name="nonEmployeeFlag">
			<label>Non Employee Flag </label>
			<shortLabel>Non Employee Flag </shortLabel>
			<maxLength>1</maxLength>
			<validationPattern><alphaNumeric /></validationPattern>
			<control>
				<text size="1" />
			</control>
			<summary>Non Employee Flag </summary>
			<description>Non Employee Flag </description>
			</attribute>

			<attribute name="personId">
			<label>Person Id </label>
			<shortLabel>Person Id </shortLabel>
			<maxLength>10</maxLength>
			<validationPattern><numeric /></validationPattern>
			<control>
				<text size="10" />
			</control>
			<summary>Person Id </summary>
			<description>Person Id </description>
			</attribute>

			<attribute name="proposalNumber">
			<label>Proposal Number </label>
			<shortLabel>Proposal Number </shortLabel>
			<maxLength>12</maxLength>
			<validationPattern><alphaNumeric /></validationPattern>
			<control>
				<text size="8" />
			</control>
			<summary>Proposal Number </summary>
			<description>Proposal Number </description>
			</attribute>

			<attribute name="versionNumber">
			<label>Version Number </label>
			<shortLabel>Version Number </shortLabel>
			<maxLength>3</maxLength>
			<validationPattern><numeric /></validationPattern>
			<control>
				<text size="3" />
			</control>
			<summary>Version Number </summary>
			<description>Version Number </description>
			</attribute>

			<attribute name="appointmentType">
			<label>Appointment Type </label>
			<shortLabel>Appointment Type </shortLabel>
			<maxLength>2</maxLength>
			<validationPattern><alphaNumeric /></validationPattern>
			<required>true</required>
			<control>
                <select valuesFinderClass="org.kuali.core.lookup.keyvalues.PersistableBusinessObjectValuesFinder" businessObjectClass="org.kuali.kra.budget.bo.AppointmentType" keyAttribute="appointmentTypeCode" labelAttribute="description" includeKeyInLabel="false"/>
            </control>
			<summary>Appointment Type </summary>
			<description>Appointment Type </description>
			</attribute>

			<attributeReference name="calculationBase" sourceClassName="org.kuali.kra.bo.KraAttributeReferenceDummy" sourceAttributeName="genericAmount">
			<label>Base Salary</label>
			<shortLabel>Base Salary</shortLabel>
			<required>true</required>
			<summary>Base Salary </summary>
			<description>Calculation Base </description>			
			</attributeReference>

			<attributeReference name="personName" sourceClassName="org.kuali.kra.bo.Person" sourceAttributeName="fullName" >
			<label>Personnel</label>
			<shortLabel>Personnel</shortLabel>
			<summary>Personnel</summary>
			<description>Personnel</description>			
			</attributeReference>

			<attributeReference name="jobTitle" sourceClassName="org.kuali.kra.budget.bo.JobCode" sourceAttributeName="jobTitle" />
			
		</attributes>

        <objectHelp parameterNamespace="KRA-B" parameterDetailType="D" parameterName="budgetPersonHelpUrl"/> 
    </businessObject>

</dictionaryEntry>
