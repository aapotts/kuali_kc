<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="PostSubmissionStatus" parent="PostSubmissionStatus-parentBean" />

  <bean id="PostSubmissionStatus-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kra.proposaldevelopment.bo.PostSubmissionStatus" />
    <property name="inquiryDefinition" >
      <ref bean="PostSubmissionStatus-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="PostSubmissionStatus-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="postSubmissionStatusCode" />
    <property name="attributes" >
      <list>
        <ref bean="PostSubmissionStatus-postSubmissionStatusCode" />
        <ref bean="PostSubmissionStatus-description" />
        <ref bean="PostSubmissionStatus-definition" />
        <ref bean="PostSubmissionStatus-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="PostSubmissionStatus-postSubmissionStatusCode" parent="PostSubmissionStatus-postSubmissionStatusCode-parentBean" />

  <bean id="PostSubmissionStatus-postSubmissionStatusCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="postSubmissionStatusCode" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Post Submission Status Type" />
    <property name="shortLabel" value="Post Submission Status Type" />
    <property name="maxLength" value="3" />
    <property name="validationPattern" >
      <bean parent="AlphaNumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
    <property name="summary" value="Post Submission Status Code" />
    <property name="description" value="Post Submission Status Code" />
  </bean>
  <bean id="PostSubmissionStatus-description" parent="PostSubmissionStatus-description-parentBean" />

  <bean id="PostSubmissionStatus-description-parentBean" abstract="true" parent="KraAttributeReferenceDummy-description">
  </bean>
  <bean id="PostSubmissionStatus-definition" parent="PostSubmissionStatus-definition-parentBean" />

  <bean id="PostSubmissionStatus-definition-parentBean" abstract="true" parent="KraAttributeReferenceDummy-description">
    <property name="name" value="definition" />
    <property name="label" value="Definition" />
    <property name="shortLabel" value="Definition" />
    <property name="summary" value="Definition" />
    <property name="description" value="Definition" />
  </bean>
  <bean id="PostSubmissionStatus-versionNumber" parent="PostSubmissionStatus-versionNumber-parentBean" />

  <bean id="PostSubmissionStatus-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="PostSubmissionStatus-inquiryDefinition" parent="PostSubmissionStatus-inquiryDefinition-parentBean" />

  <bean id="PostSubmissionStatus-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Post Submission Status Code" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Post Submission Status Code" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="postSubmissionStatusCode"
                    p:forceInquiry="true" />
              <bean parent="FieldDefinition"
                    p:attributeName="description" />
              <bean parent="FieldDefinition"
                    p:attributeName="definition" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="PostSubmissionStatus-lookupDefinition" parent="PostSubmissionStatus-lookupDefinition-parentBean" />

  <bean id="PostSubmissionStatus-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Post Submission Status Code Lookup" />
    <property name="menubar" value="&lt;a href=&quot;index.jsp&quot;&gt;Main&lt;/a&gt;" />
    <property name="instructions" value="Look up a Post Submission Status Code" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="postSubmissionStatusCode" />
        <bean parent="FieldDefinition"
              p:attributeName="description" />
        <bean parent="FieldDefinition"
              p:attributeName="definition" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="postSubmissionStatusCode"
              p:forceInquiry="true" />
        <bean parent="FieldDefinition"
              p:attributeName="description" />
        <bean parent="FieldDefinition"
              p:attributeName="definition" />
      </list>
    </property>
  </bean>
</beans>
