<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="AwardSpecialReview" parent="AwardSpecialReview-parentBean" />
	<bean id="AwardSpecialReview-parentBean" abstract="true" parent="BusinessObjectEntry">
		<property name="businessObjectClass" value="org.kuali.kra.award.bo.AwardSpecialReview" />
		<property name="inquiryDefinition" >
			<ref bean="AwardSpecialReview-inquiryDefinition" />
		</property>
		<property name="lookupDefinition" >
			<ref bean="AwardSpecialReview-lookupDefinition" />
		</property>
		<property name="titleAttribute" value="awardSpecialReviewId" />
		<property name="attributes" >
			<list>
				<ref bean="AwardSpecialReview-awardSpecialReviewId" />
				<ref bean="AwardSpecialReview-specialReviewNumber" />
				<ref bean="AwardSpecialReview-specialReviewCode" />
				<ref bean="AwardSpecialReview-approvalTypeCode" />
				<ref bean="AwardSpecialReview-protocolNumber" />
				<ref bean="AwardSpecialReview-applicationDate" />
				<ref bean="AwardSpecialReview-approvalDate" />
				<ref bean="AwardSpecialReview-comments" />
				<ref bean="AwardSpecialReview-versionNumber" />
			</list>
		</property>
	</bean>

	<!-- Attribute Definitions -->
	
	<bean id="AwardSpecialReview-awardSpecialReviewId" parent="AwardSpecialReview-awardSpecialReviewId-parentBean" />
	<bean id="AwardSpecialReview-awardSpecialReviewId-parentBean" abstract="true" parent="AttributeDefinition" >
		<property name="name" value="awardSpecialReviewId" />
		<property name="forceUppercase" value="false" />
		<property name="label" value="Award Special Review Id" />
		<property name="shortLabel" value="Award Special Review Id" />
		<property name="maxLength" value="22" />
		<property name="validationPattern" >
			<bean parent="NumericValidationPattern" />
		</property>
		<property name="control" >
			<bean parent="TextControlDefinition"
				p:size="10" />
		</property>
		<property name="summary" value="Award Special Review Id" />
		<property name="description" value="Award Special Review Id" />
	</bean>

	<bean id="AwardSpecialReview-specialReviewNumber" parent="AwardSpecialReview-specialReviewNumber-parentBean" />
	<bean id="AwardSpecialReview-specialReviewNumber-parentBean" abstract="true" parent="AttributeDefinition" >
		<property name="name" value="specialReviewNumber" />
		<property name="forceUppercase" value="false" />
		<property name="label" value="Special Review Number" />
		<property name="shortLabel" value="Special Review Number" />
		<property name="maxLength" value="22" />
		<property name="validationPattern" >
			<bean parent="NumericValidationPattern" />
		</property>
		<property name="control" >
			<bean parent="TextControlDefinition"
				p:size="10" />
		</property>
		<property name="summary" value="Special Review Number" />
		<property name="description" value="Special Review Number" />
	</bean>

	<bean id="AwardSpecialReview-specialReviewCode" parent="AwardSpecialReview-specialReviewCode-parentBean" />
	<bean id="AwardSpecialReview-specialReviewCode-parentBean" abstract="true" parent="AttributeDefinition" >
	    <property name="name" value="specialReviewCode" />
	    <property name="forceUppercase" value="false" />
	    <property name="label" value="Type" />
	    <property name="shortLabel" value="Special Review Code" />
	    <property name="maxLength" value="3" />
	    <property name="validationPattern" >
	      <bean parent="AlphaNumericValidationPattern" />
	    </property>
	    <property name="required" value="true" />
	    <property name="control" >
	      <bean parent="SelectControlDefinition"
	            p:businessObjectClass="org.kuali.kra.bo.SpecialReview"
	            p:valuesFinderClass="org.kuali.kra.lookup.keyvalue.ExtendedPersistableBusinessObjectValuesFinder"
	            p:includeKeyInLabel="false"
	            p:keyAttribute="specialReviewCode"
	            p:labelAttribute="description" />
	    </property>
	    <property name="summary" value="Special Review Code" />
	    <property name="description" value="The type indicates which special review, if any, is relevant to the document (proposal/award/irb)." />
	</bean>

	<bean id="AwardSpecialReview-approvalTypeCode" parent="AwardSpecialReview-approvalTypeCode-parentBean" />
	<bean id="AwardSpecialReview-approvalTypeCode-parentBean" abstract="true" parent="AttributeDefinition" >
	    <property name="name" value="approvalTypeCode" />
	    <property name="forceUppercase" value="false" />
	    <property name="label" value="Approval Status" />
	    <property name="shortLabel" value="Approval Type Code" />
	    <property name="maxLength" value="3" />
	    <property name="validationPattern" >
	      <bean parent="AlphaNumericValidationPattern" />
	    </property>
	    <property name="required" value="true" />
	    <property name="control" >
	      <bean parent="SelectControlDefinition"
	            p:valuesFinderClass="org.kuali.kra.proposaldevelopment.lookup.keyvalue.SpecialReviewApprovalTypeValuesFinder"
	            p:includeKeyInLabel="false" />
	    </property>
	    <property name="summary" value="Approval Type Code" />
	    <property name="description" value="The approval status indicates if the special review relevant to the document (proposal/award/irb) is pending approval, or already approved." />
	</bean>

	<bean id="AwardSpecialReview-protocolNumber" parent="AwardSpecialReview-protocolNumber-parentBean" />
	<bean id="AwardSpecialReview-protocolNumber-parentBean" abstract="true" parent="AttributeDefinition" >
		<property name="name" value="protocolNumber" />
		<property name="forceUppercase" value="false" />
		<property name="label" value="Protocol Number" />
		<property name="shortLabel" value="Protocol Number" />
		<property name="maxLength" value="20" />
		<property name="validationPattern" >
			<bean parent="AlphaNumericValidationPattern" />
		</property>
		<property name="control" >
			<bean parent="TextControlDefinition"
				p:size="10" />
		</property>
		<property name="summary" value="Protocol Number" />
		<property name="description" value="Protocol Number" />
	</bean>

	<bean id="AwardSpecialReview-applicationDate" parent="AwardSpecialReview-applicationDate-parentBean" />
	<bean id="AwardSpecialReview-applicationDate-parentBean" abstract="true" parent="KraAttributeReferenceDummy-genericDate" >
		<property name="name" value="applicationDate" />
		<property name="label" value="Application Date" />
		<property name="shortLabel" value="Application Date" />
		<property name="summary" value="Application Date" />
		<property name="description" value="Application Date" />
	</bean>

	<bean id="AwardSpecialReview-approvalDate" parent="AwardSpecialReview-approvalDate-parentBean" />
	<bean id="AwardSpecialReview-approvalDate-parentBean" abstract="true" parent="KraAttributeReferenceDummy-genericDate" >
		<property name="name" value="approvalDate" />
		<property name="label" value="Approval Date" />
		<property name="shortLabel" value="Approval Date" />
		<property name="summary" value="Approval Date" />
		<property name="description" value="Approval Date" />
	</bean>

	<bean id="AwardSpecialReview-comments" parent="AwardSpecialReview-comments-parentBean" />
	<bean id="AwardSpecialReview-comments-parentBean" abstract="true" parent="AttributeDefinition" >
		<property name="name" value="comments" />
		<property name="forceUppercase" value="false" />
		<property name="label" value="Comments" />
		<property name="shortLabel" value="Comments" />
		<property name="maxLength" value="0" />
		<property name="validationPattern" >
			<bean parent="AlphaNumericValidationPattern" />
		</property>
		<property name="control" >
			<bean parent="TextControlDefinition"
				p:size="5" />
		</property>
		<property name="summary" value="Comments" />
		<property name="description" value="Comments" />
	</bean>

	<bean id="AwardSpecialReview-versionNumber" parent="AwardSpecialReview-versionNumber-parentBean" />
	<bean id="AwardSpecialReview-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
	</bean>

	<!-- Business Object Inquiry Definition -->
	<bean id="AwardSpecialReview-inquiryDefinition" parent="AwardSpecialReview-inquiryDefinition-parentBean" />
	<bean id="AwardSpecialReview-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
		<property name="title" value="Award Special Review Id" />
		<property name="inquirySections" >
			<list>
				<bean parent="InquirySectionDefinition">
					<property name="title" value="Award Special Review Id" />
					<property name="numberOfColumns" value="1" />
					<property name="inquiryFields" >
						<list>
							<bean parent="FieldDefinition"
								p:attributeName="awardSpecialReviewId" 
								p:forceInquiry="true" />
							<bean parent="FieldDefinition"
								p:attributeName="specialReviewNumber" />
							<bean parent="FieldDefinition"
								p:attributeName="specialReviewCode" />
							<bean parent="FieldDefinition"
								p:attributeName="approvalTypeCode" />
							<bean parent="FieldDefinition"
								p:attributeName="protocolNumber" />
							<bean parent="FieldDefinition"
								p:attributeName="applicationDate" />
							<bean parent="FieldDefinition"
								p:attributeName="approvalDate" />
							<bean parent="FieldDefinition"
								p:attributeName="comments" />
						</list>
					</property>
				</bean>
			</list>
		</property>
	</bean>

	<!-- Business Object Lookup Definition -->
	<bean id="AwardSpecialReview-lookupDefinition" parent="AwardSpecialReview-lookupDefinition-parentBean" />
	<bean id="AwardSpecialReview-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
		<property name="title" value="Award Special Review Lookup" />
		<property name="menubar" value="&lt;a href=&quot;index.jsp&quot;&gt;Main&lt;/a&gt;" />
		<property name="instructions" value="Look up a Award Special Review" />
		<property name="defaultSort" >
			<bean parent="SortDefinition" >
		</bean>
		</property>
		<property name="lookupFields" >
			<list>
				<bean parent="FieldDefinition"
					p:attributeName="awardSpecialReviewId" 
					p:forceInquiry="true" />
				<bean parent="FieldDefinition"
					p:attributeName="specialReviewNumber" />
				<bean parent="FieldDefinition"
					p:attributeName="specialReviewCode" />
				<bean parent="FieldDefinition"
					p:attributeName="approvalTypeCode" />
				<bean parent="FieldDefinition"
					p:attributeName="protocolNumber" />
				<bean parent="FieldDefinition"
					p:attributeName="applicationDate" />
				<bean parent="FieldDefinition"
					p:attributeName="approvalDate" />
				<bean parent="FieldDefinition"
					p:attributeName="comments" />
			</list>
		</property>
		<property name="resultFields" >
			<list>
				<bean parent="FieldDefinition"
					p:attributeName="awardSpecialReviewId" 
					p:forceInquiry="true" />
				<bean parent="FieldDefinition"
					p:attributeName="specialReviewNumber" />
				<bean parent="FieldDefinition"
					p:attributeName="specialReviewCode" />
				<bean parent="FieldDefinition"
					p:attributeName="approvalTypeCode" />
				<bean parent="FieldDefinition"
					p:attributeName="protocolNumber" />
				<bean parent="FieldDefinition"
					p:attributeName="applicationDate" />
				<bean parent="FieldDefinition"
					p:attributeName="approvalDate" />
				<bean parent="FieldDefinition"
					p:attributeName="comments" />
			</list>
		</property>
	</bean>
</beans>
