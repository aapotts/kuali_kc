<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.kra.budget.bo.BudgetLineItemCalculatedAmount">

        <inquiry>
            <title>Budget Details Cal Amts</title>

            <inquirySections><inquirySection title="Budget Details Cal Amts"><inquiryFields>
            	<field attributeName="budgetPeriod" forceInquiry="true"/>
				<field attributeName="lineItemNumber" forceInquiry="true"/>
				<field attributeName="proposalNumber" forceInquiry="true"/>
				<field attributeName="rateClassCode" forceInquiry="true"/>
				<field attributeName="rateTypeCode" forceInquiry="true"/>
				<field attributeName="versionNumber" forceInquiry="true"/>
				<field attributeName="applyRateFlag"/>
				<field attributeName="calculatedCost"/>
				<field attributeName="calculatedCostSharing"/>

            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Budget Details Cal Amts Lookup</title>
			<menubar>
				<![CDATA[<a href="index.jsp">Main</a>]]>
			</menubar>
            <instructions>Look up a Budget Details Cal Amts</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                	<sortAttribute attributeName="budgetPeriod"/>
					<sortAttribute attributeName="lineItemNumber"/>
					<sortAttribute attributeName="proposalNumber"/>
					<sortAttribute attributeName="rateClassCode"/>
					<sortAttribute attributeName="rateTypeCode"/>
					<sortAttribute attributeName="versionNumber"/>

                </sortAttributes>
            </defaultSort>

            <lookupFields>
            	<lookupField attributeName="budgetPeriod" required="false"/>
				<lookupField attributeName="lineItemNumber" required="false"/>
				<lookupField attributeName="proposalNumber" required="false"/>
				<lookupField attributeName="rateClassCode" required="false"/>
				<lookupField attributeName="rateTypeCode" required="false"/>
				<lookupField attributeName="versionNumber" required="false"/>
				<lookupField attributeName="applyRateFlag" required="false"/>
				<lookupField attributeName="calculatedCost" required="false"/>
				<lookupField attributeName="calculatedCostSharing" required="false"/>

            </lookupFields>

            <resultFields>
            	<field attributeName="budgetPeriod" forceInquiry="true"/>
				<field attributeName="lineItemNumber" forceInquiry="true"/>
				<field attributeName="proposalNumber" forceInquiry="true"/>
				<field attributeName="rateClassCode" forceInquiry="true"/>
				<field attributeName="rateTypeCode" forceInquiry="true"/>
				<field attributeName="versionNumber" forceInquiry="true"/>
				<field attributeName="applyRateFlag" forceInquiry="false"/>
				<field attributeName="calculatedCost" forceInquiry="false"/>
				<field attributeName="calculatedCostSharing" forceInquiry="false"/>

            </resultFields>
        </lookup>

        <titleAttribute>budgetPeriod</titleAttribute>
		<!-- <titleAttribute>lineItemNumber</titleAttribute>
		<titleAttribute>proposalNumber</titleAttribute>
		<titleAttribute>rateClassCode</titleAttribute>
		<titleAttribute>rateTypeCode</titleAttribute>
		<titleAttribute>versionNumber</titleAttribute> -->
        <attributes>
	        <attribute name="budgetPeriod">
			<label>Budget Period </label>
			<shortLabel>Budget Period </shortLabel>
			<maxLength>3</maxLength>
			<validationPattern><numeric /></validationPattern>
			<control>
				<text size="3" />
			</control>
			<summary>Budget Period </summary>
			<description>Budget Period </description>
			</attribute>

			<attribute name="lineItemNumber">
			<label>Line Item Number </label>
			<shortLabel>Line Item Number </shortLabel>
			<maxLength>3</maxLength>
			<validationPattern><numeric /></validationPattern>
			<control>
				<text size="3" />
			</control>
			<summary>Line Item Number </summary>
			<description>Line Item Number </description>
			</attribute>

			<attribute name="proposalNumber">
			<label>Proposal Number </label>
			<shortLabel>Proposal Number </shortLabel>
			<maxLength>12</maxLength>
			<validationPattern><alphaNumeric /></validationPattern>
			<control>
				<text size="12" />
			</control>
			<summary>Proposal Number </summary>
			<description>Proposal Number </description>
			</attribute>

			<attribute name="rateClassCode">
			<label>Rate Class </label>
			<shortLabel>Rate Class </shortLabel>
			<maxLength>3</maxLength>
			<validationPattern><numeric /></validationPattern>
			<control>
				<hidden />
			</control>
			<summary>Rate Class </summary>
			<description>Rate Class </description>
			</attribute>

			<attribute name="rateTypeCode">
			<label>Rate Type </label>
			<shortLabel>Rate Type </shortLabel>
			<maxLength>3</maxLength>
			<validationPattern><numeric /></validationPattern>
			<control>
				<hidden />
			</control>
			<summary>Rate Type </summary>
			<description>Rate Type </description>
			</attribute>

			<attribute name="versionNumber">
			<label>Version Number </label>
			<shortLabel>Version Number </shortLabel>
			<maxLength>3</maxLength>
			<validationPattern><numeric /></validationPattern>
			<control>
				<text size="3" />
			</control>
			<summary>Version Number </summary>
			<description>Version Number </description>
			</attribute>

			<attribute name="applyRateFlag">
			<label>Apply Rate? </label>
			<shortLabel>Apply Rate? </shortLabel>
			<maxLength>1</maxLength>
			<validationPattern><alphaNumeric /></validationPattern>
			<control>
				<checkbox />
			</control>
			<summary>Apply Rate? </summary>
			<description>Apply Rate? </description>
			</attribute>

            <attributeReference name="calculatedCost" sourceClassName="org.kuali.kra.bo.KraAttributeReferenceDummy" sourceAttributeName="genericAmount">
			<label>Rate Cost </label>
			<shortLabel>Rate Cost </shortLabel>
			<summary>Rate Cost </summary>
			<description>Rate Cost </description>
			</attributeReference>

            <attributeReference name="calculatedCostSharing" sourceClassName="org.kuali.kra.bo.KraAttributeReferenceDummy" sourceAttributeName="genericAmount">
			<label>Rate Cost Sharing </label>
			<shortLabel>Rate Cost Sharing </shortLabel>
			<summary>Rate Cost Sharing </summary>
			<description>Rate Cost Sharing </description>
			</attributeReference>

		</attributes>
    </businessObject>

</dictionaryEntry>
