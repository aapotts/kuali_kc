<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.kra.bo.RateType">

        <inquiry>
            <title>Rate Type</title>

            <inquirySections><inquirySection title="Rate Type"><inquiryFields>
            	<field attributeName="rateClassCode" forceInquiry="true"/>
				<field attributeName="rateTypeCode" forceInquiry="true"/>
				<field attributeName="description"/>

            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Rate Type Lookup</title>
			<menubar>
				<![CDATA[<a href="/kra-dev/">Main</a>]]>
			</menubar>
            <instructions>Look up a Rate Type</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                	<sortAttribute attributeName="rateClassCode"/>
					<sortAttribute attributeName="rateTypeCode"/>

                </sortAttributes>
            </defaultSort>

            <lookupFields>
            	<lookupField attributeName="rateClassCode" required="false"/>
				<lookupField attributeName="rateTypeCode" required="false"/>
				<lookupField attributeName="description" required="false"/>

            </lookupFields>

            <resultFields>
            	<field attributeName="rateClassCode" forceInquiry="true"/>
				<field attributeName="rateTypeCode" forceInquiry="true"/>
				<field attributeName="description" forceInquiry="false"/>

            </resultFields>
        </lookup>

        <titleAttribute>rateClassCode</titleAttribute>
		<titleAttribute>rateTypeCode</titleAttribute>
        <attributes>

	        <attribute name="rateClassCode">
			<label>Rate Class Code </label>
			<shortLabel>Rate Class Code </shortLabel>
			<maxLength>3</maxLength>
			<validationPattern><numeric /></validationPattern>
			<control>
				<text size="3" />
			</control>
			<summary>Rate Class Code </summary>
			<description>Rate Class Code </description>
			</attribute>

			<attribute name="rateTypeCode">
			<label>Rate Type Code </label>
			<shortLabel>Rate Type Code </shortLabel>
			<maxLength>3</maxLength>
			<validationPattern><numeric /></validationPattern>
			<control>
				<text size="3" />
			</control>
			<summary>Rate Type Code </summary>
			<description>Rate Type Code </description>
			</attribute>

			<attribute name="description">
			<label>Description </label>
			<shortLabel>Description </shortLabel>
			<maxLength>200</maxLength>
			<validationPattern></validationPattern>
			<control>
				<text size="200" />
			</control>
			<summary>Description </summary>
			<description>Description </description>
			</attribute>

		</attributes>
    </businessObject>

</dictionaryEntry>
