<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="KimRoleType" parent="KimRoleType-parentBean" />

  <bean id="KimRoleType-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kra.kim.bo.KimRoleType" />
    <property name="inquiryDefinition" >
      <ref bean="KimRoleType-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="KimRoleType-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="roleTypeCode" />
    <property name="attributes" >
      <list>
        <ref bean="KimRoleType-roleTypeCode" />
        <ref bean="KimRoleType-description" />
        <ref bean="KimRoleType-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="KimRoleType-roleTypeCode" parent="KimRoleType-roleTypeCode-parentBean" />

  <bean id="KimRoleType-roleTypeCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="roleTypeCode" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Role Type" />
    <property name="shortLabel" value="Role Type" />
    <property name="maxLength" value="3" />
    <property name="validationPattern" >
      <bean parent="AlphaNumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
    <property name="summary" value="Role Type" />
    <property name="description" value="Role Type" />
  </bean>
  <bean id="KimRoleType-description" parent="KimRoleType-description-parentBean" />

  <bean id="KimRoleType-description-parentBean" abstract="true" parent="KraAttributeReferenceDummy-description">
  </bean>
  <bean id="KimRoleType-versionNumber" parent="KimRoleType-versionNumber-parentBean" />

  <bean id="KimRoleType-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="KimRoleType-inquiryDefinition" parent="KimRoleType-inquiryDefinition-parentBean" />

  <bean id="KimRoleType-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Role Type" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Role Type" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="roleTypeCode"
                    p:forceInquiry="true" />
              <bean parent="FieldDefinition"
                    p:attributeName="description" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="KimRoleType-lookupDefinition" parent="KimRoleType-lookupDefinition-parentBean" />

  <bean id="KimRoleType-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Role Type Lookup" />
    <property name="menubar" value="&lt;a href=&quot;index.jsp&quot;&gt;Main&lt;/a&gt;" />
    <property name="instructions" value="Look up a Role Type" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="roleTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="description" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="roleTypeCode"
              p:forceInquiry="true" />
        <bean parent="FieldDefinition"
              p:attributeName="description" />
      </list>
    </property>
  </bean>
</beans>
