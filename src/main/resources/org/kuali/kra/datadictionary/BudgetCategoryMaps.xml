<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.kra.budget.bo.BudgetCategoryMap">

        <inquiry>
            <title>Budget Category Maps</title>

            <inquirySections><inquirySection title="Budget Category Maps"><inquiryFields>
            	<field attributeName="mappingName" forceInquiry="true"/>
				<field attributeName="targetCategoryCode" forceInquiry="true"/>
				<field attributeName="categoryType"/>
				<field attributeName="description"/>

            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Budget Category Maps Lookup</title>
			<menubar>
				<![CDATA[<a href="/kra-dev/">Main</a>]]>
			</menubar>
            <instructions>Look up a Budget Category Maps</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                	<sortAttribute attributeName="mappingName"/>
					<sortAttribute attributeName="targetCategoryCode"/>

                </sortAttributes>
            </defaultSort>

            <lookupFields>
            	<lookupField attributeName="mappingName" required="false"/>
				<lookupField attributeName="targetCategoryCode" required="false"/>
				<lookupField attributeName="categoryType" required="false"/>
				<lookupField attributeName="description" required="false"/>

            </lookupFields>

            <resultFields>
            	<field attributeName="mappingName" forceInquiry="true"/>
				<field attributeName="targetCategoryCode" forceInquiry="true"/>
				<field attributeName="categoryType" forceInquiry="false"/>
				<field attributeName="description" forceInquiry="false"/>

            </resultFields>
        </lookup>

        <titleAttribute>mappingName</titleAttribute>
		<!-- <titleAttribute>targetCategoryCode</titleAttribute> -->
        <attributes>

	        <attribute name="mappingName">
			<label>Mapping Name </label>
			<shortLabel>Mapping Name </shortLabel>
			<maxLength>100</maxLength>
			<validationPattern><anyCharacter /></validationPattern>
			<control>
				<text size="100" />
			</control>
			<summary>Mapping Name </summary>
			<description>Mapping Name </description>
			</attribute>

			<attribute name="targetCategoryCode">
			<label>Target Category Code </label>
			<shortLabel>Target Category Code </shortLabel>
			<maxLength>15</maxLength>
			<validationPattern><alphaNumeric /></validationPattern>
			<control>
				<text size="15" />
			</control>
			<summary>Target Category Code </summary>
			<description>Target Category Code </description>
			</attribute>

			<attribute name="categoryType">
			<label>Category Type </label>
			<shortLabel>Category Type </shortLabel>
			<maxLength>200</maxLength>
			<validationPattern><anyCharacter /></validationPattern>
			<control>
				<text size="200" />
			</control>
			<summary>Category Type </summary>
			<description>Category Type </description>
			</attribute>

			<attribute name="description">
			<label>Description </label>
			<shortLabel>Description </shortLabel>
			<maxLength>200</maxLength>
			<validationPattern><anyCharacter /></validationPattern>
			<control>
				<text size="200" />
			</control>
			<summary>Description </summary>
			<description>Description </description>
			</attribute>

		</attributes>
    </businessObject>

</dictionaryEntry>
