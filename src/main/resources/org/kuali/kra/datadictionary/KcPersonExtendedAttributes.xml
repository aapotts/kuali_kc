    <!-- NOTE: JHulslander, I commented this out while working on a maintenance issue, this doesn't seem to hurt having it commented, so leaving it commented out, may be able to remove at a later time
    <property name="relationships" >
      <list>
        <bean parent="RelationshipDefinition">
          <property name="objectAttributeName" value="homeUnit" />
          <property name="primitiveAttributes" >
            <list>
              <bean parent="PrimitiveAttributeDefinition"
                    p:sourceName="homeUnitIdentifier"
                    p:targetName="unitNumber" />
            </list>
          </property>
          <property name="supportAttributes" >
            <list>
              <bean parent="SupportAttributeDefinition"
                    p:identifier="true"
                    p:sourceName="homeUnit.unitName"
                    p:targetName="unitName" />
            </list>
          </property>
        </bean>
      </list>
    </property>-->
