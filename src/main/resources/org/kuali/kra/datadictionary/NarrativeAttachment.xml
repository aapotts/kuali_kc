<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.kra.proposaldevelopment.bo.NarrativeAttachment">

        <inquiry>
            <title>Proposal Attachment</title>

            <inquirySections><inquirySection title="Proposal Attachments"><inquiryFields>
            	<field attributeName="moduleNumber" forceInquiry="true"/>
				<field attributeName="proposalNumber" forceInquiry="true"/>
				<field attributeName="fileName"/>
				<field attributeName="narrativeData"/>

            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Proposal Attachement Lookup</title>
			<menubar>
				<![CDATA[<a href="/kra-dev/">Main</a>]]>
			</menubar>
            <instructions>Look up a Proposal attachment</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                	<sortAttribute attributeName="moduleNumber"/>
					<sortAttribute attributeName="proposalNumber"/>

                </sortAttributes>
            </defaultSort>

            <lookupFields>
            	<lookupField attributeName="moduleNumber" required="false"/>
				<lookupField attributeName="proposalNumber" required="false"/>
				<lookupField attributeName="fileName" required="false"/>
				<lookupField attributeName="narrativeData" required="false"/>

            </lookupFields>

            <resultFields>
            	<field attributeName="moduleNumber" forceInquiry="true"/>
				<field attributeName="proposalNumber" forceInquiry="true"/>
				<field attributeName="fileName" forceInquiry="false"/>
				<field attributeName="narrativeData" forceInquiry="false"/>

            </resultFields>
        </lookup>

        <titleAttribute>moduleNumber</titleAttribute>
        <attributes>

	        <attribute name="moduleNumber">
			<label>Module Number </label>
			<shortLabel>Module Number </shortLabel>
			<maxLength>4</maxLength>
			<validationPattern><numeric /></validationPattern>
			<control>
				<text size="4" />
			</control>
			<summary>Module Number </summary>
			<description>Module Number </description>
			</attribute>

			<attribute name="proposalNumber">
			<label>Proposal Number </label>
			<shortLabel>Proposal Number </shortLabel>
			<maxLength>12</maxLength>
			<validationPattern><alphaNumeric /></validationPattern>
			<control>
				<text size="12" />
			</control>
			<summary>Proposal Number </summary>
			<description>Proposal Number </description>
			</attribute>

			<attribute name="fileName">
			<label>File Name </label>
			<shortLabel>File Name </shortLabel>
			<maxLength>150</maxLength>
			<validationPattern><alphaNumeric /></validationPattern>
			<control>
				<text size="30" />
			</control>
			<summary>File Name </summary>
			<description>File Name </description>
			</attribute>

			<attribute name="narrativeData">
			<label>Attachment File</label>
			<shortLabel>Attachment </shortLabel>
			<maxLength>0</maxLength>
			<validationPattern><alphaNumeric /></validationPattern>
			<control>
				<text size="0" />
			</control>
			<summary>Narrative Attachment File</summary>
			<description>Narrative Attachment File </description>
			</attribute>

		</attributes>
    </businessObject>

</dictionaryEntry>
