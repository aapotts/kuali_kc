<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="ProtocolKeyPerson" parent="ProtocolKeyPerson-parentBean" />
	<bean id="ProtocolKeyPerson-parentBean" abstract="true" parent="BusinessObjectEntry">
		<property name="businessObjectClass" value="org.kuali.kra.irb.bo.ProtocolKeyPerson" />
		<property name="inquiryDefinition" >
			<ref bean="ProtocolKeyPerson-inquiryDefinition" />
		</property>
		<property name="lookupDefinition" >
			<ref bean="ProtocolKeyPerson-lookupDefinition" />
		</property>
		<property name="titleAttribute" value="protocolKeyPersonsId" />
		<property name="attributes" >
			<list>
				<ref bean="ProtocolKeyPerson-protocolKeyPersonsId" />
				<ref bean="ProtocolKeyPerson-protocolId" />
				<ref bean="ProtocolKeyPerson-protocolNumber" />
				<ref bean="ProtocolKeyPerson-sequenceNumber" />
				<ref bean="ProtocolKeyPerson-personId" />
				<ref bean="ProtocolKeyPerson-personName" />
				<ref bean="ProtocolKeyPerson-personRole" />
				<ref bean="ProtocolKeyPerson-nonEmployeeFlag" />
				<ref bean="ProtocolKeyPerson-affiliationTypeCode" />
				<ref bean="ProtocolKeyPerson-versionNumber" />
			</list>
		</property>
	</bean>

	<!-- Attribute Definitions -->
	
	<bean id="ProtocolKeyPerson-protocolKeyPersonsId" parent="ProtocolKeyPerson-protocolKeyPersonsId-parentBean" />
	<bean id="ProtocolKeyPerson-protocolKeyPersonsId-parentBean" abstract="true" parent="AttributeDefinition" >
		<property name="name" value="protocolKeyPersonsId" />
		<property name="forceUppercase" value="false" />
		<property name="label" value="Protocol Key Persons Id" />
		<property name="shortLabel" value="Protocol Key Persons Id" />
		<property name="maxLength" value="22" />
		<property name="validationPattern" >
			<bean parent="NumericValidationPattern" />
		</property>
		<property name="control" >
			<bean parent="TextControlDefinition"
				p:size="10" />
		</property>
		<property name="summary" value="Protocol Key Persons Id" />
		<property name="description" value="Protocol Key Persons Id" />
	</bean>

	<bean id="ProtocolKeyPerson-protocolId" parent="ProtocolKeyPerson-protocolId-parentBean" />
	<bean id="ProtocolKeyPerson-protocolId-parentBean" abstract="true" parent="AttributeDefinition" >
		<property name="name" value="protocolId" />
		<property name="forceUppercase" value="false" />
		<property name="label" value="Protocol Id" />
		<property name="shortLabel" value="Protocol Id" />
		<property name="maxLength" value="22" />
		<property name="validationPattern" >
			<bean parent="NumericValidationPattern" />
		</property>
		<property name="control" >
			<bean parent="TextControlDefinition"
				p:size="10" />
		</property>
		<property name="summary" value="Protocol Id" />
		<property name="description" value="Protocol Id" />
	</bean>

	<bean id="ProtocolKeyPerson-protocolNumber" parent="ProtocolKeyPerson-protocolNumber-parentBean" />
	<bean id="ProtocolKeyPerson-protocolNumber-parentBean" abstract="true" parent="AttributeDefinition" >
		<property name="name" value="protocolNumber" />
		<property name="forceUppercase" value="false" />
		<property name="label" value="Protocol Number" />
		<property name="shortLabel" value="Protocol Number" />
		<property name="maxLength" value="20" />
		<property name="validationPattern" >
			<bean parent="AlphaNumericValidationPattern" />
		</property>
		<property name="control" >
			<bean parent="TextControlDefinition"
				p:size="10" />
		</property>
		<property name="summary" value="Protocol Number" />
		<property name="description" value="Protocol Number" />
	</bean>

	<bean id="ProtocolKeyPerson-sequenceNumber" parent="ProtocolKeyPerson-sequenceNumber-parentBean" />
	<bean id="ProtocolKeyPerson-sequenceNumber-parentBean" abstract="true" parent="AttributeDefinition" >
		<property name="name" value="sequenceNumber" />
		<property name="forceUppercase" value="false" />
		<property name="label" value="Sequence Number" />
		<property name="shortLabel" value="Sequence Number" />
		<property name="maxLength" value="22" />
		<property name="validationPattern" >
			<bean parent="NumericValidationPattern" />
		</property>
		<property name="control" >
			<bean parent="TextControlDefinition"
				p:size="10" />
		</property>
		<property name="summary" value="Sequence Number" />
		<property name="description" value="Sequence Number" />
	</bean>

	<bean id="ProtocolKeyPerson-personId" parent="ProtocolKeyPerson-personId-parentBean" />
	<bean id="ProtocolKeyPerson-personId-parentBean" abstract="true" parent="AttributeDefinition" >
		<property name="name" value="personId" />
		<property name="forceUppercase" value="false" />
		<property name="label" value="Person Id" />
		<property name="shortLabel" value="Person Id" />
		<property name="maxLength" value="9" />
		<property name="validationPattern" >
			<bean parent="AlphaNumericValidationPattern" />
		</property>
		<property name="control" >
			<bean parent="TextControlDefinition"
				p:size="10" />
		</property>
		<property name="summary" value="Person Id" />
		<property name="description" value="Person Id" />
	</bean>

	<bean id="ProtocolKeyPerson-personName" parent="ProtocolKeyPerson-personName-parentBean" />
	<bean id="ProtocolKeyPerson-personName-parentBean" abstract="true" parent="AttributeDefinition" >
		<property name="name" value="personName" />
		<property name="forceUppercase" value="false" />
		<property name="label" value="Person Name" />
		<property name="shortLabel" value="Person Name" />
		<property name="maxLength" value="90" />
		<property name="validationPattern" >
			<bean parent="AlphaNumericValidationPattern" />
		</property>
		<property name="control" >
			<bean parent="TextControlDefinition"
				p:size="10" />
		</property>
		<property name="summary" value="Person Name" />
		<property name="description" value="Person Name" />
	</bean>

	<bean id="ProtocolKeyPerson-personRole" parent="ProtocolKeyPerson-personRole-parentBean" />
	<bean id="ProtocolKeyPerson-personRole-parentBean" abstract="true" parent="AttributeDefinition" >
		<property name="name" value="personRole" />
		<property name="forceUppercase" value="false" />
		<property name="label" value="Person Role" />
		<property name="shortLabel" value="Person Role" />
		<property name="maxLength" value="60" />
		<property name="validationPattern" >
			<bean parent="AlphaNumericValidationPattern" />
		</property>
		<property name="control" >
			<bean parent="TextControlDefinition"
				p:size="10" />
		</property>
		<property name="summary" value="Person Role" />
		<property name="description" value="Person Role" />
	</bean>

	<bean id="ProtocolKeyPerson-nonEmployeeFlag" parent="ProtocolKeyPerson-nonEmployeeFlag-parentBean" />
	<bean id="ProtocolKeyPerson-nonEmployeeFlag-parentBean" abstract="true" parent="AttributeReferenceDummy-genericBoolean" >
		<property name="name" value="nonEmployeeFlag" />
		<property name="label" value="Non Employee Flag" />
		<property name="shortLabel" value="Non Employee Flag" />
		<property name="summary" value="Non Employee Flag" />
		<property name="description" value="Non Employee Flag" />
	</bean>

	<bean id="ProtocolKeyPerson-affiliationTypeCode" parent="ProtocolKeyPerson-affiliationTypeCode-parentBean" />
	<bean id="ProtocolKeyPerson-affiliationTypeCode-parentBean" abstract="true" parent="AttributeDefinition" >
		<property name="name" value="affiliationTypeCode" />
		<property name="forceUppercase" value="false" />
		<property name="label" value="Affiliation Type Code" />
		<property name="shortLabel" value="Affiliation Type Code" />
		<property name="maxLength" value="22" />
		<property name="validationPattern" >
			<bean parent="NumericValidationPattern" />
		</property>
		<property name="control" >
			<bean parent="TextControlDefinition"
				p:size="10" />
		</property>
		<property name="summary" value="Affiliation Type Code" />
		<property name="description" value="Affiliation Type Code" />
	</bean>

	<bean id="ProtocolKeyPerson-versionNumber" parent="ProtocolKeyPerson-versionNumber-parentBean" />
	<bean id="ProtocolKeyPerson-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
	</bean>

	<!-- Business Object Inquiry Definition -->
	<bean id="ProtocolKeyPerson-inquiryDefinition" parent="ProtocolKeyPerson-inquiryDefinition-parentBean" />
	<bean id="ProtocolKeyPerson-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
		<property name="title" value="Protocol Key Persons Id" />
		<property name="inquirySections" >
			<list>
				<bean parent="InquirySectionDefinition">
					<property name="title" value="Protocol Key Persons Id" />
					<property name="numberOfColumns" value="1" />
					<property name="inquiryFields" >
						<list>
							<bean parent="FieldDefinition"
								p:attributeName="protocolKeyPersonsId" 
								p:forceInquiry="true" />
							<bean parent="FieldDefinition"
								p:attributeName="protocolId" />
							<bean parent="FieldDefinition"
								p:attributeName="protocolNumber" />
							<bean parent="FieldDefinition"
								p:attributeName="sequenceNumber" />
							<bean parent="FieldDefinition"
								p:attributeName="personId" />
							<bean parent="FieldDefinition"
								p:attributeName="personName" />
							<bean parent="FieldDefinition"
								p:attributeName="personRole" />
							<bean parent="FieldDefinition"
								p:attributeName="nonEmployeeFlag" />
							<bean parent="FieldDefinition"
								p:attributeName="affiliationTypeCode" />
						</list>
					</property>
				</bean>
			</list>
		</property>
	</bean>

	<!-- Business Object Lookup Definition -->
	<bean id="ProtocolKeyPerson-lookupDefinition" parent="ProtocolKeyPerson-lookupDefinition-parentBean" />
	<bean id="ProtocolKeyPerson-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
		<property name="title" value="Protocol Key Persons Lookup" />
		<property name="menubar" value="&lt;a href=&quot;index.jsp&quot;&gt;Main&lt;/a&gt;" />
		<property name="instructions" value="Look up a Protocol Key Persons" />
		<property name="defaultSort" >
			<bean parent="SortDefinition" >
		</bean>
		</property>
		<property name="lookupFields" >
			<list>
				<bean parent="FieldDefinition"
					p:attributeName="protocolKeyPersonsId" 
					p:forceInquiry="true" />
				<bean parent="FieldDefinition"
					p:attributeName="protocolId" />
				<bean parent="FieldDefinition"
					p:attributeName="protocolNumber" />
				<bean parent="FieldDefinition"
					p:attributeName="sequenceNumber" />
				<bean parent="FieldDefinition"
					p:attributeName="personId" />
				<bean parent="FieldDefinition"
					p:attributeName="personName" />
				<bean parent="FieldDefinition"
					p:attributeName="personRole" />
				<bean parent="FieldDefinition"
					p:attributeName="nonEmployeeFlag" />
				<bean parent="FieldDefinition"
					p:attributeName="affiliationTypeCode" />
			</list>
		</property>
		<property name="resultFields" >
			<list>
				<bean parent="FieldDefinition"
					p:attributeName="protocolKeyPersonsId" 
					p:forceInquiry="true" />
				<bean parent="FieldDefinition"
					p:attributeName="protocolId" />
				<bean parent="FieldDefinition"
					p:attributeName="protocolNumber" />
				<bean parent="FieldDefinition"
					p:attributeName="sequenceNumber" />
				<bean parent="FieldDefinition"
					p:attributeName="personId" />
				<bean parent="FieldDefinition"
					p:attributeName="personName" />
				<bean parent="FieldDefinition"
					p:attributeName="personRole" />
				<bean parent="FieldDefinition"
					p:attributeName="nonEmployeeFlag" />
				<bean parent="FieldDefinition"
					p:attributeName="affiliationTypeCode" />
			</list>
		</property>
	</bean>
</beans>
