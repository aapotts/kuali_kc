<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="BudgetDocument" parent="BudgetDocument-parentBean"/>

  <bean id="BudgetDocument-parentBean" abstract="true" parent="TransactionalDocumentEntry">
    
    
    
    
    <property name="usePessimisticLocking" value="true"/>
    <property name="useWorkflowPessimisticLocking" value="false"/>
    
    <property name="documentTypeName" value="BudgetDocument"/>
    <property name="documentClass" value="org.kuali.kra.budget.document.BudgetDocument"/>
    <property name="businessRulesClass" value="org.kuali.kra.budget.rules.BudgetDocumentRule"/>
    <property name="documentAuthorizerClass" value="org.kuali.kra.budget.document.authorization.BudgetDocumentAuthorizer"/>
    
    <property name="allowsCopy" value="true"/>
    <property name="headerNavigationList">
      <list>
        <bean parent="HeaderNavigation">
          <property name="helpDefinition">
            <bean parent="HelpDefinition" p:parameterNamespace="KRA-B" p:parameterName="budgetVersionsHelp" p:parameterDetailType="D"/>
          </property>
          <property name="headerTabDisplayName" value="Budget Versions"/>
          <property name="headerTabNavigateTo" value="versions"/>
        </bean>
        <bean parent="HeaderNavigation">
          <property name="helpDefinition">
            <bean parent="HelpDefinition" p:parameterNamespace="KRA-B" p:parameterName="budgetSummaryHelp" p:parameterDetailType="D"/>
          </property>
          <property name="headerTabDisplayName" value="Summary"/>
          <property name="headerTabNavigateTo" value="summary"/>
        </bean>
        <bean parent="HeaderNavigation">
          <property name="helpDefinition">
            <bean parent="HelpDefinition" p:parameterNamespace="KRA-B" p:parameterName="budgetRatesHelp" p:parameterDetailType="D"/>
          </property>
          <property name="headerTabDisplayName" value="Rates"/>
          <property name="headerTabNavigateTo" value="rates"/>
        </bean>
        <bean parent="HeaderNavigation">
          <property name="helpDefinition">
            <bean parent="HelpDefinition" p:parameterNamespace="KRA-B" p:parameterName="budgetPersonnelHelp" p:parameterDetailType="D"/>
          </property>
          <property name="headerTabDisplayName" value="Project Personnel"/>
          <property name="headerTabNavigateTo" value="personnel"/>
        </bean>
        <bean parent="HeaderNavigation">
          <property name="helpDefinition">
            <bean parent="HelpDefinition" p:parameterNamespace="KRA-B" p:parameterName="budgetExpensesHelp" p:parameterDetailType="D"/>
          </property>
          <property name="headerTabDisplayName" value="Expenses"/>
          <property name="headerTabNavigateTo" value="expenses"/>
        </bean>
        <bean parent="HeaderNavigation">
          <property name="helpDefinition">
            <bean parent="HelpDefinition" p:parameterNamespace="KRA-B" p:parameterName="budgetTotalsHelp" p:parameterDetailType="D"/>
          </property>
          <property name="headerTabDisplayName" value="Totals"/>
          <property name="headerTabNavigateTo" value="totals"/>
        </bean>
        <bean parent="HeaderNavigation">
          <property name="helpDefinition">
            <bean parent="HelpDefinition" p:parameterNamespace="KRA-B" p:parameterName="budgetDistributionAndIncomeHelp" p:parameterDetailType="D"/>
          </property>
          <property name="headerTabDisplayName" value="Distribution &amp; Income"/>
          <property name="headerTabNavigateTo" value="distributionAndIncome"/>
        </bean>
        <bean parent="HeaderNavigation">
          <property name="helpDefinition">
            <bean parent="HelpDefinition" p:parameterNamespace="KRA-B" p:parameterName="budgetModularBudgetHelp" p:parameterDetailType="D"/>
          </property>
          <property name="headerTabDisplayName" value="Modular Budget"/>
          <property name="headerTabNavigateTo" value="modularBudget"/>
        </bean>
        <bean parent="HeaderNavigation">
          <property name="helpDefinition">
            <bean parent="HelpDefinition" p:parameterNamespace="KRA-B" p:parameterName="budgetActionsHelp" p:parameterDetailType="D"/>
          </property>
          <property name="headerTabDisplayName" value="Budget Actions"/>
          <property name="headerTabNavigateTo" value="budgetActions"/>
        </bean>
      </list>
    </property>
    
    <property name="attributes">
      <list>
        <ref bean="BudgetDocument-proposalNumber"/>
        <ref bean="BudgetDocument-costSharingAmount"/>
        <ref bean="BudgetDocument-endDate"/>
        <ref bean="BudgetDocument-finalVersionFlag"/>
        <ref bean="BudgetDocument-modularBudgetFlag"/>
        <ref bean="BudgetDocument-ohRateClassCode"/>
        <ref bean="BudgetDocument-ohRateTypeCode"/>
        <ref bean="BudgetDocument-residualFunds"/>
        <ref bean="BudgetDocument-startDate"/>
        <ref bean="BudgetDocument-totalCost"/>
        <ref bean="BudgetDocument-totalCostLimit"/>
        <ref bean="BudgetDocument-totalDirectCost"/>
        <ref bean="BudgetDocument-totalIndirectCost"/>
        <ref bean="BudgetDocument-underrecoveryAmount"/>
        <ref bean="BudgetDocument-urRateClassCode"/>
        <ref bean="BudgetDocument-comments"/>
        <ref bean="BudgetDocument-budgetJustification"/>
        <ref bean="BudgetDocument-budgetVersionNumber"/>
        <ref bean="BudgetDocument-onOffCampusFlag"/>
        <ref bean="BudgetDocument-versionNumber"/>
      </list>
    </property>
    
    <property name="workflowAttributes">
    	<ref bean="BudgetDocument-workflowAttributes"/>
    </property>
    
  </bean>

<!-- Attribute Definitions -->


  <bean id="BudgetDocument-proposalNumber" parent="BudgetDocument-proposalNumber-parentBean"/>

  <bean id="BudgetDocument-proposalNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="proposalNumber"/>
    <property name="forceUppercase" value="false"/>
    <property name="label" value="Proposal Number"/>
    <property name="shortLabel" value="Proposal Number"/>
    <property name="maxLength" value="12"/>
    <property name="validationPattern">
      <bean parent="AlphaNumericValidationPattern"/>
    </property>
    <property name="required" value="false"/>
    <property name="control">
      <bean parent="HiddenControlDefinition"/>
    </property>
    <property name="summary" value="Proposal Number"/>
    <property name="description" value="Proposal Number"/>
  </bean>
  <bean id="BudgetDocument-costSharingAmount" parent="BudgetDocument-costSharingAmount-parentBean"/>

  <bean id="BudgetDocument-costSharingAmount-parentBean" abstract="true" parent="KraAttributeReferenceDummy-genericAmount">
    <property name="name" value="costSharingAmount"/>
    <property name="label" value="Cost Sharing Amount"/>
    <property name="shortLabel" value="Cost Sharing Amount"/>
    <property name="summary" value="Cost Sharing Amount"/>
    <property name="description" value="Cost Sharing Amount"/>
  </bean>
  <bean id="BudgetDocument-endDate" parent="BudgetDocument-endDate-parentBean"/>

  <bean id="BudgetDocument-endDate-parentBean" abstract="true" parent="AttributeReferenceDummy-genericDate">
    <property name="name" value="endDate"/>
    <property name="label" value="Project End Date"/>
    <property name="shortLabel" value="Project End Dt"/>
    <property name="summary" value="Project End Date"/>
    <property name="description" value="Project End Date"/>
  </bean>
  <bean id="BudgetDocument-finalVersionFlag" parent="BudgetDocument-finalVersionFlag-parentBean"/>

  <bean id="BudgetDocument-finalVersionFlag-parentBean" abstract="true" parent="AttributeReferenceDummy-genericBoolean">
    <property name="name" value="finalVersionFlag"/>
    <property name="label" value="Final?"/>
    <property name="shortLabel" value="Final"/>
    <property name="summary" value="Final Version Flag"/>
    <property name="description" value="Final Version Flag"/>
  </bean>
  <bean id="BudgetDocument-modularBudgetFlag" parent="BudgetDocument-modularBudgetFlag-parentBean"/>

  <bean id="BudgetDocument-modularBudgetFlag-parentBean" abstract="true" parent="AttributeReferenceDummy-genericBoolean">
    <property name="name" value="modularBudgetFlag"/>
    <property name="label" value="Modular Budget?"/>
    <property name="shortLabel" value="Modular Budget?"/>
    <property name="summary" value="Modular Budget Flag"/>
    <property name="description" value="Modular Budget Flag"/>
  </bean>
  <bean id="BudgetDocument-ohRateClassCode" parent="BudgetDocument-ohRateClassCode-parentBean"/>

  <bean id="BudgetDocument-ohRateClassCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="ohRateClassCode"/>
    <property name="forceUppercase" value="false"/>
    <property name="label" value="F&amp;A Rate Type"/>
    <property name="shortLabel" value="F&amp;A Rate Type"/>
    <property name="maxLength" value="3"/>
    <property name="validationPattern">
      <bean parent="NumericValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="SelectControlDefinition" p:valuesFinderClass="org.kuali.kra.budget.lookup.keyvalue.BudgetRateTypeValuesFinder" p:includeKeyInLabel="false"/>
    </property>
    <property name="summary" value="F&amp;A Rate Type"/>
    <property name="description" value="F&amp;A Rate Type"/>
  </bean>
  <bean id="BudgetDocument-ohRateTypeCode" parent="BudgetDocument-ohRateTypeCode-parentBean"/>

  <bean id="BudgetDocument-ohRateTypeCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="ohRateTypeCode"/>
    <property name="forceUppercase" value="false"/>
    <property name="label" value="F&amp;A Rate Type"/>
    <property name="shortLabel" value="F&amp;A Rate Type"/>
    <property name="maxLength" value="3"/>
    <property name="validationPattern">
      <bean parent="NumericValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="3"/>
    </property>
    <property name="summary" value="F&amp;A Rate Type"/>
    <property name="description" value="F&amp;A Rate Type"/>
  </bean>
  <bean id="BudgetDocument-residualFunds" parent="BudgetDocument-residualFunds-parentBean"/>

  <bean id="BudgetDocument-residualFunds-parentBean" abstract="true" parent="KraAttributeReferenceDummy-genericAmount">
    <property name="name" value="residualFunds"/>
    <property name="label" value="Residual Funds"/>
    <property name="shortLabel" value="Residual Funds"/>
    <property name="summary" value="Residual Funds"/>
    <property name="description" value="Residual Funds"/>
  </bean>
  <bean id="BudgetDocument-startDate" parent="BudgetDocument-startDate-parentBean"/>

  <bean id="BudgetDocument-startDate-parentBean" abstract="true" parent="AttributeReferenceDummy-genericDate">
    <property name="name" value="startDate"/>
    <property name="label" value="Project Start Date"/>
    <property name="shortLabel" value="Project Start Date"/>
    <property name="summary" value="Project Start Date"/>
    <property name="description" value="Project Start Date"/>
  </bean>
  <bean id="BudgetDocument-totalCost" parent="BudgetDocument-totalCost-parentBean"/>

  <bean id="BudgetDocument-totalCost-parentBean" abstract="true" parent="KraAttributeReferenceDummy-genericAmount">
    <property name="name" value="totalCost"/>
    <property name="label" value="Total Cost"/>
    <property name="shortLabel" value="Total Cost"/>
    <property name="summary" value="Total Cost"/>
    <property name="description" value="Total Cost"/>
  </bean>
  <bean id="BudgetDocument-totalCostLimit" parent="BudgetDocument-totalCostLimit-parentBean"/>

  <bean id="BudgetDocument-totalCostLimit-parentBean" abstract="true" parent="KraAttributeReferenceDummy-genericAmount">
    <property name="name" value="totalCostLimit"/>
    <property name="label" value="Total Cost Limit"/>
    <property name="shortLabel" value="Total Cost Limit"/>
    <property name="summary" value="Total Cost Limit"/>
    <property name="description" value="Total Cost Limit"/>
  </bean>
  <bean id="BudgetDocument-totalDirectCost" parent="BudgetDocument-totalDirectCost-parentBean"/>

  <bean id="BudgetDocument-totalDirectCost-parentBean" abstract="true" parent="KraAttributeReferenceDummy-genericAmount">
    <property name="name" value="totalDirectCost"/>
    <property name="label" value="Total Direct Cost"/>
    <property name="shortLabel" value="Direct Cost"/>
    <property name="summary" value="Total Direct Cost"/>
    <property name="description" value="Total Direct Cost"/>
  </bean>
  <bean id="BudgetDocument-totalIndirectCost" parent="BudgetDocument-totalIndirectCost-parentBean"/>

  <bean id="BudgetDocument-totalIndirectCost-parentBean" abstract="true" parent="KraAttributeReferenceDummy-genericAmount">
    <property name="name" value="totalIndirectCost"/>
    <property name="label" value="Total Indirect Cost"/>
    <property name="shortLabel" value="F &amp; A"/>
    <property name="summary" value="Total Indirect Cost"/>
    <property name="description" value="Total Indirect Cost"/>
  </bean>
  <bean id="BudgetDocument-underrecoveryAmount" parent="BudgetDocument-underrecoveryAmount-parentBean"/>

  <bean id="BudgetDocument-underrecoveryAmount-parentBean" abstract="true" parent="KraAttributeReferenceDummy-genericAmount">
    <property name="name" value="underrecoveryAmount"/>
    <property name="label" value="Underrecovery Amount"/>
    <property name="shortLabel" value="Underrecovery Amount"/>
    <property name="summary" value="Underrecovery Amount"/>
    <property name="description" value="Underrecovery Amount"/>
  </bean>
  <bean id="BudgetDocument-urRateClassCode" parent="BudgetDocument-urRateClassCode-parentBean"/>

  <bean id="BudgetDocument-urRateClassCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="urRateClassCode"/>
    <property name="forceUppercase" value="false"/>
    <property name="label" value="Unrecovered F &amp; A Rate Type"/>
    <property name="shortLabel" value="Unrecovered F &amp; A Rate Type"/>
    <property name="maxLength" value="3"/>
    <property name="validationPattern">
      <bean parent="NumericValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="SelectControlDefinition" p:valuesFinderClass="org.kuali.kra.budget.lookup.keyvalue.BudgetRateTypeValuesFinder" p:includeKeyInLabel="false"/>
    </property>
    <property name="summary" value="Ur Rate Class Code"/>
    <property name="description" value="Ur Rate Class Code"/>
  </bean>
  <bean id="BudgetDocument-comments" parent="BudgetDocument-comments-parentBean"/>

  <bean id="BudgetDocument-comments-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="comments"/>
    <property name="forceUppercase" value="false"/>
    <property name="label" value="Comments"/>
    <property name="shortLabel" value="Comments"/>
    <property name="maxLength" value="2000"/>
    <property name="validationPattern">
      <bean parent="AnyCharacterValidationPattern" p:allowWhitespace="true"/>
    </property>
    <property name="required" value="false"/>
    <property name="control">
      <bean parent="TextareaControlDefinition" p:rows="3" p:cols="30"/>
    </property>
    <property name="summary" value="The Comments for this budget."/>
    <property name="description" value="The Comments for this budget."/>
  </bean>
  <bean id="BudgetDocument-budgetJustification" parent="BudgetDocument-budgetJustification-parentBean"/>

  <bean id="BudgetDocument-budgetJustification-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="budgetJustification"/>
    <property name="forceUppercase" value="false"/>
    <property name="label" value="Budget Justification"/>
    <property name="shortLabel" value="Budget Justification"/>
    <property name="maxLength" value="4000"/>
    <property name="validationPattern">
      <bean parent="AnyCharacterValidationPattern" p:allowWhitespace="true"/>
    </property>
    <property name="required" value="false"/>
    <property name="control">
      <bean parent="TextareaControlDefinition" p:rows="8" p:cols="60"/>
    </property>
    <property name="summary" value="Budget Justification"/>
    <property name="description" value="Budget Justification"/>
  </bean>
  <bean id="BudgetDocument-budgetVersionNumber" parent="BudgetDocument-budgetVersionNumber-parentBean"/>

  <bean id="BudgetDocument-budgetVersionNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="budgetVersionNumber"/>
    <property name="forceUppercase" value="false"/>
    <property name="label" value="Budget Version Number"/>
    <property name="shortLabel" value="Version #"/>
    <property name="maxLength" value="3"/>
    <property name="validationPattern">
      <bean parent="NumericValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="3"/>
    </property>
    <property name="summary" value="Budget Version Number"/>
    <property name="description" value="Budget Version Number for header display"/>
  </bean>
  <bean id="BudgetDocument-onOffCampusFlag" parent="BudgetDocument-onOffCampusFlag-parentBean"/>

  <bean id="BudgetDocument-onOffCampusFlag-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="onOffCampusFlag"/>
    <property name="forceUppercase" value="false"/>
    <property name="label" value="On/Off Campus"/>
    <property name="shortLabel" value="On/Off Campus"/>
    <property name="maxLength" value="3"/>
    <property name="validationPattern">
      <bean parent="AlphaNumericValidationPattern"/>
    </property>
    <property name="control">
      <bean parent="SelectControlDefinition" p:valuesFinderClass="org.kuali.kra.budget.lookup.keyvalue.BudgetOnOffCampusValuesFinder" p:includeKeyInLabel="false"/>
    </property>
    <property name="summary" value="On Off Campus flag"/>
    <property name="description" value="On Off Campus flag allowing the user to set all expense line items to be either 'all on' or 'all off-campus'; overriding the object code level defaults."/>
  </bean>
  <bean id="BudgetDocument-versionNumber" parent="BudgetDocument-versionNumber-parentBean"/>

  <bean id="BudgetDocument-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
  
  </bean>
  
  <bean id="BudgetDocument-workflowAttributes" parent="BudgetDocument-workflowAttributes-parentBean"/>

  <bean id="BudgetDocument-workflowAttributes-parentBean" abstract="true" parent="WorkflowAttributes">
