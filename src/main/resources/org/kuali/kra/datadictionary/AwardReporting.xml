<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

    <bean id="AwardReporting" parent="AwardReporting-parentBean" />
    <bean id="AwardReporting-parentBean" abstract="true" parent="BusinessObjectEntry">
        <property name="businessObjectClass" value="org.kuali.kra.award.paymentreports.awardreports.reporting.AwardReporting" />
		<property name="objectLabel" value="Award Reporting" />
        <property name="inquiryDefinition" >
            <ref bean="AwardReporting-inquiryDefinition" />
        </property>
        <property name="lookupDefinition" >
            <ref bean="AwardReporting-lookupDefinition" />
        </property>
        <property name="titleAttribute" value="awardReportingId" />
        <property name="attributes" >
            <list>
                <ref bean="AwardReporting-awardReportingId" />
                <ref bean="AwardReporting-awardReportTermId" />
                <ref bean="AwardReporting-awardNumber" />
				<ref bean="AwardReporting-sequenceNumber" />                
                <ref bean="AwardReporting-dueDate" />
                <ref bean="AwardReporting-overdueCounter" />
                <ref bean="AwardReporting-reportStatusCode" />
                <ref bean="AwardReporting-activityDate" />
                <ref bean="AwardReporting-comments" />
                <ref bean="AwardReporting-personId" />
                <ref bean="AwardReporting-versionNumber" />
            </list>
        </property>
    </bean>

    <!-- Attribute Definitions -->
    
    <bean id="AwardReporting-awardReportingId" parent="AwardReporting-awardReportingId-parentBean" />
    <bean id="AwardReporting-awardReportingId-parentBean" abstract="true" parent="AttributeDefinition" >
        <property name="name" value="awardReportingId" />
        <property name="forceUppercase" value="false" />
        <property name="label" value="Award Reporting Id" />
        <property name="shortLabel" value="Award Reporting Id" />
        <property name="maxLength" value="22" />
        <property name="validationPattern" >
            <bean parent="NumericValidationPattern" />
        </property>
        <property name="control" >
            <bean parent="TextControlDefinition"
                p:size="10" />
        </property>
        <property name="summary" value="Award Reporting Id" />
        <property name="description" value="Award Reporting Id" />
    </bean>
    
    <bean id="AwardReporting-awardReportTermId" parent="AwardReporting-awardReportTermId-parentBean" />
	<bean id="AwardReporting-awardReportTermId-parentBean" abstract="true" parent="AttributeDefinition" >
		<property name="name" value="awardReportTermId" />
		<property name="forceUppercase" value="false" />
		<property name="label" value="Award Report Terms Id" />
		<property name="shortLabel" value="Award Report Terms Id" />
		<property name="maxLength" value="22" />
		<property name="validationPattern" >
			<bean parent="NumericValidationPattern" />
		</property>
		<property name="control" >
			<bean parent="TextControlDefinition"
				p:size="10" />
		</property>
		<property name="summary" value="Award Report Terms Id" />
		<property name="description" value="Award Report Terms Id" />
	</bean>

    <bean id="AwardReporting-awardNumber" parent="AwardReporting-awardNumber-parentBean" />
    <bean id="AwardReporting-awardNumber-parentBean" abstract="true" parent="Award-awardNumber" >
    </bean>
    
    <bean id="AwardReporting-sequenceNumber" parent="AwardReporting-sequenceNumber-parentBean" />
	<bean id="AwardReporting-sequenceNumber-parentBean" abstract="true" parent="AttributeDefinition" >
		<property name="name" value="sequenceNumber" />
		<property name="forceUppercase" value="false" />
		<property name="label" value="Sequence Number" />
		<property name="shortLabel" value="Sequence Number" />
		<property name="maxLength" value="22" />
		<property name="validationPattern" >
			<bean parent="NumericValidationPattern" />
		</property>
		<property name="control" >
			<bean parent="TextControlDefinition"
				p:size="10" />
		</property>
		<property name="summary" value="Sequence Number" />
		<property name="description" value="Sequence Number" />
	</bean>

    <bean id="AwardReporting-dueDate" parent="AwardReporting-dueDate-parentBean" />
    <bean id="AwardReporting-dueDate-parentBean" abstract="true" parent="KraAttributeReferenceDummy-genericDate" >
        <property name="name" value="dueDate" />
        <property name="label" value="Due Date" />
        <property name="shortLabel" value="Due Date" />
        <property name="summary" value="Due Date" />
        <property name="description" value="Due Date" />
    </bean>

    <bean id="AwardReporting-overdueCounter" parent="AwardReporting-overdueCounter-parentBean" />
    <bean id="AwardReporting-overdueCounter-parentBean" abstract="true" parent="AttributeDefinition" >
        <property name="name" value="overdueCounter" />
        <property name="forceUppercase" value="false" />
        <property name="label" value="Overdue" />
        <property name="shortLabel" value="Overdue" />
        <property name="maxLength" value="22" />
        <property name="validationPattern" >
            <bean parent="NumericValidationPattern" />
        </property>
        <property name="control" >
            <bean parent="TextControlDefinition"
                p:size="10" />
        </property>
        <property name="summary" value="Overdue" />
        <property name="description" value="Overdue" />
    </bean>

    <bean id="AwardReporting-reportStatusCode" parent="AwardReporting-reportStatusCode-parentBean" />
    <bean id="AwardReporting-reportStatusCode-parentBean" abstract="true" parent="AttributeDefinition" >
        <property name="name" value="reportStatusCode" />
        <property name="forceUppercase" value="false" />
        <property name="label" value="Status" />
        <property name="shortLabel" value="Status" />
        <property name="maxLength" value="22" />
        <property name="validationPattern" >
            <bean parent="NumericValidationPattern" />
        </property>
        <property name="control" >
            <bean parent="TextControlDefinition"
                p:size="10" />
        </property>
        <property name="summary" value="Status" />
        <property name="description" value="Status" />
    </bean>

    <bean id="AwardReporting-activityDate" parent="AwardReporting-activityDate-parentBean" />
    <bean id="AwardReporting-activityDate-parentBean" abstract="true" parent="KraAttributeReferenceDummy-genericDate" >
        <property name="name" value="activityDate" />
        <property name="label" value="Activity Date" />
        <property name="shortLabel" value="Activity Date" />
        <property name="summary" value="Activity Date" />
        <property name="description" value="Activity Date" />
    </bean>

    <bean id="AwardReporting-comments" parent="AwardReporting-comments-parentBean" />
    <bean id="AwardReporting-comments-parentBean" abstract="true" parent="AttributeDefinition" >
        <property name="name" value="comments" />
        <property name="forceUppercase" value="false" />
        <property name="label" value="Comments" />
        <property name="shortLabel" value="Comments" />
        <property name="maxLength" value="1000" />
        <property name="validationPattern" >
            <bean parent="AlphaNumericValidationPattern" />
        </property>
        <property name="control" >
            <bean parent="TextControlDefinition"
                p:size="10" />
        </property>
        <property name="summary" value="Comments" />
        <property name="description" value="Comments" />
    </bean>

    <bean id="AwardReporting-personId" parent="AwardReporting-personId-parentBean" />
    <bean id="AwardReporting-personId-parentBean" abstract="true" parent="AttributeDefinition" >
        <property name="name" value="personId" />
        <property name="forceUppercase" value="false" />
        <property name="label" value="Person Id" />
        <property name="shortLabel" value="Person Id" />
        <property name="maxLength" value="40" />
        <property name="validationPattern" >
            <bean parent="AlphaNumericValidationPattern" />
        </property>
        <property name="control" >
            <bean parent="TextControlDefinition"
                p:size="10" />
        </property>
        <property name="summary" value="Person Id" />
        <property name="description" value="Person Id" />
    </bean>

    <bean id="AwardReporting-versionNumber" parent="AwardReporting-versionNumber-parentBean" />
    <bean id="AwardReporting-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
    </bean>

    <!-- Business Object Inquiry Definition -->
    <bean id="AwardReporting-inquiryDefinition" parent="AwardReporting-inquiryDefinition-parentBean" />
    <bean id="AwardReporting-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
        <property name="title" value="Award Reporting Id" />
        <property name="inquirySections" >
            <list>
                <bean parent="InquirySectionDefinition">
                    <property name="title" value="Award Reporting Id" />
                    <property name="numberOfColumns" value="1" />
                    <property name="inquiryFields" >
                        <list>
                            <bean parent="FieldDefinition"
                                p:attributeName="awardReportingId" 
                                p:forceInquiry="true" />
                            <bean parent="FieldDefinition"
                                p:attributeName="awardReportTermId" />
                            <bean parent="FieldDefinition"
                                p:attributeName="awardNumber" />
                            <bean parent="FieldDefinition"
                    			p:attributeName="sequenceNumber" />                                
                            <bean parent="FieldDefinition"
                                p:attributeName="dueDate" />
                            <bean parent="FieldDefinition"
                                p:attributeName="overdueCounter" />
                            <bean parent="FieldDefinition"
                                p:attributeName="reportStatusCode" />
                            <bean parent="FieldDefinition"
                                p:attributeName="activityDate" />
                            <bean parent="FieldDefinition"
                                p:attributeName="comments" />
                            <bean parent="FieldDefinition"
                                p:attributeName="personId" />                            
                        </list>
                    </property>
                </bean>
            </list>
        </property>
    </bean>

    <!-- Business Object Lookup Definition -->
    <bean id="AwardReporting-lookupDefinition" parent="AwardReporting-lookupDefinition-parentBean" />
    <bean id="AwardReporting-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
        <property name="title" value="Award Reporting Lookup" />
        <property name="menubar" value="&lt;a href=&quot;index.jsp&quot;&gt;Main&lt;/a&gt;" />

        <property name="defaultSort" >
            <bean parent="SortDefinition" >
        </bean>
        </property>
        <property name="lookupFields" >
            <list>
                <bean parent="FieldDefinition"
                    p:attributeName="awardReportingId" 
                    p:forceInquiry="true" />
                <bean parent="FieldDefinition"
                    p:attributeName="awardReportTermId" />    
                <bean parent="FieldDefinition"
                    p:attributeName="awardNumber" />
                <bean parent="FieldDefinition"
                    p:attributeName="sequenceNumber" />                    
                <bean parent="FieldDefinition"
                    p:attributeName="dueDate" />
                <bean parent="FieldDefinition"
                    p:attributeName="overdueCounter" />
                <bean parent="FieldDefinition"
                    p:attributeName="reportStatusCode" />
                <bean parent="FieldDefinition"
                    p:attributeName="activityDate" />
                <bean parent="FieldDefinition"
                    p:attributeName="comments" />
                <bean parent="FieldDefinition"
                    p:attributeName="personId" />                
            </list>
        </property>
        <property name="resultFields" >
            <list>
                <bean parent="FieldDefinition"
                    p:attributeName="awardReportingId" 
                    p:forceInquiry="true" />
                <bean parent="FieldDefinition"
                    p:attributeName="awardReportTermId" />    
                <bean parent="FieldDefinition"
                    p:attributeName="awardNumber" />
                <bean parent="FieldDefinition"
                    p:attributeName="sequenceNumber" />
                <bean parent="FieldDefinition"
                    p:attributeName="dueDate" />
                <bean parent="FieldDefinition"
                    p:attributeName="overdueCounter" />
                <bean parent="FieldDefinition"
                    p:attributeName="reportStatusCode" />
                <bean parent="FieldDefinition"
                    p:attributeName="activityDate" />
                <bean parent="FieldDefinition"
                    p:attributeName="comments" />
                <bean parent="FieldDefinition"
                    p:attributeName="personId" />                
            </list>
        </property>
    </bean>
</beans>
