<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.kra.proposaldevelopment.bo.NsfCode">

        <inquiry>
            <title>NSF Code</title>

            <inquirySections><inquirySection title="NSF Code"><inquiryFields>
                <field attributeName="nsfCode" forceInquiry="true" />
                <field attributeName="description" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>NSF Code Lookup</title>
			<menubar>
				<![CDATA[<a href="/kra-dev/index.html">Main</a>]]>
			</menubar>
            <instructions>Look up a NSF Code</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="nsfCode" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="nsfCode" required="false" />
                <lookupField attributeName="description" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="nsfCode" forceInquiry="true" />
                <field attributeName="description" forceInquiry="false"/>
            </resultFields>
        </lookup>

        <titleAttribute>nsfCode</titleAttribute>

        <attributes>

            <attribute name="nsfCode">
                <label>NSF Code</label>
                <shortLabel>NSF Code</shortLabel>

                <maxLength>15</maxLength>
                <validationPattern><alphaNumeric /></validationPattern>

                <control>
                    <text size="10" />
                </control>

                <summary>NSF Code</summary>
                <description>NSF Code</description>
			</attribute>

            <attributeReference name="description" sourceClassName="org.kuali.kra.bo.KraAttributeReferenceDummy" sourceAttributeName="description" />

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

		</attributes>
    </businessObject>

</dictionaryEntry>