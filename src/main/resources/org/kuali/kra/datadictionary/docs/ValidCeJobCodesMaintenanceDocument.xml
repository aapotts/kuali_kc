<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="ValidCeJobCodesMaintenanceDocument" parent="ValidCeJobCodesMaintenanceDocument-parentBean" />

  <bean id="ValidCeJobCodesMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kra.budget.bo.ValidCeJobCode" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="ValidCeJobCodesMaintenanceDocument-EditValidCeJobCodes" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>costElement</value>
        <value>jobCode</value>
      </list>
    </property>
    <property name="label" value="ValidCeJobCodes Maintenance Document" />
    <property name="shortLabel" value="validCeJobCodesMaintDoc" />
    <property name="summary" value="ValidCeJobCodes Maintenance Document" />
    <property name="description" value="Document used to create or update ValidCeJobCodes Reference objects" />
    <property name="documentTypeName" value="ValidCeJobCodesMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="VACE" />
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>WorkflowAdmin</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="ValidCeJobCodesMaintenanceDocument-EditValidCeJobCodes" parent="ValidCeJobCodesMaintenanceDocument-EditValidCeJobCodes-parentBean" />

  <bean id="ValidCeJobCodesMaintenanceDocument-EditValidCeJobCodes-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit ValidCeJobCodes" />
    <property name="title" value="Edit ValidCeJobCodes" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="costElement"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="jobCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>
</beans>
