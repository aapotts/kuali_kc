<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="UnitAdministratorTypeMaintenanceDocument" parent="UnitAdministratorTypeMaintenanceDocument-parentBean" />

  <bean id="UnitAdministratorTypeMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kra.bo.UnitAdministratorType" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="UnitAdministratorTypeMaintenanceDocument-EditUnitAdministratorType" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>unitAdministratorTypeCode</value>
      </list>
    </property>
    <property name="label" value="UnitAdministratorType Maintenance Document" />
    <property name="shortLabel" value="unitAdministratorTypeMaintDoc" />
    <property name="summary" value="UnitAdministratorType Maintenance Document" />
    <property name="description" value="Document used to create or update UnitAdministratorType Reference objects" />
    <property name="documentTypeName" value="UnitAdministratorTypeMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="UADT" />
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>WorkflowAdmin</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="UnitAdministratorTypeMaintenanceDocument-EditUnitAdministratorType" parent="UnitAdministratorTypeMaintenanceDocument-EditUnitAdministratorType-parentBean" />

  <bean id="UnitAdministratorTypeMaintenanceDocument-EditUnitAdministratorType-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit UnitAdministratorType" />
    <property name="title" value="Edit UnitAdministratorType" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="unitAdministratorTypeCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="description"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>
</beans>
