<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../../core/datadictionary/dataDictionary-1_0.dtd">

<dictionaryEntry>
    <maintenanceDocument>
        <businessObjectClass>org.kuali.kra.bo.Organization</businessObjectClass>
        <maintainableClass>org.kuali.kra.maintenance.OrganizationMaintenableImpl</maintainableClass> 
        <businessRulesClass>org.kuali.kra.rules.OrganizationMaintenanceDocumentRule</businessRulesClass>
                                                
		 <!-- 
		<maintainableClass>
			org.kuali.core.maintenance.KualiMaintainableImpl
		</maintainableClass>
		 -->
        <documentAuthorizerClass>org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase</documentAuthorizerClass>
        <authorizations>
            <authorization action="initiate">
                <workgroups>
                    <workgroup>WorkflowAdmin</workgroup>
                </workgroups>
            </authorization>
        </authorizations>

        <documentTypeName>OrganizationMaintenanceDocument</documentTypeName>
        <documentTypeCode>ORGA</documentTypeCode>

        <label>Organization Maintenance Document</label>
        <shortLabel>organizationMaintDoc</shortLabel>

        <summary>Organization Maintenance Document</summary>
        <description>Document used to create or update Organization Reference objects</description>

        <lockingKeys>
        	<field attributeName="organizationId"/>

        </lockingKeys>

        <maintainableSections>
            <maintainableSection title="Edit Organization">
                <maintainableItems>
                	<maintainableField name="organizationId" required="true"/>
					<maintainableField name="address" required="false"/>
					<maintainableField name="agencySymbol" required="false"/>
					<maintainableField name="animalWelfareAssurance" required="false"/>
					<maintainableField name="cableAddress" required="false"/>
					<maintainableField name="cageNumber" required="false"/>
					<maintainableField name="cognizantAuditor" required="false"/>
					<maintainableField name="comGovEntityCode" required="false"/>
					<maintainableField name="congressionalDistrict" required="false"/>
					<maintainableField name="contactAddressId" required="true"/>
					<maintainableField name="county" required="false"/>
					<maintainableField name="dodacNumber" required="false"/>
					<maintainableField name="dunsNumber" required="false"/>
					<maintainableField name="dunsPlusFourNumber" required="false"/>
					<maintainableField name="fedralEmployerId" required="false"/>
					<maintainableField name="humanSubAssurance" required="false"/>
					<maintainableField name="incorporatedDate" required="false"/>
					<maintainableField name="incorporatedIn" required="false"/>
					<maintainableField name="indirectCostRateAgreement" required="false"/>
					<maintainableField name="irsTaxExcemption" required="false"/>
					<maintainableField name="massEmployeeClaim" required="false"/>
					<maintainableField name="massTaxExcemptNum" required="false"/>
					<maintainableField name="nsfInstitutionalCode" required="false"/>
					<maintainableField name="numberOfEmployees" required="false"/>
					<maintainableField name="onrResidentRep" required="false"/>
					<maintainableField name="organizationName" required="true"/>
					<maintainableField name="phsAcount" required="false"/>
					<maintainableField name="scienceMisconductComplDate" required="false"/>
					<maintainableField name="telexNumber" required="false"/>
					<maintainableField name="vendorCode" required="false"/>

                    <maintainableField name="versionNumber" />
                </maintainableItems>
            </maintainableSection>
            
            <maintainableSection title="Organization Type">
                <maintainableItems>
                    <maintainableCollection name="organizationTypes" businessObjectClass="org.kuali.kra.bo.OrganizationType" includeMultipleLookupLine="true" sourceClassName="org.kuali.kra.bo.OrganizationType" sourceAttributeName="organizationTypes" includeAddLine="true">
                        <!-- <maintainableField name="organizationId"/> --> 
                        <maintainableField name="organizationTypeCode"  required="true" />
                    </maintainableCollection>
                </maintainableItems>
            </maintainableSection>

            <maintainableSection title="Organization Audit">
                <maintainableItems>
                    <maintainableCollection name="organizationAudits" businessObjectClass="org.kuali.kra.bo.OrganizationAudit"  includeAddLine="true">
	                	<maintainableField name="fiscalYear" required="true"/>
						<maintainableField name="auditAccepted" required="false"/>
						<maintainableField name="auditComment" required="false"/>
                    </maintainableCollection>
                </maintainableItems>
            </maintainableSection>

            <maintainableSection title="Organization Indirectcost">
                <maintainableItems>
	                    <maintainableCollection name="organizationIdcs" businessObjectClass="org.kuali.kra.bo.OrganizationIndirectcost"  includeAddLine="true">
	                	<maintainableField name="idcNumber" required="true"/>
						<maintainableField name="applicableIndirectcostRate" required="false"/>
						<maintainableField name="endDate" required="false"/>
						<maintainableField name="idcComment" required="false"/>
						<maintainableField name="idcRateTypeCode" required="false"/>
						<maintainableField name="requestedDate" required="false"/>
						<maintainableField name="startDate" required="false"/>
                    </maintainableCollection>
                </maintainableItems>
            </maintainableSection>

            <maintainableSection title="Organization Ynq">
                <maintainableItems>
                    <maintainableCollection name="organizationYnqs" businessObjectClass="org.kuali.kra.bo.OrganizationYnq"  includeAddLine="false">
                       <maintainableField name="ynq.description" readOnly="true" /> 
						<maintainableField name="questionId" readOnly="true" />
						<maintainableField name="answer" required="true"/>
						<maintainableField name="explanation" required="false"/>
						<maintainableField name="reviewDate" required="false"/>
						<summaryFields>
                            <summaryField name="ynq.description" />
                        </summaryFields>
      
                    </maintainableCollection>
                </maintainableItems>
            </maintainableSection>
            
        </maintainableSections>
    </maintenanceDocument>
</dictionaryEntry>
