<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="FinancialIndirectCostTypeCodeMaintenanceDocument" parent="FinancialIndirectCostTypeCodeMaintenanceDocument-parentBean"/>

  <bean id="FinancialIndirectCostTypeCodeMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kra.external.award.FinancialIndirectCostTypeCode"/>
    <property name="maintainableClass" value="org.kuali.kra.maintenance.KraMaintainableImpl"/>
    <property name="maintainableSections">
      <list>
        <ref bean="FinancialIndirectCostTypeCodeMaintenanceDocument-EditFinancialIndirectCostTypeCode"/>
      </list>
    </property>
    <property name="allowsNewOrCopy" value="true"/>
    <property name="lockingKeys">
      <list>
        <value>rateClassCode</value>
        <value>rateTypeCode</value>
      </list>
    </property>
    
    <property name="documentTypeName" value="FinancialIndirectCostTypeCodeMaintenanceDocument"/>
    <property name="documentAuthorizerClass" value="org.kuali.rice.kns.document.authorization.MaintenanceDocumentAuthorizerBase"/>
  	<property name="businessRulesClass" value="org.kuali.kra.external.award.FinancialIndirectCostTypeCodeDocumentRule"/>
    
    
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="FinancialIndirectCostTypeCodeMaintenanceDocument-EditFinancialIndirectCostTypeCode" parent="FinancialIndirectCostTypeCodeMaintenanceDocument-EditFinancialIndirectCostTypeCode-parentBean"/>

  <bean id="FinancialIndirectCostTypeCodeMaintenanceDocument-EditFinancialIndirectCostTypeCode-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit FinancialIndirectCostTypeCode"/>
    <property name="title" value="Edit FinancialIndirectCostTypeCode"/>
    <property name="maintainableItems">
      <list>
        <bean parent="MaintainableFieldDefinition" p:name="rateClassCode" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="rateTypeCode" p:required="true"/>
		<bean parent="MaintainableFieldDefinition" p:name="idcRateTypeCode" p:required="true"/>
      </list>
    </property>
  </bean>
</beans>

