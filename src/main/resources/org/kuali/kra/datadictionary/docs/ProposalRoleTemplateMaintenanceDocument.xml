<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../../core/datadictionary/dataDictionary-1_0.dtd">

<dictionaryEntry>
    <maintenanceDocument>
        <businessObjectClass>org.kuali.kra.proposaldevelopment.bo.ProposalRoleTemplate</businessObjectClass>
        <maintainableClass>org.kuali.core.maintenance.KualiMaintainableImpl</maintainableClass>

        <documentAuthorizerClass>org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase</documentAuthorizerClass>
        <authorizations>
            <authorization action="initiate">
                <workgroups>
                    <workgroup>WorkflowAdmin</workgroup>
                </workgroups>
            </authorization>
        </authorizations>

        <documentTypeName>ProposalRoleTemplateMaintenanceDocument</documentTypeName>
        <documentTypeCode>PRTD</documentTypeCode>

        <label>Proposal Role Template Maintenance Document</label>
        <shortLabel>proposalRoleTemplateMaintDoc</shortLabel>

        <summary>ProposalRoleTemplate Maintenance Document</summary>
        <description>Document used to create or update Proposal Role Template Reference objects</description>

        <lockingKeys>
            <field attributeName="id"/>
        </lockingKeys>

        <maintainableSections>
            <maintainableSection title="Edit Proposal Role Template">
                <maintainableItems>
                    <maintainableField name="id" required="false"/>
                    <maintainableField name="personId" required="true"/>
                    <maintainableField name="roleName" required="true">
                        <defaultValue>unassigned</defaultValue>
                    </maintainableField>
                    <maintainableField name="unitNumber" required="true"/>
                    <maintainableField name="active" required="true">
                        <defaultValue>true</defaultValue>
                    </maintainableField>
                    <maintainableField name="versionNumber" />
                </maintainableItems>
            </maintainableSection>
        </maintainableSections>
    </maintenanceDocument>
</dictionaryEntry>
