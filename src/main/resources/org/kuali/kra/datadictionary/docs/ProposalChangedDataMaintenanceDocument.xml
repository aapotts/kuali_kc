<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="PropChangedDataMaintenanceDocument" parent="PropChangedDataMaintenanceDocument-parentBean" />

  <bean id="PropChangedDataMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kra.proposaldevelopment.bo.ProposalChangedData" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="PropChangedDataMaintenanceDocument-EditPropChangedData" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>changeNumber</value>
        <value>columnName</value>
        <value>proposalNumber</value>
      </list>
    </property>
    <property name="label" value="PropChangedData Maintenance Document" />
    <property name="shortLabel" value="propChangedDataMaintDoc" />
    <property name="summary" value="PropChangedData Maintenance Document" />
    <property name="description" value="Document used to create or update PropChangedData Reference objects" />
    <property name="documentTypeName" value="PropChangedDataMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="PRCH" />
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>WorkflowAdmin</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="PropChangedDataMaintenanceDocument-EditPropChangedData" parent="PropChangedDataMaintenanceDocument-EditPropChangedData-parentBean" />

  <bean id="PropChangedDataMaintenanceDocument-EditPropChangedData-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit PropChangedData" />
    <property name="title" value="Edit PropChangedData" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="changeNumber"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="columnName"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="proposalNumber"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="changedValue" />
        <bean parent="MaintainableFieldDefinition"
              p:name="comments" />
        <bean parent="MaintainableFieldDefinition"
              p:name="displayValue" />
        <bean parent="MaintainableFieldDefinition"
              p:name="oldDisplayValue" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>
</beans>
