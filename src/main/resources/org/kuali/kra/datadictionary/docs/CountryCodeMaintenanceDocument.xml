<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="CountryCodeMaintenanceDocument" parent="CountryCodeMaintenanceDocument-parentBean"/>

  <bean id="CountryCodeMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kra.bo.CountryCode"/>
    <property name="maintainableClass" value="org.kuali.kra.maintenance.KraMaintainableImpl"/>
    <property name="maintainableSections">
      <list>
        <ref bean="CountryCodeMaintenanceDocument-EditCountryCodes"/>
      </list>
    </property>
    <property name="lockingKeys">
      <list>
        <value>id</value>
      </list>
    </property>
    
    
    
    
    
    <property name="documentTypeName" value="CountryCodeMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.kra.kim.KcMaintenanceDocumentAuthorizerBase"/>
    
    
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="CountryCodeMaintenanceDocument-EditCountryCodes" parent="CountryCodeMaintenanceDocument-EditCountryCodes-parentBean"/>

  <bean id="CountryCodeMaintenanceDocument-EditCountryCodes-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Country Codes"/>
    <property name="title" value="Edit Country Codes"/>
    <property name="maintainableItems">
      <list>
        <bean parent="MaintainableFieldDefinition" p:name="countryName" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="twoCharacterCountryCode" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="threeCharacterCountryCode" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="versionNumber"/>
      </list>
    </property>
  </bean>
</beans>