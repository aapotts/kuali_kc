<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="CustomAttributeDocumentMaintenanceDocument" parent="CustomAttributeDocumentMaintenanceDocument-parentBean" />

  <bean id="CustomAttributeDocumentMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kra.bo.CustomAttributeDocument" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="CustomAttributeDocumentMaintenanceDocument-EditCustomAttributeDocument" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>customAttributeId</value>
        <value>documentTypeCode</value>
      </list>
    </property>
    <property name="label" value="CustomAttributeDocument Maintenance Document" />
    <property name="shortLabel" value="customAttributeDocumentMaintDoc" />
    <property name="summary" value="CustomAttributeDocument Maintenance Document" />
    <property name="description" value="Document used to create or update CustomAttributeDocument Reference objects" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KRA-M"
            p:parameterName="customAttributeDocumentMaintenanceHelp"
            p:parameterDetailType="D" />
    </property>
    <property name="documentTypeName" value="CustomAttributeDocumentMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="CADM" />
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>WorkflowAdmin</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="CustomAttributeDocumentMaintenanceDocument-EditCustomAttributeDocument" parent="CustomAttributeDocumentMaintenanceDocument-EditCustomAttributeDocument-parentBean" />

  <bean id="CustomAttributeDocumentMaintenanceDocument-EditCustomAttributeDocument-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit CustomAttributeDocument" />
    <property name="title" value="Edit CustomAttributeDocument" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="customAttributeId"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="documentTypeCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="required" />
        <bean parent="MaintainableFieldDefinition"
              p:name="typeName" />
        <bean parent="MaintainableFieldDefinition"
              p:name="active" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>
</beans>
