<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="PropLocationMaintenanceDocument" parent="PropLocationMaintenanceDocument-parentBean"/>

  <bean id="PropLocationMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kra.proposaldevelopment.bo.ProposalLocation"/>
    <property name="maintainableClass" value="org.kuali.rice.kns.maintenance.KualiMaintainableImpl"/>
    <property name="maintainableSections">
      <list>
        <ref bean="PropLocationMaintenanceDocument-EditPropLocation"/>
      </list>
    </property>
    <property name="lockingKeys">
      <list>
        <value>location</value>
        <value>proposalNumber</value>
      </list>
    </property>
    
    
    
    
    <property name="documentTypeName" value="PropLocationMaintenanceDocument"/>
    <property name="documentAuthorizerClass" value="org.kuali.rice.kns.document.authorization.MaintenanceDocumentAuthorizerBase"/>
    
    
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="PropLocationMaintenanceDocument-EditPropLocation" parent="PropLocationMaintenanceDocument-EditPropLocation-parentBean"/>

  <bean id="PropLocationMaintenanceDocument-EditPropLocation-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit PropLocation"/>
    <property name="title" value="Edit PropLocation"/>
    <property name="maintainableItems">
      <list>
        <bean parent="MaintainableFieldDefinition" p:name="location" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="proposalNumber" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="rolodexId"/>
        <bean parent="MaintainableFieldDefinition" p:name="versionNumber"/>
      </list>
    </property>
  </bean>
</beans>