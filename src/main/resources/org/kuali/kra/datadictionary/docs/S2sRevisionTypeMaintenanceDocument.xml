<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="S2sRevisionTypeMaintenanceDocument" parent="S2sRevisionTypeMaintenanceDocument-parentBean" />

  <bean id="S2sRevisionTypeMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kra.bo.S2sRevisionType" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="S2sRevisionTypeMaintenanceDocument-EditS2SRevisionType" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>s2sRevisionTypeCode</value>
      </list>
    </property>
    <property name="label" value="S2S Revision Type Maintenance Document" />
    <property name="shortLabel" value="s2sRevisionTypeMaintDoc" />
    <property name="summary" value="S2S Revision Type Maintenance Document" />
    <property name="description" value="Document used to create or update S2sRevisionType Reference objects" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KRA-M"
            p:parameterName="s2sRevisionTypeMaintenanceHelp"
            p:parameterDetailType="D" />
    </property>
    <property name="documentTypeName" value="S2sRevisionTypeMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="S2RE" />
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>WorkflowAdmin</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="S2sRevisionTypeMaintenanceDocument-EditS2SRevisionType" parent="S2sRevisionTypeMaintenanceDocument-EditS2SRevisionType-parentBean" />

  <bean id="S2sRevisionTypeMaintenanceDocument-EditS2SRevisionType-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit S2S Revision Type" />
    <property name="title" value="Edit S2S Revision Type" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="s2sRevisionTypeCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="description"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>
</beans>
