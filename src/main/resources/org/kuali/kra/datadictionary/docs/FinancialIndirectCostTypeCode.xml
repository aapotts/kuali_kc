<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="FinancialIndirectCostTypeCode" parent="FinancialIndirectCostTypeCode-parentBean" />

  <bean id="FinancialIndirectCostTypeCode-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kra.external.award.FinancialIndirectCostTypeCode" />
		<property name="objectLabel" value="FinancialIndirectCostTypeCode" />
    <property name="inquiryDefinition" >
      <ref bean="FinancialIndirectCostTypeCode-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="FinancialIndirectCostTypeCode-lookupDefinition" />
    </property>
    <property name="attributes" >
      <list>
        <ref bean="FinancialIndirectCostTypeCode-rateClassCode" />
        <ref bean="FinancialIndirectCostTypeCode-rateTypeCode" />
        <ref bean="FinancialIndirectCostTypeCode-idcRateTypeCode" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->
<bean id="FinancialIndirectCostTypeCode-idcRateTypeCode" parent="FinancialIndirectCostTypeCode-idcRateTypeCode-parentBean" />

  <bean id="FinancialIndirectCostTypeCode-idcRateTypeCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="idcRateTypeCode" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Idc Rate Type Code" />
    <property name="shortLabel" value="Idc Rate Type Code" />
    <property name="maxLength" value="8" />
    <property name="validationPattern" >
      <bean parent="AlphaNumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="8" />
    </property>
    <property name="summary" value="Idc Rate Type Code" />
    <property name="description" value="Idc Rate Type Code" />
  </bean>
 
  <bean id="FinancialIndirectCostTypeCode-rateClassCode" parent="FinancialIndirectCostTypeCode-rateClassCode-parentBean" />

  <bean id="FinancialIndirectCostTypeCode-rateClassCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="rateClassCode" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Rate Class Code" />
    <property name="shortLabel" value="Rate Class Code" />
    <property name="maxLength" value="3" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="3" />
    </property>
    <property name="summary" value="Rate Class Code" />
    <property name="description" value="Rate Class Code" />
  </bean>
  <bean id="FinancialIndirectCostTypeCode-rateTypeCode" parent="FinancialIndirectCostTypeCode-rateTypeCode-parentBean" />

  <bean id="FinancialIndirectCostTypeCode-rateTypeCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="rateTypeCode" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Rate Type Code" />
    <property name="shortLabel" value="Rate Type Code" />
    <property name="maxLength" value="3" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="3" />
    </property>
    <property name="summary" value="Rate Type Code" />
    <property name="description" value="Rate Type Code" />
  </bean>
 
<!-- Business Object Inquiry Definition -->


  <bean id="FinancialIndirectCostTypeCode-inquiryDefinition" parent="FinancialIndirectCostTypeCode-inquiryDefinition-parentBean" />

  <bean id="FinancialIndirectCostTypeCode-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="FinancialIndirectCostTypeCode" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="FinancialIndirectCostTypeCode" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="rateClassCode"
                    p:forceInquiry="true" />
              <bean parent="FieldDefinition"
                    p:attributeName="rateTypeCode"
                    p:forceInquiry="true" />
              <bean parent="FieldDefinition"
                    p:attributeName="idcRateTypeCode" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="FinancialIndirectCostTypeCode-lookupDefinition" parent="FinancialIndirectCostTypeCode-lookupDefinition-parentBean" />

  <bean id="FinancialIndirectCostTypeCode-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="FinancialIndirectCostTypeCode Lookup" />
    <property name="menubar" value="&lt;a href=&quot;index.jsp&quot;&gt;Main&lt;/a&gt;" />

    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="rateClassCode" />
        <bean parent="FieldDefinition"
              p:attributeName="rateTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="idcRateTypeCode" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="rateClassCode"
              p:forceInquiry="true" />
        <bean parent="FieldDefinition"
              p:attributeName="rateTypeCode"
              p:forceInquiry="true" />
        <bean parent="FieldDefinition"
              p:attributeName="idcRateTypeCode" />
      </list>
    </property>
  </bean>
</beans>
