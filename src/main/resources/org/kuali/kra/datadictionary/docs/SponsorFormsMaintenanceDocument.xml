<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="SponsorFormsMaintenanceDocument" parent="SponsorFormsMaintenanceDocument-parentBean" />

  <bean id="SponsorFormsMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kra.bo.SponsorForms" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="SponsorFormsMaintenanceDocument-EditSponsorForms" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>packageNumber</value>
      </list>
    </property>
    <property name="label" value="SponsorForms Maintenance Document" />
    <property name="shortLabel" value="sponsorFormsMaintDoc" />
    <property name="summary" value="SponsorForms Maintenance Document" />
    <property name="description" value="Document used to create or update SponsorForms Reference objects" />
    <property name="documentTypeName" value="SponsorFormsMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="SPFO" />
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>WorkflowAdmin</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="SponsorFormsMaintenanceDocument-EditSponsorForms" parent="SponsorFormsMaintenanceDocument-EditSponsorForms-parentBean" />

  <bean id="SponsorFormsMaintenanceDocument-EditSponsorForms-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit SponsorForms" />
    <property name="title" value="Edit SponsorForms" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="packageName"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="packageNumber"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="sponsorCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>
</beans>
