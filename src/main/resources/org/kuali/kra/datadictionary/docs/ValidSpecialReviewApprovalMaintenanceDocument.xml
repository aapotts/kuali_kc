<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../../core/datadictionary/dataDictionary-1_0.dtd">

<dictionaryEntry>
    <maintenanceDocument>
        <businessObjectClass>org.kuali.kra.bo.ValidSpecialReviewApproval</businessObjectClass>
        <maintainableClass>org.kuali.kra.maintenance.KraMaintainableImpl</maintainableClass>

        <documentAuthorizerClass>org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase</documentAuthorizerClass>
        <authorizations>
            <authorization action="initiate">
                <workgroups>
                    <workgroup>WorkflowAdmin</workgroup>
                </workgroups>
            </authorization>
        </authorizations>

        <documentTypeName>ValidSpecialReviewApprovalMaintenanceDocument</documentTypeName>
        <documentTypeCode>VSRA</documentTypeCode>

        <label>Valid Special Review Approval Maintenance Document</label>
        <shortLabel>validSpecialReviewApprovalMaintDoc</shortLabel>

        <summary>Valid Special Review Approval Maintenance Document</summary>
        <description>Document used to create or update Special Review Reference objects</description>
        <help parameterNamespace="KRA-M" parameterDetailType="D" parameterName="validSpecialReviewApprovalMaintenanceHelp" />

        <lockingKeys>
            <field attributeName="approvalTypeCode" />
            <field attributeName="specialReviewCode" />
        </lockingKeys>

        <maintainableSections>
            <maintainableSection title="Edit Valid Special Review Approval">
                <maintainableItems>
                    <maintainableField name="approvalTypeCode" required="true" />
                    <maintainableField name="specialReviewCode" required="true" />
                    <maintainableField name="applicationDateFlag" />
                    <maintainableField name="approvalDateFlag" />
                    <maintainableField name="protocolNumberFlag" />
                    <maintainableField name="exemptNumberFlag" />
                    <maintainableField name="versionNumber" />
                </maintainableItems>
            </maintainableSection>
        </maintainableSections>
    </maintenanceDocument>
</dictionaryEntry>
