        <bean parent="MaintainableFieldDefinition" p:name="facultyFlag"/>
        <bean parent="MaintainableFieldDefinition" p:name="graduateStudentStaffFlag"/>
        <bean parent="MaintainableFieldDefinition" p:name="researchStaffFlag"/>
