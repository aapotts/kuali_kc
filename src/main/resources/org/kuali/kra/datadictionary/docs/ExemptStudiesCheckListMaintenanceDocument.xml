<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="ExemptStudiesCheckListMaintenanceDocument" parent="ExemptStudiesCheckListMaintenanceDocument-parentBean" />

  <bean id="ExemptStudiesCheckListMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kra.irb.bo.ExemptStudiesCheckListItem" />
    <property name="maintainableClass" value="org.kuali.kra.maintenance.KraMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="ExemptStudiesCheckListMaintenanceDocument-EditExemptStudiesCheckLists" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>exemptStudiesCheckListCode</value>
      </list>
    </property>
    <property name="label" value="Exempt Studies CheckList Maintenance Document" />
    <property name="shortLabel" value="exemptStudiesCheckListMaintDoc" />
    <property name="summary" value="Exempt Studies CheckList Maintenance Document" />
    <property name="description" value="Document used to create or update Exempt Studies CheckList Reference objects" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KRA-M"
            p:parameterName="exemptStudiesCheckListMaintenanceHelp"
            p:parameterDetailType="D" />
    </property>
    <property name="documentTypeName" value="ExemptStudiesCheckListMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="ESCK" />
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>WorkflowAdmin</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="ExemptStudiesCheckListMaintenanceDocument-EditExemptStudiesCheckLists" parent="ExemptStudiesCheckListMaintenanceDocument-EditExemptStudiesCheckLists-parentBean" />

  <bean id="ExemptStudiesCheckListMaintenanceDocument-EditExemptStudiesCheckLists-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Exempt Studies CheckLists" />
    <property name="title" value="Edit Exempt Studies CheckLists" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="exemptStudiesCheckListCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="description"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>
</beans>
