<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="KimRolePermission" parent="KimRolePermission-parentBean" />

  <bean id="KimRolePermission-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kra.kim.bo.KimRolePermission" />
		<property name="objectLabel" value="Kim Role Permission" />
    <property name="inquiryDefinition" >
      <ref bean="KimRolePermission-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="KimRolePermission-lookupDefinition" />
    </property>
    <property name="attributes" >
      <list>
        <ref bean="KimRolePermission-roleId" />
        <ref bean="KimRolePermission-permissionId" />
        <ref bean="KimRolePermission-roleName" />
        <ref bean="KimRolePermission-permissionName" />
        <ref bean="KimRolePermission-roleTypeName" />
        <ref bean="KimRolePermission-active" />
        <ref bean="KimRolePermission-versionNumber" />
      </list>
    </property>
    <property name="relationships" >
      <list>
        <bean parent="RelationshipDefinition">
          <property name="objectAttributeName" value="permission" />
          <property name="primitiveAttributes" >
            <list>
              <bean parent="PrimitiveAttributeDefinition"
                    p:sourceName="permissionId"
                    p:targetName="id" />
            </list>
          </property>
          <property name="supportAttributes" >
            <list>
              <bean parent="SupportAttributeDefinition"
                    p:identifier="true"
                    p:sourceName="permission.name"
                    p:targetName="name" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="KimRolePermission-roleId" parent="KimRolePermission-roleId-parentBean" />

  <bean id="KimRolePermission-roleId-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="roleId" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Role Id" />
    <property name="shortLabel" value="Role Id" />
    <property name="maxLength" value="8" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="40" />
    </property>
    <property name="summary" value="Role Id" />
    <property name="description" value="Role Id" />
  </bean>
  <bean id="KimRolePermission-permissionId" parent="KimRolePermission-permissionId-parentBean" />

  <bean id="KimRolePermission-permissionId-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="permissionId" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Permission Id" />
    <property name="shortLabel" value="Permission Id" />
    <property name="maxLength" value="8" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="40" />
    </property>
    <property name="summary" value="Permission Id" />
    <property name="description" value="Permission Id" />
  </bean>
  <bean id="KimRolePermission-roleName" parent="KimRolePermission-roleName-parentBean" />

  <bean id="KimRolePermission-roleName-parentBean" abstract="true" parent="KimRole-name">
    <property name="name" value="roleName" />
  </bean>
  <bean id="KimRolePermission-permissionName" parent="KimRolePermission-permissionName-parentBean" />

  <bean id="KimRolePermission-permissionName-parentBean" abstract="true" parent="KimPermission-name">
    <property name="name" value="permissionName" />
  </bean>
  <bean id="KimRolePermission-roleTypeName" parent="KimRolePermission-roleTypeName-parentBean" />

  <bean id="KimRolePermission-roleTypeName-parentBean" abstract="true" parent="KimRole-roleTypeName">
  </bean>
  <bean id="KimRolePermission-active" parent="KimRolePermission-active-parentBean" />

  <bean id="KimRolePermission-active-parentBean" abstract="true" parent="AttributeReferenceDummy-genericBoolean">
    <property name="name" value="active" />
    <property name="label" value="Active" />
    <property name="shortLabel" value="Active" />
    <property name="summary" value="Active" />
    <property name="description" value="Active" />
  </bean>
  <bean id="KimRolePermission-versionNumber" parent="KimRolePermission-versionNumber-parentBean" />

  <bean id="KimRolePermission-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="KimRolePermission-inquiryDefinition" parent="KimRolePermission-inquiryDefinition-parentBean" />

  <bean id="KimRolePermission-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Role-Permission" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="RolePermission" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="roleId" />
              <bean parent="FieldDefinition"
                    p:attributeName="permissionId" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="KimRolePermission-lookupDefinition" parent="KimRolePermission-lookupDefinition-parentBean" />

  <bean id="KimRolePermission-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Role-Permission Lookup" />
    <property name="menubar" value="&lt;a href=&quot;index.jsp&quot;&gt;Main&lt;/a&gt;" />

    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="roleId" />
        <bean parent="FieldDefinition"
              p:attributeName="permissionId" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="roleId" />
        <bean parent="FieldDefinition"
              p:attributeName="roleName" />
        <bean parent="FieldDefinition"
              p:attributeName="roleTypeName" />
        <bean parent="FieldDefinition"
              p:attributeName="permissionId" />
        <bean parent="FieldDefinition"
              p:attributeName="permissionName" />
        <bean parent="FieldDefinition"
              p:attributeName="active" />
      </list>
    </property>
  </bean>
</beans>
