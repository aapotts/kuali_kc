<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.kra.bo.PersonEditableField">

        <inquiry>
            <title>Person Editable Fields</title>
            <inquirySections><inquirySection title="Person Editable Field"><inquiryFields>
                <field attributeName="fieldName" forceInquiry="true"/>
                <field attributeName="active" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Person Editable Field Lookup</title>
			<menubar>
				<![CDATA[<a href="index.jsp">Main</a>]]>
			</menubar>
            <instructions>Look up a PersonEditable Field</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                	<sortAttribute attributeName="fieldName"/>
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="fieldName" />
                <lookupField attributeName="active" />
            </lookupFields>

            <resultFields>
                <field attributeName="fieldName" forceInquiry="true"/>
                <field attributeName="active" forceInquiry="true"/>
           </resultFields>
        </lookup>

        <titleAttribute>fieldName</titleAttribute>
        <attributes>

			<attribute name="fieldName">
                <label>Field Name </label>
			    <shortLabel>Field Name </shortLabel>
			    <maxLength>30</maxLength>
			    <validationPattern><alphaNumeric /></validationPattern>
                <control>
                    <select valuesFinderClass="org.kuali.kra.proposaldevelopment.lookup.keyvalue.PersonEditableFieldValuesFinder" />
                </control>
                <summary>Field Name </summary>
                <description>Field Name </description>
			</attribute>

            <attributeReference name="active" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericBoolean">
                <label>Active</label>
 	            <shortLabel>Active </shortLabel>
 	            <summary>Active</summary>
                <description>Active</description>
            </attributeReference>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />
		</attributes>
    </businessObject>

</dictionaryEntry>
