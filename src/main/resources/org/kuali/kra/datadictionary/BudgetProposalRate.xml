<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.kra.budget.bo.BudgetProposalRate">

        <inquiry>
            <title>Prop Rates</title>

            <inquirySections><inquirySection title="Prop Rates"><inquiryFields>
            	<field attributeName="fiscalYear" forceInquiry="true"/>
				<field attributeName="onOffCampusFlag" forceInquiry="true"/>
				<field attributeName="proposalNumber" forceInquiry="true"/>
				<field attributeName="rateClassCode" forceInquiry="true"/>
				<field attributeName="rateTypeCode" forceInquiry="true"/>
				<field attributeName="startDate" forceInquiry="true"/>
				<field attributeName="versionNumber" forceInquiry="true"/>
				<field attributeName="activityTypeCode"/>
				<field attributeName="applicableRate"/>
				<field attributeName="instituteRate"/>

            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Prop Rates Lookup</title>
			<menubar>
				<![CDATA[<a href="index.jsp">Main</a>]]>
			</menubar>
            <instructions>Look up a Prop Rates</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                	<sortAttribute attributeName="fiscalYear"/>
					<sortAttribute attributeName="onOffCampusFlag"/>
					<sortAttribute attributeName="proposalNumber"/>
					<sortAttribute attributeName="rateClassCode"/>
					<sortAttribute attributeName="rateTypeCode"/>
					<sortAttribute attributeName="startDate"/>
					<sortAttribute attributeName="versionNumber"/>

                </sortAttributes>
            </defaultSort>

            <lookupFields>
            	<lookupField attributeName="fiscalYear" required="false"/>
				<lookupField attributeName="onOffCampusFlag" required="false"/>
				<lookupField attributeName="proposalNumber" required="false"/>
				<lookupField attributeName="rateClassCode" required="false"/>
				<lookupField attributeName="rateTypeCode" required="false"/>
				<lookupField attributeName="startDate" required="false"/>
				<lookupField attributeName="versionNumber" required="false"/>
				<lookupField attributeName="activityTypeCode" required="false"/>
				<lookupField attributeName="applicableRate" required="false"/>
				<lookupField attributeName="instituteRate" required="false"/>

            </lookupFields>

            <resultFields>
            	<field attributeName="fiscalYear" forceInquiry="true"/>
				<field attributeName="onOffCampusFlag" forceInquiry="true"/>
				<field attributeName="proposalNumber" forceInquiry="true"/>
				<field attributeName="rateClassCode" forceInquiry="true"/>
				<field attributeName="rateTypeCode" forceInquiry="true"/>
				<field attributeName="startDate" forceInquiry="true"/>
				<field attributeName="versionNumber" forceInquiry="true"/>
				<field attributeName="activityTypeCode" forceInquiry="false"/>
				<field attributeName="applicableRate" forceInquiry="false"/>
				<field attributeName="instituteRate" forceInquiry="false"/>

            </resultFields>
        </lookup>

        <titleAttribute>fiscalYear</titleAttribute>
		<!-- <titleAttribute>onOffCampusFlag</titleAttribute>
		<titleAttribute>proposalNumber</titleAttribute>
		<titleAttribute>rateClassCode</titleAttribute>
		<titleAttribute>rateTypeCode</titleAttribute>
		<titleAttribute>startDate</titleAttribute>
		<titleAttribute>versionNumber</titleAttribute> -->
        <attributes>

	        <attribute name="fiscalYear">
			<label>Fiscal Year </label>
			<shortLabel>Fiscal Year </shortLabel>
			<maxLength>4</maxLength>
			<validationPattern><alphaNumeric /></validationPattern>
			<control>
				<text size="4" />
			</control>
			<summary>Fiscal Year </summary>
			<description>Fiscal Year </description>
			</attribute>

			<attribute name="onOffCampusFlag">
			<label>On Off Campus Flag </label>
			<shortLabel>On Off Campus Flag </shortLabel>
			<maxLength>1</maxLength>
			<validationPattern><alphaNumeric /></validationPattern>
				<control>
				<text size="1" />
				</control>
			<summary>On Off Campus Flag </summary>
			<description>On Off Campus Flag </description>
			</attribute>


			<attribute name="viewLocation">
			<label>Location</label>
			<shortLabel>Location</shortLabel>
			<maxLength>1</maxLength>
			<validationPattern><alphaNumeric /></validationPattern>
				<control>
					<select valuesFinderClass="org.kuali.kra.budget.lookup.keyvalue.BudgetRatesLocationValuesFinder" />
				</control>
			<summary>On Off Campus Flag </summary>
			<description>On Off Campus Flag </description>
			</attribute>

			<attribute name="affectedBudgetPeriod">
			<label>Affected Budget Period</label>
			<shortLabel>Affected Budget Period</shortLabel>
			<maxLength>1000</maxLength>
				<control>
				<text size="12" />
				</control>
			<summary>Affected Budget Period</summary>
			<description>Affected Budget Period</description>
			</attribute>

			<attribute name="budgetPeriod">
			<label>Budget Period</label>
			<shortLabel>Budget Period</shortLabel>
			<maxLength>1</maxLength>
			<validationPattern><alphaNumeric /></validationPattern>
				<control>
					<select valuesFinderClass="org.kuali.kra.budget.lookup.keyvalue.BudgetRatesBudgetPeriodValuesFinder" />
				</control>
			<summary>Budget Period</summary>
			<description>Budget Period </description>
			</attribute>

			<attribute name="proposalNumber">
			<label>Proposal Number </label>
			<shortLabel>Proposal Number </shortLabel>
			<maxLength>12</maxLength>
			<validationPattern><alphaNumeric /></validationPattern>
			<control>
				<text size="12" />
			</control>
			<summary>Proposal Number </summary>
			<description>Proposal Number </description>
			</attribute>

			<attribute name="rateClassCode">
			<label>Rate Class Code </label>
			<shortLabel>Rate Class Code </shortLabel>
			<maxLength>3</maxLength>
			<validationPattern><numeric /></validationPattern>
			<control>
				<text size="3" />
			</control>
			<summary>Rate Class Code </summary>
			<description>Rate Class Code </description>
			</attribute>

			<attribute name="rateTypeCode">
			<label>Rate Type Code </label>
			<shortLabel>Rate Type Code </shortLabel>
			<maxLength>3</maxLength>
			<validationPattern><numeric /></validationPattern>
			<control>
				<text size="3" />
			</control>
			<summary>Rate Type Code </summary>
			<description>Rate Type Code </description>
			</attribute>

            <attributeReference name="startDate" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericDate" >
                <label>Start Date</label>
                <shortLabel>Start Date</shortLabel>
                <summary>Start Date</summary>
                <description>Start Date.</description>
      		</attributeReference>

			<attribute name="versionNumber">
			<label>Version Number </label>
			<shortLabel>Version Number </shortLabel>
			<maxLength>3</maxLength>
			<validationPattern><numeric /></validationPattern>
			<control>
				<text size="3" />
			</control>
			<summary>Version Number </summary>
			<description>Version Number </description>
			</attribute>

			<attribute name="activityTypeCode">
			<label>Activity Type Code </label>
			<shortLabel>Activity Type Code </shortLabel>
			<maxLength>3</maxLength>
			<validationPattern><numeric /></validationPattern>
			<control>
				<text size="3" />
			</control>
			<summary>Activity Type Code </summary>
			<description>Activity Type Code </description>
			
			</attribute>

            <attribute name="applicableRate">
			<label>Applicable Rate </label>
			<shortLabel>Applicable Rate </shortLabel>
			<maxLength>7</maxLength>
			<required>true</required>
			<control>
				<text size="5" />
			</control>
			<summary>Applicable Rate </summary>
			<description>Applicable Rate </description>
      		</attribute>

            <attribute name="instituteRate" >
			<label>Institute Rate </label>
			<shortLabel>Institute Rate </shortLabel>
			<maxLength>7</maxLength>
			<control>
				<text size="5" />
			</control>
			<summary>Institute Rate </summary>
			<description>Institute Rate </description>
      		</attribute>

		</attributes>
    </businessObject>

</dictionaryEntry>
