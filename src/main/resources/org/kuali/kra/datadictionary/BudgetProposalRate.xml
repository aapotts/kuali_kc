<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="BudgetProposalRate" parent="BudgetProposalRate-parentBean" />

  <bean id="BudgetProposalRate-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kra.budget.rates.BudgetProposalRate" />
    <property name="inquiryDefinition" >
      <ref bean="BudgetProposalRate-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="BudgetProposalRate-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="fiscalYear" />
    <property name="attributes" >
      <list>
        <ref bean="BudgetProposalRate-fiscalYear" />
        <ref bean="BudgetProposalRate-onOffCampusFlag" />
        <ref bean="BudgetProposalRate-viewLocation" />
        <ref bean="BudgetProposalRate-affectedBudgetPeriod" />
        <ref bean="BudgetProposalRate-budgetPeriod" />
        <ref bean="BudgetProposalRate-proposalNumber" />
        <ref bean="BudgetProposalRate-rateClassCode" />
        <ref bean="BudgetProposalRate-rateTypeCode" />
        <ref bean="BudgetProposalRate-startDate" />
        <ref bean="BudgetProposalRate-versionNumber" />
        <ref bean="BudgetProposalRate-activityTypeCode" />
        <ref bean="BudgetProposalRate-applicableRate" />
        <ref bean="BudgetProposalRate-instituteRate" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="BudgetProposalRate-fiscalYear" parent="BudgetProposalRate-fiscalYear-parentBean" />

  <bean id="BudgetProposalRate-fiscalYear-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="fiscalYear" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Fiscal Year" />
    <property name="shortLabel" value="Fiscal Year" />
    <property name="maxLength" value="4" />
    <property name="validationPattern" >
      <bean parent="AlphaNumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="4" />
    </property>
    <property name="summary" value="Fiscal Year" />
    <property name="description" value="Fiscal Year" />
  </bean>
  <bean id="BudgetProposalRate-onOffCampusFlag" parent="BudgetProposalRate-onOffCampusFlag-parentBean" />

  <bean id="BudgetProposalRate-onOffCampusFlag-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="onOffCampusFlag" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="On Campus" />
    <property name="shortLabel" value="On Campus" />
    <property name="maxLength" value="1" />
    <property name="validationPattern" >
      <bean parent="AlphaNumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="1" />
    </property>
    <property name="summary" value="On Off Campus Flag" />
    <property name="description" value="On Off Campus Flag" />
  </bean>
  <bean id="BudgetProposalRate-viewLocation" parent="BudgetProposalRate-viewLocation-parentBean" />

  <bean id="BudgetProposalRate-viewLocation-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="viewLocation" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Location" />
    <property name="shortLabel" value="Location" />
    <property name="maxLength" value="1" />
    <property name="validationPattern" >
      <bean parent="AlphaNumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.kra.budget.lookup.keyvalue.BudgetRatesLocationValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
    <property name="summary" value="On Off Campus Flag" />
    <property name="description" value="On Off Campus Flag" />
  </bean>
  <bean id="BudgetProposalRate-affectedBudgetPeriod" parent="BudgetProposalRate-affectedBudgetPeriod-parentBean" />

  <bean id="BudgetProposalRate-affectedBudgetPeriod-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="affectedBudgetPeriod" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Affected Budget Period" />
    <property name="shortLabel" value="Affected Budget Period" />
    <property name="maxLength" value="1000" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="12" />
    </property>
    <property name="summary" value="Affected Budget Period" />
    <property name="description" value="Affected Budget Period" />
  </bean>
  <bean id="BudgetProposalRate-budgetPeriod" parent="BudgetProposalRate-budgetPeriod-parentBean" />

  <bean id="BudgetProposalRate-budgetPeriod-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="budgetPeriod" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Budget Period" />
    <property name="shortLabel" value="Budget Period" />
    <property name="maxLength" value="1" />
    <property name="validationPattern" >
      <bean parent="AlphaNumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.kra.budget.lookup.keyvalue.BudgetRatesBudgetPeriodValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
    <property name="summary" value="Budget Period" />
    <property name="description" value="Budget Period" />
  </bean>
  <bean id="BudgetProposalRate-proposalNumber" parent="BudgetProposalRate-proposalNumber-parentBean" />

  <bean id="BudgetProposalRate-proposalNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="proposalNumber" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Proposal Number" />
    <property name="shortLabel" value="Proposal Number" />
    <property name="maxLength" value="12" />
    <property name="validationPattern" >
      <bean parent="AlphaNumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="12" />
    </property>
    <property name="summary" value="Proposal Number" />
    <property name="description" value="Proposal Number" />
  </bean>
  <bean id="BudgetProposalRate-rateClassCode" parent="BudgetProposalRate-rateClassCode-parentBean" />

  <bean id="BudgetProposalRate-rateClassCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="rateClassCode" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Rate Class Code" />
    <property name="shortLabel" value="Rate Class Code" />
    <property name="maxLength" value="3" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="3" />
    </property>
    <property name="summary" value="Rate Class Code" />
    <property name="description" value="Rate Class Code" />
  </bean>
  <bean id="BudgetProposalRate-rateTypeCode" parent="BudgetProposalRate-rateTypeCode-parentBean" />

  <bean id="BudgetProposalRate-rateTypeCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="rateTypeCode" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Rate Type Code" />
    <property name="shortLabel" value="Rate Type Code" />
    <property name="maxLength" value="3" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="3" />
    </property>
    <property name="summary" value="Rate Type Code" />
    <property name="description" value="Rate Type Code" />
  </bean>
  <bean id="BudgetProposalRate-startDate" parent="BudgetProposalRate-startDate-parentBean" />

  <bean id="BudgetProposalRate-startDate-parentBean" abstract="true" parent="AttributeReferenceDummy-genericDate">
    <property name="name" value="startDate" />
    <property name="label" value="Start Date" />
    <property name="shortLabel" value="Start Date" />
    <property name="summary" value="Start Date" />
    <property name="description" value="Start Date." />
  </bean>
  <bean id="BudgetProposalRate-versionNumber" parent="BudgetProposalRate-versionNumber-parentBean" />

  <bean id="BudgetProposalRate-versionNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="versionNumber" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Version Number" />
    <property name="shortLabel" value="Version Number" />
    <property name="maxLength" value="3" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="3" />
    </property>
    <property name="summary" value="Version Number" />
    <property name="description" value="Version Number" />
  </bean>
  <bean id="BudgetProposalRate-activityTypeCode" parent="BudgetProposalRate-activityTypeCode-parentBean" />

  <bean id="BudgetProposalRate-activityTypeCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="activityTypeCode" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Activity Type Code" />
    <property name="shortLabel" value="Activity Type Code" />
    <property name="maxLength" value="3" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="3" />
    </property>
    <property name="summary" value="Activity Type Code" />
    <property name="description" value="Activity Type Code" />
  </bean>
  <bean id="BudgetProposalRate-applicableRate" parent="BudgetProposalRate-applicableRate-parentBean" />

  <bean id="BudgetProposalRate-applicableRate-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="applicableRate" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Applicable Rate" />
    <property name="shortLabel" value="Applicable Rate" />
    <property name="maxLength" value="7" />
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="5" />
    </property>
    <property name="summary" value="Applicable Rate" />
    <property name="description" value="Applicable Rate" />
  </bean>
  <bean id="BudgetProposalRate-instituteRate" parent="BudgetProposalRate-instituteRate-parentBean" />

  <bean id="BudgetProposalRate-instituteRate-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="instituteRate" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Institute Rate" />
    <property name="shortLabel" value="Institute Rate" />
    <property name="maxLength" value="7" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="5" />
    </property>
    <property name="summary" value="Institute Rate" />
    <property name="description" value="Institute Rate" />
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="BudgetProposalRate-inquiryDefinition" parent="BudgetProposalRate-inquiryDefinition-parentBean" />

  <bean id="BudgetProposalRate-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Prop Rates" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Prop Rates" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="fiscalYear"
                    p:forceInquiry="true" />
              <bean parent="FieldDefinition"
                    p:attributeName="onOffCampusFlag"
                    p:forceInquiry="true" />
              <bean parent="FieldDefinition"
                    p:attributeName="proposalNumber"
                    p:forceInquiry="true" />
              <bean parent="FieldDefinition"
                    p:attributeName="rateClassCode"
                    p:forceInquiry="true" />
              <bean parent="FieldDefinition"
                    p:attributeName="rateTypeCode"
                    p:forceInquiry="true" />
              <bean parent="FieldDefinition"
                    p:attributeName="startDate"
                    p:forceInquiry="true" />
              <bean parent="FieldDefinition"
                    p:attributeName="versionNumber"
                    p:forceInquiry="true" />
              <bean parent="FieldDefinition"
                    p:attributeName="activityTypeCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="applicableRate" />
              <bean parent="FieldDefinition"
                    p:attributeName="instituteRate" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="BudgetProposalRate-lookupDefinition" parent="BudgetProposalRate-lookupDefinition-parentBean" />

  <bean id="BudgetProposalRate-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Prop Rates Lookup" />
    <property name="menubar" value="&lt;a href=&quot;index.jsp&quot;&gt;Main&lt;/a&gt;" />
    <property name="instructions" value="Look up a Prop Rates" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="fiscalYear" />
        <bean parent="FieldDefinition"
              p:attributeName="onOffCampusFlag" />
        <bean parent="FieldDefinition"
              p:attributeName="proposalNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="rateClassCode" />
        <bean parent="FieldDefinition"
              p:attributeName="rateTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="startDate" />
        <bean parent="FieldDefinition"
              p:attributeName="versionNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="activityTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="applicableRate" />
        <bean parent="FieldDefinition"
              p:attributeName="instituteRate" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="fiscalYear"
              p:forceInquiry="true" />
        <bean parent="FieldDefinition"
              p:attributeName="onOffCampusFlag"
              p:forceInquiry="true" />
        <bean parent="FieldDefinition"
              p:attributeName="proposalNumber"
              p:forceInquiry="true" />
        <bean parent="FieldDefinition"
              p:attributeName="rateClassCode"
              p:forceInquiry="true" />
        <bean parent="FieldDefinition"
              p:attributeName="rateTypeCode"
              p:forceInquiry="true" />
        <bean parent="FieldDefinition"
              p:attributeName="startDate"
              p:forceInquiry="true" />
        <bean parent="FieldDefinition"
              p:attributeName="versionNumber"
              p:forceInquiry="true" />
        <bean parent="FieldDefinition"
              p:attributeName="activityTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="applicableRate" />
        <bean parent="FieldDefinition"
              p:attributeName="instituteRate" />
      </list>
    </property>
  </bean>
</beans>
