<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "dataDictionary-1_0.dtd">

<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.kra.proposaldevelopment.bo.Proposal">

        <inquiry>
            <title>Proposal</title>

            <inquirySections><inquirySection title="Proposal"><inquiryFields>
				<field attributeName="proposalNumber" forceInquiry="true"/>
				<field attributeName="proposalType.description" forceInquiry="false"/>
				<field attributeName="ownedByUnit" forceInquiry="false"/>
				<field attributeName="activityType.description" forceInquiry="false"/>
				<field attributeName="title" forceInquiry="false"/>
				<field attributeName="sponsorCode" forceInquiry="false"/>
				<field attributeName="requestedStartDateInitial" forceInquiry="false"/>
				<field attributeName="requestedEndDateInitial" forceInquiry="false"/>
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Proposal Lookup</title>
			<menubar>
				<![CDATA[<a href="/kra-dev/">Main</a>]]>
			</menubar>
            <instructions>Look up a Proposal</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
					<sortAttribute attributeName="proposalNumber"/>
                </sortAttributes>
            </defaultSort>

            <lookupFields>
				<lookupField attributeName="proposalNumber" required="false" forceLookup="true"/>
				<lookupField attributeName="proposalTypeCode" required="false"/>
				<lookupField attributeName="ownedByUnit" required="false"/>
				<lookupField attributeName="activityTypeCode" required="false"/>
				<lookupField attributeName="title" required="false"/>
				<lookupField attributeName="sponsorCode" required="false" forceLookup="true"/>
				<lookupField attributeName="requestedStartDateInitial" required="false"/>
				<lookupField attributeName="requestedEndDateInitial" required="false"/>
            </lookupFields>

            <resultFields>
				<field attributeName="proposalNumber" forceInquiry="true"/>
				<field attributeName="proposalType.description" forceInquiry="false"/>
				<field attributeName="ownedByUnit" forceInquiry="false"/>
				<field attributeName="activityType.description" forceInquiry="false"/>
				<field attributeName="title" forceInquiry="false"/>
				<field attributeName="sponsorCode" forceInquiry="false"/>
				<field attributeName="requestedStartDateInitial" forceInquiry="false"/>
				<field attributeName="requestedEndDateInitial" forceInquiry="false"/>
            </resultFields>
        </lookup>

        <titleAttribute>proposalNumber</titleAttribute>
        <attributes>

			<attribute name="proposalNumber">
                <label>Proposal Number</label>
                <shortLabel>Proposal Number</shortLabel>

                <maxLength>12</maxLength>
                <validationPattern><numeric /></validationPattern>

                <required>false</required>

                <control>
					<text size="14"/>
                </control>

                <summary>Proposal Number</summary>
                <description>Proposal Number</description>
			</attribute>
            <attribute name="title">
                <label>Project Title</label>
                <shortLabel>Title</shortLabel>

                <maxLength>500</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true" /></validationPattern>

                <required>true</required>

                <control>
                    <textarea rows="3" cols="80" />
                </control>

                <summary>The title of the propsal.</summary>
                <description>The title of the propsal.</description>
			</attribute>

			<attribute name="sponsorCode">
				<label>Agency/Sponsor #</label>
				<shortLabel>Agency/Sponsor #</shortLabel>

				<maxLength>6</maxLength>
				<validationPattern><anyCharacter /></validationPattern>

				<required>true</required>

				<control>
					<text size="12"/>
				</control>

				<summary>Agency/Sponsor #</summary>
				<description>Project Sponsor</description>
			</attribute>

            <attributeReference name="requestedStartDateInitial" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericDate" >
                <label>Start Date</label>
                <shortLabel>Start Dt</shortLabel>

 				<required>true</required>

                <summary>The Start Date of the Proposal</summary>
                <description>Yep, it's when the Proposal begins...</description>
      		</attributeReference>

            <attributeReference name="requestedEndDateInitial" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericDate" >
                <label>End Date</label>
                <shortLabel>Endt Dt</shortLabel>

 				<required>true</required>

                <summary>The End Date of the Proposal</summary>
                <description>Yep, it's when the Proposal ends...</description>
      		</attributeReference>

      		<attribute name="activityTypeCode">
                <label>Activity Type</label>
                <shortLabel>Activity Type</shortLabel>

                <maxLength>3</maxLength>
                <validationPattern>
                    <alphaNumeric />
                </validationPattern>

                <required>true</required>

                <control>
                    <select valuesFinderClass="org.kuali.kra.proposaldevelopment.lookup.keyvalue.ActivityTypeValuesFinder" />
                </control>

                <summary>Activity Type summary</summary>
                <description>Activity Type description</description>
            </attribute>

      		<attribute name="activityType.description">
                <label>Activity Type</label>
                <shortLabel>Activity Type</shortLabel>

                <maxLength>50</maxLength>
                <validationPattern><alphaNumeric allowWhitespace="true"/></validationPattern>

                <control>
					<text size="52"/>
                </control>

                <summary>Activity Type</summary>
                <description>Activity Type description</description>
            </attribute>

            <attribute name="ownedByUnit">
                <label>Lead Unit</label>
                <shortLabel>Lead Unit</shortLabel>

                <maxLength>8</maxLength>
                <validationPattern><anyCharacter /></validationPattern>

                <required>true</required>

                <control>
                    <select valuesFinderClass="org.kuali.kra.proposaldevelopment.lookup.keyvalue.LeadUnitValuesFinder" />
                </control>

                <summary>Lead Unit (Owned by Unit)</summary>
                <description>Lead Unit (Owned by Unit)</description>
			</attribute>

			<attribute name="proposalTypeCode">
                <label>Proposal Type</label>
                <shortLabel>Proposal Type</shortLabel>

                <maxLength>3</maxLength>
                <validationPattern><alphaNumeric /></validationPattern>

                <required>true</required>

                <control>
                    <select valuesFinderClass="org.kuali.kra.lookup.keyvalue.ProposalTypeValuesFinder" />
                </control>

                <summary>The Types that the Proposal Could be.</summary>
                <description>Isn't it obvious?</description>
            </attribute>

			<attribute name="proposalType.description">
                <label>Proposal Type</label>
                <shortLabel>Proposal Type</shortLabel>

                <maxLength>50</maxLength>
                <validationPattern><alphaNumeric allowWhitespace="true"/></validationPattern>

                <control>
					<text size="52"/>
                </control>

                <summary>The Types that the Proposal Could be.</summary>
                <description>Isn't it obvious?</description>
            </attribute>

        </attributes>
        <relationships>
			<relationship objectAttributeName="sponsor">
				<primitiveAttribute sourceName="sponsorCode" targetName="sponsorCode" />
			</relationship>
            <relationship objectAttributeName="activityType" >
                <primitiveAttribute sourceName="activityType.activityTypeCode" targetName="activityTypeCode" />
                <supportAttribute sourceName="activityType.description" targetName="description" />
            </relationship>
            <relationship objectAttributeName="proposalType" >
                <primitiveAttribute sourceName="proposalType.proposalTypeCode" targetName="proposalTypeCode" />
                <supportAttribute sourceName="proposalType.description" targetName="description" />
            </relationship>
		</relationships>
    </businessObject>
</dictionaryEntry>
