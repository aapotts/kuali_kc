<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.kra.bo.DegreeType">

        <inquiry>
            <title>Degree Type</title>

            <inquirySections><inquirySection title="Degree Type"><inquiryFields>
                <field attributeName="degreeCode" forceInquiry="true" />
                <field attributeName="degreeLevel" />
                <field attributeName="description" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Degree Type Lookup</title>
			<menubar>
				<![CDATA[<a href="index.jsp">Main</a>]]>
			</menubar>
            <instructions>Look up a Degree Type</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="degreeCode" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="degreeCode" required="false" />
                <lookupField attributeName="degreeLevel" required="false" />
                <lookupField attributeName="description" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="degreeCode" forceInquiry="true" />
                <field attributeName="degreeLevel" forceInquiry="false" />
                <field attributeName="description" forceInquiry="false"/>
            </resultFields>
        </lookup>

        <titleAttribute>degreeCode</titleAttribute>

        <attributes>

            <attribute name="degreeCode">
                <label>Degree Code</label>
                <shortLabel>Degree Code</shortLabel>

                <maxLength>7</maxLength>
                <validationPattern><alphaNumeric /></validationPattern>

                <control>
                    <text size="6" />
                </control>

                <summary>Degree Code</summary>
                <description>Degree Code</description>
			</attribute>

            <attribute name="degreeLevel" forceUppercase="true">
                <label>Degree Level </label>
                <shortLabel>Level</shortLabel>

                <maxLength>3</maxLength>
                <validationPattern><numeric /></validationPattern>

                <control>
                    <text size="4" />
                </control>

                <summary>Degree Level</summary>
                <description>Degree Level description...</description>
			</attribute>

            <attributeReference name="description" sourceClassName="org.kuali.kra.bo.KraAttributeReferenceDummy" sourceAttributeName="description" />

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />
		</attributes>
    </businessObject>

</dictionaryEntry>