<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.kra.budget.bo.ValidCeRateType">

        <inquiry>
            <title>Valid Ce Rate Types</title>

            <inquirySections><inquirySection title="Valid Ce Rate Types"><inquiryFields>
            	<field attributeName="costElement" forceInquiry="true"/>
				<field attributeName="rateClassCode" forceInquiry="true"/>
				<field attributeName="rateTypeCode" forceInquiry="true"/>

            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Valid Ce Rate Types Lookup</title>
			<menubar>
				<![CDATA[<a href="/kra-dev/">Main</a>]]>
			</menubar>
            <instructions>Look up a Valid Ce Rate Types</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                	<sortAttribute attributeName="costElement"/>
					<sortAttribute attributeName="rateClassCode"/>
					<sortAttribute attributeName="rateTypeCode"/>

                </sortAttributes>
            </defaultSort>

            <lookupFields>
            	<lookupField attributeName="costElement" required="false"/>
				<lookupField attributeName="rateClassCode" required="false"/>
				<lookupField attributeName="rateTypeCode" required="false"/>

            </lookupFields>

            <resultFields>
            	<field attributeName="costElement" forceInquiry="true"/>
				<field attributeName="rateClassCode" forceInquiry="true"/>
				<field attributeName="rateTypeCode" forceInquiry="true"/>

            </resultFields>
        </lookup>

        <titleAttribute>costElement</titleAttribute>
		<!-- <titleAttribute>rateClassCode</titleAttribute>
		<titleAttribute>rateTypeCode</titleAttribute> -->
        <attributes>

	        <attribute name="costElement">
			<label>Cost Element </label>
			<shortLabel>Cost Element </shortLabel>
			<maxLength>8</maxLength>
			<validationPattern><alphaNumeric /></validationPattern>
			<control>
				<text size="8" />
			</control>
			<summary>Cost Element </summary>
			<description>Cost Element </description>
			</attribute>

			<attribute name="rateClassCode">
			<label>Rate Class Code </label>
			<shortLabel>Rate Class Code </shortLabel>
			<maxLength>3</maxLength>
			<validationPattern><numeric /></validationPattern>
			<control>
				<text size="3" />
			</control>
			<summary>Rate Class Code </summary>
			<description>Rate Class Code </description>
			</attribute>

			<attribute name="rateTypeCode">
			<label>Rate Type Code </label>
			<shortLabel>Rate Type Code </shortLabel>
			<maxLength>3</maxLength>
			<validationPattern><numeric /></validationPattern>
			<control>
				<text size="3" />
			</control>
			<summary>Rate Type Code </summary>
			<description>Rate Type Code </description>
			</attribute>

		</attributes>
    </businessObject>

</dictionaryEntry>
