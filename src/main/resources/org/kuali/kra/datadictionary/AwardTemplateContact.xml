	<bean id="AwardTemplateContact-roleCode" parent="AwardTemplateContact-roleCode-parentBean"/>
	<bean abstract="true" id="AwardTemplateContact-roleCode-parentBean" parent="AttributeDefinition">
		<property name="name" value="roleCode"/>
		<property name="forceUppercase" value="false"/>
		<property name="label" value="Contact Type Code"/>
		<property name="shortLabel" value="Contact Type Code"/>
		<property name="maxLength" value="3"/>
		<property name="validationPattern">
			<bean parent="NumericValidationPattern"/>
		</property>
	    <property name="control">
	      <bean p:businessObjectClass="org.kuali.kra.award.home.ContactType" p:includeBlankRow="false" p:includeKeyInLabel="false" p:keyAttribute="roleCode" p:labelAttribute="description" p:valuesFinderClass="org.kuali.kra.lookup.keyvalue.ExtendedPersistableBusinessObjectValuesFinder" parent="SelectControlDefinition"/>
	    </property>
		<property name="summary" value="Contact Type Code"/>
		<property name="description" value="Contact Type Code"/>
	</bean>

	<bean id="AwardTemplateContact-rolodexId" parent="AwardTemplateContact-rolodexId-parentBean"/>
	<bean abstract="true" id="AwardTemplateContact-rolodexId-parentBean" parent="AttributeDefinition">
		<property name="name" value="rolodexId"/>
		<property name="forceUppercase" value="false"/>
		<property name="label" value="Rolodex Id"/>
		<property name="shortLabel" value="Rolodex Id"/>
		<property name="maxLength" value="22"/>
		<property name="validationPattern">
			<bean parent="NumericValidationPattern"/>
		</property>
		<property name="control">
			<bean p:size="10" parent="TextControlDefinition"/>
		</property>
		<property name="summary" value="Rolodex Id"/>
		<property name="description" value="Rolodex Id"/>
	</bean>

	<bean id="AwardTemplateContact-versionNumber" parent="AwardTemplateContact-versionNumber-parentBean"/>
	<bean abstract="true" id="AwardTemplateContact-versionNumber-parentBean" parent="AttributeReferenceDummy-versionNumber">
	</bean>

	<!-- Business Object Inquiry Definition -->
	<bean id="AwardTemplateContact-inquiryDefinition" parent="AwardTemplateContact-inquiryDefinition-parentBean"/>
	<bean abstract="true" id="AwardTemplateContact-inquiryDefinition-parentBean" parent="InquiryDefinition">
		<property name="title" value="Template Contact Id"/>
		<property name="inquirySections">
			<list>
				<bean parent="InquirySectionDefinition">
					<property name="title" value="Template Contact Id"/>
					<property name="numberOfColumns" value="1"/>
					<property name="inquiryFields">
						<list>
							<bean p:attributeName="templateContactId" p:forceInquiry="true" parent="FieldDefinition"/>
							<!--<bean parent="FieldDefinition"
								p:attributeName="templateCode" />-->
							<bean p:attributeName="roleCode" parent="FieldDefinition"/>
							<bean p:attributeName="rolodexId" parent="FieldDefinition"/>
						</list>
					</property>
				</bean>
			</list>
		</property>
	</bean>

	<!-- Business Object Lookup Definition -->
	<bean id="AwardTemplateContact-lookupDefinition" parent="AwardTemplateContact-lookupDefinition-parentBean"/>
	<bean abstract="true" id="AwardTemplateContact-lookupDefinition-parentBean" parent="LookupDefinition">
		<property name="title" value="Template Contact Lookup"/>
		<property name="menubar" value="&lt;a href=&quot;index.jsp&quot;&gt;Main&lt;/a&gt;"/>

		<property name="defaultSort">
			<bean parent="SortDefinition">
		</bean>
		</property>
		<property name="lookupFields">
			<list>
				<bean p:attributeName="templateContactId" p:forceInquiry="true" parent="FieldDefinition"/>
				<!--<bean parent="FieldDefinition"
					p:attributeName="templateCode" />-->
				<bean p:attributeName="roleCode" parent="FieldDefinition"/>
				<bean p:attributeName="rolodexId" parent="FieldDefinition"/>
			</list>
		</property>
		<property name="resultFields">
			<list>
				<bean p:attributeName="templateContactId" p:forceInquiry="true" parent="FieldDefinition"/>
				<!--<bean parent="FieldDefinition"
					p:attributeName="templateCode" />-->
				<bean p:attributeName="roleCode" parent="FieldDefinition"/>
				<bean p:attributeName="rolodexId" parent="FieldDefinition"/>
			</list>
		</property>
	</bean>
</beans>