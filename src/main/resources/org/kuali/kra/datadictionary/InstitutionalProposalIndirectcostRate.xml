        </property>
        <property name="summary" value="Proposal Indirectcost Rate Id" />
        <property name="description" value="Proposal Indirectcost Rate Id" />
    </bean>

    <bean id="InstitutionalProposalIndirectcostRate-proposalNumber" parent="InstitutionalProposalIndirectcostRate-proposalNumber-parentBean" />
