    </bean>

    <!-- Business Object Inquiry Definition -->
    <bean id="InstitutionalProposalIndirectcostRate-inquiryDefinition" parent="InstitutionalProposalIndirectcostRate-inquiryDefinition-parentBean" />
    <bean id="InstitutionalProposalIndirectcostRate-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
        <property name="title" value="Proposal Indirectcost Rate Id" />
        <property name="inquirySections" >
            <list>
                <bean parent="InquirySectionDefinition">
                    <property name="title" value="Proposal Indirectcost Rate Id" />
                    <property name="numberOfColumns" value="1" />
                    <property name="inquiryFields" >
                        <list>
                            <bean parent="FieldDefinition"
                                p:attributeName="proposalIndirectcostRateId" 
                                p:forceInquiry="true" />
                            <bean parent="FieldDefinition"
                                p:attributeName="proposalNumber" />
                            <bean parent="FieldDefinition"
                                p:attributeName="sequenceNumber" />
                            <bean parent="FieldDefinition"
                                p:attributeName="applicableIndirectcostRate" />
                            <bean parent="FieldDefinition"
                                p:attributeName="indirectcostRateTypeCode" />
                            <bean parent="FieldDefinition"
                                p:attributeName="fiscalYear" />
                            <bean parent="FieldDefinition"
