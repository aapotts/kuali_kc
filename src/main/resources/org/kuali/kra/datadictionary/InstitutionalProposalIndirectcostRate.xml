        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

    <bean id="InstitutionalProposalIndirectcostRate" parent="InstitutionalProposalIndirectcostRate-parentBean" />
    <bean id="InstitutionalProposalIndirectcostRate-parentBean" abstract="true" parent="BusinessObjectEntry">
        <property name="businessObjectClass" value="org.kuali.kra.institutionalproposal.home.InstitutionalProposalIndirectcostRate" />
        <property name="inquiryDefinition" >
            <ref bean="InstitutionalProposalIndirectcostRate-inquiryDefinition" />
        </property>
        <property name="lookupDefinition" >
            <ref bean="InstitutionalProposalIndirectcostRate-lookupDefinition" />
        </property>
        <property name="titleAttribute" value="proposalIndirectcostRateId" />
        <property name="attributes" >
            <list>
                <ref bean="InstitutionalProposalIndirectcostRate-proposalIndirectcostRateId" />
                <ref bean="InstitutionalProposalIndirectcostRate-proposalNumber" />
                <ref bean="InstitutionalProposalIndirectcostRate-sequenceNumber" />
                <ref bean="InstitutionalProposalIndirectcostRate-applicableIndirectcostRate" />
                <ref bean="InstitutionalProposalIndirectcostRate-indirectcostRateTypeCode" />
