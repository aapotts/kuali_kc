    <bean id="InstitutionalProposalIndirectcostRate-proposalIndirectcostRateId-parentBean" abstract="true" parent="AttributeDefinition" >
        <property name="name" value="proposalIndirectcostRateId" />
        <property name="forceUppercase" value="false" />
        <property name="label" value="Proposal Indirectcost Rate Id" />
        <property name="shortLabel" value="Proposal Indirectcost Rate Id" />
