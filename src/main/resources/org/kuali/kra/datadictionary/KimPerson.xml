<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="KimPerson" parent="KimPerson-parentBean" />

  <bean id="KimPerson-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kra.kim.bo.KimPerson" />
    <property name="attributes" >
      <list>
        <ref bean="KimPerson-id" />
        <ref bean="KimPerson-username" />
        <ref bean="KimPerson-password" />
        <ref bean="KimPerson-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="KimPerson-id" parent="KimPerson-id-parentBean" />

  <bean id="KimPerson-id-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="id" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="KIM Person Id" />
    <property name="shortLabel" value="KIM Person Id" />
    <property name="maxLength" value="8" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="HiddenControlDefinition" />
    </property>
    <property name="summary" value="KIM Person Id" />
    <property name="description" value="KIM Person Id" />
  </bean>
  <bean id="KimPerson-username" parent="KimPerson-username-parentBean" />

  <bean id="KimPerson-username-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="username" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="User Name" />
    <property name="shortLabel" value="User Name" />
    <property name="maxLength" value="500" />
    <property name="control" >
      <bean parent="HiddenControlDefinition" />
    </property>
    <property name="summary" value="User Name" />
    <property name="description" value="User Name" />
  </bean>
  <bean id="KimPerson-password" parent="KimPerson-password-parentBean" />

  <bean id="KimPerson-password-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="password" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Password" />
    <property name="shortLabel" value="Password" />
    <property name="maxLength" value="500" />
    <property name="control" >
      <bean parent="HiddenControlDefinition" />
    </property>
    <property name="summary" value="Password" />
    <property name="description" value="Password" />
  </bean>
  <bean id="KimPerson-versionNumber" parent="KimPerson-versionNumber-parentBean" />

  <bean id="KimPerson-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
  </bean>
</beans>
