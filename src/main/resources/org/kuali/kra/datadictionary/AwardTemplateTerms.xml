<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="AwardTemplateTerms" parent="AwardTemplateTerms-parentBean" />
	<bean id="AwardTemplateTerms-parentBean" abstract="true" parent="BusinessObjectEntry">
		<property name="businessObjectClass" value="org.kuali.kra.award.bo.AwardTemplateTerms" />
		<property name="inquiryDefinition" >
			<ref bean="AwardTemplateTerms-inquiryDefinition" />
		</property>
		<property name="lookupDefinition" >
			<ref bean="AwardTemplateTerms-lookupDefinition" />
		</property>
		<property name="titleAttribute" value="awardTemplateTermsId" />
		<property name="attributes" >
			<list>
				<ref bean="AwardTemplateTerms-awardTemplateTermsId" />
				<!--<ref bean="AwardTemplateTerms-awardTemplateCode" />-->
				<ref bean="AwardTemplateTerms-awardSponsorTermsId" />
				<ref bean="AwardTemplateTerms-versionNumber" />
			</list>
		</property>
	</bean>

	<!-- Attribute Definitions -->
	
	<bean id="AwardTemplateTerms-awardTemplateTermsId" parent="AwardTemplateTerms-awardTemplateTermsId-parentBean" />
	<bean id="AwardTemplateTerms-awardTemplateTermsId-parentBean" abstract="true" parent="AttributeDefinition" >
		<property name="name" value="awardTemplateTermsId" />
		<property name="forceUppercase" value="false" />
		<property name="label" value="Award Template Terms Id" />
		<property name="shortLabel" value="Award Template Terms Id" />
		<property name="maxLength" value="22" />
		<property name="validationPattern" >
			<bean parent="NumericValidationPattern" />
		</property>
		<property name="control" >
			<bean parent="TextControlDefinition"
				p:size="10" />
		</property>
		<property name="summary" value="Award Template Terms Id" />
		<property name="description" value="Award Template Terms Id" />
	</bean>

	<!--<bean id="AwardTemplateTerms-awardTemplateCode" parent="AwardTemplateTerms-awardTemplateCode-parentBean" />
	<bean id="AwardTemplateTerms-awardTemplateCode-parentBean" abstract="true" parent="AttributeDefinition" >
		<property name="name" value="awardTemplateCode" />
		<property name="forceUppercase" value="false" />
		<property name="label" value="Award Template Code" />
		<property name="shortLabel" value="Award Template Code" />
		<property name="maxLength" value="22" />
		<property name="validationPattern" >
			<bean parent="NumericValidationPattern" />
		</property>
		<property name="control" >
			<bean parent="TextControlDefinition"
				p:size="10" />
		</property>
		<property name="summary" value="Award Template Code" />
		<property name="description" value="Award Template Code" />
	</bean>-->
	<bean id="AwardTemplateTerms-awardSponsorTermsTypeCode" parent="AwardTemplateTerms-awardSponsorTermsTypeCode-parentBean" />
	<bean id="AwardTemplateTerms-awardSponsorTermsTypeCode-parentBean" abstract="true" parent="AttributeDefinition" >
		<property name="name" value="awardSponsorTermsTypeCode" />
		<property name="forceUppercase" value="false" />
		<property name="label" value="Award Sponsor Terms Type Code" />
		<property name="shortLabel" value="Award Sponsor Terms Type Code" />
		<property name="maxLength" value="3" />
		<property name="validationPattern" >
			<bean parent="AlphaNumericValidationPattern" />
		</property>
	    <property name="control" >
	      <bean parent="SelectControlDefinition"
	            p:businessObjectClass="org.kuali.kra.award.bo.AwardSponsorTermsType"
	            p:valuesFinderClass="org.kuali.kra.lookup.keyvalue.ExtendedPersistableBusinessObjectValuesFinder"
	            p:includeKeyInLabel="false"
	            p:keyAttribute="awardSponsorTermsTypeCode"
	            p:labelAttribute="description" />
	    </property>
		<property name="summary" value="Award Sponsor Terms Type Code" />
		<property name="description" value="Award Sponsor Terms Type Code" />
	</bean>
	<bean id="AwardTemplateTerms-awardSponsorTermsId" parent="AwardTemplateTerms-awardSponsorTermsId-parentBean" />
	<bean id="AwardTemplateTerms-awardSponsorTermsId-parentBean" abstract="true" parent="AttributeDefinition" >
		<property name="name" value="awardSponsorTermsId" />
		<property name="forceUppercase" value="false" />
		<property name="label" value="Award Sponsor Terms Id" />
		<property name="shortLabel" value="Award Sponsor Terms Id" />
		<property name="maxLength" value="22" />
		<property name="validationPattern" >
			<bean parent="NumericValidationPattern" />
		</property>
	    <property name="control" >
	      <bean parent="SelectControlDefinition"
	            p:businessObjectClass="org.kuali.kra.award.bo.AwardSponsorTerms"
	            p:valuesFinderClass="org.kuali.kra.lookup.keyvalue.ExtendedPersistableBusinessObjectValuesFinder"
	            p:includeKeyInLabel="false"
	            p:keyAttribute="awardSponsorTermsId"
	            p:labelAttribute="description" />
	    </property>
		<property name="summary" value="Award Sponsor Terms Id" />
		<property name="description" value="Award Sponsor Terms Id" />
	</bean>

	<bean id="AwardTemplateTerms-versionNumber" parent="AwardTemplateTerms-versionNumber-parentBean" />
	<bean id="AwardTemplateTerms-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
	</bean>

	<!-- Business Object Inquiry Definition -->
	<bean id="AwardTemplateTerms-inquiryDefinition" parent="AwardTemplateTerms-inquiryDefinition-parentBean" />
	<bean id="AwardTemplateTerms-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
		<property name="title" value="Award Template Terms Id" />
		<property name="inquirySections" >
			<list>
				<bean parent="InquirySectionDefinition">
					<property name="title" value="Award Template Terms Id" />
					<property name="numberOfColumns" value="1" />
					<property name="inquiryFields" >
						<list>
							<bean parent="FieldDefinition"
								p:attributeName="awardTemplateTermsId" 
								p:forceInquiry="true" />
							<!--<bean parent="FieldDefinition"
								p:attributeName="awardTemplateCode" />-->
							<bean parent="FieldDefinition"
								p:attributeName="awardSponsorTermsId" />
						</list>
					</property>
				</bean>
			</list>
		</property>
	</bean>

	<!-- Business Object Lookup Definition -->
	<bean id="AwardTemplateTerms-lookupDefinition" parent="AwardTemplateTerms-lookupDefinition-parentBean" />
	<bean id="AwardTemplateTerms-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
		<property name="title" value="Award Template Terms Lookup" />
		<property name="menubar" value="&lt;a href=&quot;index.jsp&quot;&gt;Main&lt;/a&gt;" />
		<property name="instructions" value="Look up a Award Template Terms" />
		<property name="defaultSort" >
			<bean parent="SortDefinition" >
		</bean>
		</property>
		<property name="lookupFields" >
			<list>
				<bean parent="FieldDefinition"
					p:attributeName="awardTemplateTermsId" 
					p:forceInquiry="true" />
				<!--<bean parent="FieldDefinition"
					p:attributeName="awardTemplateCode" />-->
				<bean parent="FieldDefinition"
					p:attributeName="awardSponsorTermsId" />
			</list>
		</property>
		<property name="resultFields" >
			<list>
				<bean parent="FieldDefinition"
					p:attributeName="awardTemplateTermsId" 
					p:forceInquiry="true" />
				<!--<bean parent="FieldDefinition"
					p:attributeName="awardTemplateCode" />-->
				<bean parent="FieldDefinition"
					p:attributeName="awardSponsorTermsId" />
			</list>
		</property>
	</bean>
</beans>
