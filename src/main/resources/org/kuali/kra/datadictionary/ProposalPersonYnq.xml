			<menubar>
				<![CDATA[<a href="index.jsp">Main</a>]]>
			</menubar>
            <instructions>Look up a Prop Pers Ynq</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                	<sortAttribute attributeName="proposalPersonNumber"/>
					<sortAttribute attributeName="proposalNumber"/>
					<sortAttribute attributeName="questionId"/>

                </sortAttributes>
            </defaultSort>

            <lookupFields>
            	<lookupField attributeName="proposalPersonNumber" required="false"/>
