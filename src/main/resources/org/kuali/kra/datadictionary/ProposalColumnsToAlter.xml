<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.kra.proposaldevelopment.bo.ProposalColumnsToAlter">

        <inquiry>
            <title>Prop Columns To Alter</title>

            <inquirySections><inquirySection title="Prop Columns To Alter"><inquiryFields>
            	<field attributeName="columnName" forceInquiry="true"/>
				<field attributeName="columnLabel"/>
				<field attributeName="dataLength"/>
				<field attributeName="dataType"/>
				<field attributeName="hasLookup"/>
				<field attributeName="lookupClass"/>
				<field attributeName="lookupReturn"/>

            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
        	<lookupableID>customLookupable</lookupableID>
            <title>Proposal Columns To Alter Lookup</title>
			<menubar>
				<![CDATA[<a href="index.jsp">Main</a>]]>
			</menubar>
            <instructions>Look up a Proposal Columns To Alter</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                	<sortAttribute attributeName="columnName"/>

                </sortAttributes>
            </defaultSort>

            <lookupFields>
            	<lookupField attributeName="columnName" required="false"/>
				<lookupField attributeName="columnLabel" required="false"/>
				<lookupField attributeName="dataLength" required="false"/>
				<lookupField attributeName="dataType" required="false"/>
				<lookupField attributeName="hasLookup" required="false"/>
				<lookupField attributeName="lookupClass" required="false"/>
				<lookupField attributeName="lookupReturn" required="false"/>
            </lookupFields>

            <resultFields>
            	<field attributeName="columnName" forceInquiry="true"/>
				<field attributeName="columnLabel" forceInquiry="false"/>
				<field attributeName="dataLength" forceInquiry="false"/>
				<field attributeName="dataType" forceInquiry="false"/>
				<field attributeName="hasLookup" forceInquiry="false"/>
				<field attributeName="lookupClass" forceInquiry="false"/>
				<field attributeName="lookupReturn" forceInquiry="false"/>
            </resultFields>
        </lookup>

        <titleAttribute>columnName</titleAttribute>
        <attributes>

	        <attribute name="columnName">
			<label>Column Name </label>
			<shortLabel>Column Name </shortLabel>
			<maxLength>30</maxLength>
			<validationPattern><anyCharacter/></validationPattern>
			<control>
				<text size="30" />
			</control>
			<summary>Column Name </summary>
			<description>Column Name </description>
			</attribute>

			<attribute name="columnLabel">
			<label>Column Label </label>
			<shortLabel>Column Label </shortLabel>
			<maxLength>30</maxLength>
			<validationPattern><anyCharacter allowWhitespace="true"/></validationPattern>
			<control>
				<text size="30" />
			</control>
			<summary>Column Label </summary>
			<description>Column Label </description>
			</attribute>

			<attribute name="dataLength">
			<label>Data Length </label>
			<shortLabel>Data Length </shortLabel>
			<maxLength>4</maxLength>
			<validationPattern><numeric /></validationPattern>
			<control>
				<text size="4" />
			</control>
			<summary>Data Length </summary>
			<description>Data Length </description>
			</attribute>

			<attribute name="dataType">
			<label>Data Type </label>
			<shortLabel>Data Type </shortLabel>
			<maxLength>9</maxLength>
			<validationPattern><alphaNumeric /></validationPattern>
			<control>
				<select valuesFinderClass="org.kuali.kra.proposaldevelopment.lookup.keyvalue.ProposalEditableFieldDataTypeValuesFinder" />
			</control>
			<summary>Data Type </summary>
			<description>Data Type </description>
			</attribute>

			<attributeReference name="hasLookup" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericBoolean">
			<label>Has Lookup </label>
			<shortLabel>Has Lookup </shortLabel>
			<summary>Has Lookup </summary>
			<description>Has Lookup </description>
			</attributeReference>

			<attribute name="lookupClass">
			<label>Lookup Argument </label>
			<shortLabel>Lookup Argument </shortLabel>
			<maxLength>100</maxLength>
			<validationPattern><anyCharacter /></validationPattern>
			<control>
				<select valuesFinderClass="org.kuali.kra.lookup.keyvalue.LookupableBoValuesFinder" script="updateLookupReturn(this, updateLookupReturn_Callback)" />
			</control> 
			<summary>Lookup Argument </summary>
			<description>Lookup Argument </description>
			</attribute>

			<attribute name="lookupReturn">
			<label>Lookup Return </label>
			<shortLabel>Lookup Return </shortLabel>
			<maxLength>50</maxLength>
			<validationPattern><alphaNumeric /></validationPattern>
			<control>
                 <select valuesFinderClass="org.kuali.kra.lookup.keyvalue.LookupReturnValuesFinder" />
			</control>
			<summary>Lookup Return </summary>
			<description>Lookup Return </description>
			</attribute>
			
			<attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />
		</attributes>
    </businessObject>

</dictionaryEntry>
