<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.kra.bo.SpecialReviewApprovalType">

        <inquiry>
            <title>Approval Type</title>

            <inquirySections><inquirySection title="Approval Type"><inquiryFields>
                <field attributeName="approvalTypeCode" forceInquiry="true" />
                <field attributeName="description" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Approval Type Lookup</title>
			<menubar>
				<![CDATA[<a href="index.jsp">Main</a>]]>
			</menubar>
            <instructions>Look up a Approval Type</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="approvalTypeCode" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="approvalTypeCode" required="false" />
                <lookupField attributeName="description" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="approvalTypeCode" forceInquiry="true" />
                <field attributeName="description" forceInquiry="false"/>
            </resultFields>
        </lookup>

        <titleAttribute>approvalTypeCode</titleAttribute>

        <attributes>

            <attribute name="approvalTypeCode">
                <label>Approval Type Code</label>
                <shortLabel>Approval Type Code</shortLabel>

                <maxLength>3</maxLength>
                <validationPattern><alphaNumeric /></validationPattern>

                <control>
                    <text size="4" />
                </control>

                <summary>Approval Type Code</summary>
                <description>Approval Type Code</description>
			</attribute>

            <attributeReference name="description" sourceClassName="org.kuali.kra.bo.KraAttributeReferenceDummy" sourceAttributeName="description" />

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

		</attributes>
    </businessObject>

</dictionaryEntry>