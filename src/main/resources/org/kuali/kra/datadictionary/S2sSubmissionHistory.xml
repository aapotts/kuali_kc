<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.kra.s2s.bo.S2sSubmissionHistory">

        <inquiry>
            <title>S2s Submission History</title>

            <inquirySections><inquirySection title="S2s Submission History"><inquiryFields>
            	<field attributeName="id" />
            	<field attributeName="proposalNumber" forceInquiry="true"/>
            	<field attributeName="originalProposalId" />
            	<field attributeName="submittedBy" />
            	<field attributeName="s2sSubmissionTypeCode" />
            	<field attributeName="s2sRevisionTypeCode" />
            	<field attributeName="submissionTime" />
            	<field attributeName="federalIdentifier" />            	
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>  		
            <title>Submission History Lookup</title>
			<menubar>
				<![CDATA[<a href="index.jsp">Main</a>]]>
			</menubar>
            <instructions>Look up a Submission History</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                	<sortAttribute attributeName="id"/>

                </sortAttributes>
            </defaultSort>

            <lookupFields>            	
				<lookupField attributeName="proposalNumber"  required="false"/>
            	<lookupField attributeName="originalProposalId"  required="false"/>
            </lookupFields>

            <resultFields>
				<field attributeName="id" forceInquiry="false"/>
				<field attributeName="proposalNumber" forceInquiry="false"/>
				<field attributeName="originalProposalId" forceInquiry="false"/>
				<field attributeName="submittedBy" forceInquiry="false"/>
				<field attributeName="s2sSubmissionTypeCode" forceInquiry="false"/>
				<field attributeName="s2sRevisionTypeCode" forceInquiry="false"/>
				<field attributeName="submissionTime" forceInquiry="false"/>
				<field attributeName="federalIdentifier" forceInquiry="false"/>
            </resultFields>
        </lookup>
                
        <attributes>
        	
        	<attribute name="id">
				<label>ID </label>
				<shortLabel>ID </shortLabel>
				<maxLength>3</maxLength>
				<validationPattern><anyCharacter /></validationPattern>
				<control>
					<hidden />
				</control>
				<summary>ID </summary>
				<description>ID </description>
			</attribute>

	        <attribute name="proposalNumber">
				<label>Proposal Number </label>
				<shortLabel>Proposal Number </shortLabel>
				<maxLength>8</maxLength>
				<validationPattern><anyCharacter /></validationPattern>
				<control>
					<hidden />
				</control>
				<summary>Proposal Number </summary>
				<description>Proposal Number </description>
			</attribute>
			
			<attribute name="proposalNumberOrig">
				<label>Proposal Number </label>
				<shortLabel>Proposal Number </shortLabel>
				<maxLength>8</maxLength>
				<validationPattern><anyCharacter /></validationPattern>
				<control>
					<text size="8"/>
				</control>
				<summary>Proposal Number </summary>
				<description>Proposal Number </description>
			</attribute>
			
			<attribute name="originalProposalId">
				<label>Original Proposal Id </label>
				<shortLabel>Original Proposal Id </shortLabel>
				<maxLength>8</maxLength>
				<validationPattern><anyCharacter /></validationPattern>
				<control>
					<text size="8"/>
				</control>
				<summary>Original Proposal Id </summary>
				<description>Original Proposal Id </description>
			</attribute>
			
      		<attribute name="submittedBy">
				<label>Submitted By</label>
				<shortLabel>Submitted By</shortLabel>
				<maxLength>60</maxLength>
				<validationPattern><anyCharacter /></validationPattern>
				<required>false</required>
				<control>
					<text size="60"/>
				</control>
				<summary>Submitted By</summary>
				<description>Submitted By</description>
			</attribute>				
            
            <attribute name="s2sSubmissionTypeCode">
				<label>Submission Type </label>
				<shortLabel>Submission Type </shortLabel>
				<maxLength>50</maxLength>
				<validationPattern><numeric /></validationPattern>
				<control>
					<text size="20" />
				</control>
				<summary>S2s Submission Type Code </summary>
				<description>S2s Submission Type Code </description>
			</attribute>
			
			<attribute name="s2sRevisionTypeCode">
				<label>Revision Type</label>
				<shortLabel>Revision Type</shortLabel>
				<maxLength>300</maxLength>
				<validationPattern><anyCharacter allowWhitespace="true"/></validationPattern>
				<control>
					<text size="300" />
				</control>
				<summary>Revision Type </summary>
				<description>Revision Type </description>
			</attribute>
			
			<attributeReference name="submissionTime" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericTimestamp" >
				<label>Posted Timestamp</label>			
			</attributeReference>
			
			<attribute name="federalIdentifier">
				<label>Federal Identifier</label>
				<shortLabel>Federal Identifier</shortLabel>
				<maxLength>50</maxLength>
				<validationPattern><anyCharacter allowWhitespace="true" /></validationPattern>
				<required>false</required>
				<control>
					<text size="50"/>
				</control>
				<summary>Federal Identifier</summary>
				<description>Federal Identifier</description>
			</attribute>
						
            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

		</attributes>
    </businessObject>

</dictionaryEntry>
