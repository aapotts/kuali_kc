<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="S2sSubmissionHistory" parent="S2sSubmissionHistory-parentBean" />

  <bean id="S2sSubmissionHistory-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kra.s2s.bo.S2sSubmissionHistory" />
    <property name="inquiryDefinition" >
      <ref bean="S2sSubmissionHistory-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="S2sSubmissionHistory-lookupDefinition" />
    </property>
    <property name="attributes" >
      <list>
        <ref bean="S2sSubmissionHistory-id" />
        <ref bean="S2sSubmissionHistory-proposalNumber" />
        <ref bean="S2sSubmissionHistory-proposalNumberOrig" />
        <ref bean="S2sSubmissionHistory-originalProposalId" />
        <ref bean="S2sSubmissionHistory-submittedBy" />
        <ref bean="S2sSubmissionHistory-s2sSubmissionTypeCode" />
        <ref bean="S2sSubmissionHistory-s2sRevisionTypeCode" />
        <ref bean="S2sSubmissionHistory-submissionTime" />
        <ref bean="S2sSubmissionHistory-federalIdentifier" />
        <ref bean="S2sSubmissionHistory-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="S2sSubmissionHistory-id" parent="S2sSubmissionHistory-id-parentBean" />

  <bean id="S2sSubmissionHistory-id-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="id" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="ID" />
    <property name="shortLabel" value="ID" />
    <property name="maxLength" value="3" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="HiddenControlDefinition" />
    </property>
    <property name="summary" value="ID" />
    <property name="description" value="ID" />
  </bean>
  <bean id="S2sSubmissionHistory-proposalNumber" parent="S2sSubmissionHistory-proposalNumber-parentBean" />

  <bean id="S2sSubmissionHistory-proposalNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="proposalNumber" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Proposal Number" />
    <property name="shortLabel" value="Proposal Number" />
    <property name="maxLength" value="8" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="HiddenControlDefinition" />
    </property>
    <property name="summary" value="Proposal Number" />
    <property name="description" value="Proposal Number" />
  </bean>
  <bean id="S2sSubmissionHistory-proposalNumberOrig" parent="S2sSubmissionHistory-proposalNumberOrig-parentBean" />

  <bean id="S2sSubmissionHistory-proposalNumberOrig-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="proposalNumberOrig" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Proposal Number" />
    <property name="shortLabel" value="Proposal Number" />
    <property name="maxLength" value="8" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="8" />
    </property>
    <property name="summary" value="Proposal Number" />
    <property name="description" value="Proposal Number" />
  </bean>
  <bean id="S2sSubmissionHistory-originalProposalId" parent="S2sSubmissionHistory-originalProposalId-parentBean" />

  <bean id="S2sSubmissionHistory-originalProposalId-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="originalProposalId" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Original Proposal Id" />
    <property name="shortLabel" value="Original Proposal Id" />
    <property name="maxLength" value="8" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="8" />
    </property>
    <property name="summary" value="Original Proposal Id" />
    <property name="description" value="Original Proposal Id" />
  </bean>
  <bean id="S2sSubmissionHistory-submittedBy" parent="S2sSubmissionHistory-submittedBy-parentBean" />

  <bean id="S2sSubmissionHistory-submittedBy-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="submittedBy" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Submitted By" />
    <property name="shortLabel" value="Submitted By" />
    <property name="maxLength" value="60" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="required" value="false" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="60" />
    </property>
    <property name="summary" value="Submitted By" />
    <property name="description" value="Submitted By" />
  </bean>
  <bean id="S2sSubmissionHistory-s2sSubmissionTypeCode" parent="S2sSubmissionHistory-s2sSubmissionTypeCode-parentBean" />

  <bean id="S2sSubmissionHistory-s2sSubmissionTypeCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="s2sSubmissionTypeCode" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Submission Type" />
    <property name="shortLabel" value="Submission Type" />
    <property name="maxLength" value="50" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="20" />
    </property>
    <property name="summary" value="S2s Submission Type Code" />
    <property name="description" value="S2s Submission Type Code" />
  </bean>
  <bean id="S2sSubmissionHistory-s2sRevisionTypeCode" parent="S2sSubmissionHistory-s2sRevisionTypeCode-parentBean" />

  <bean id="S2sSubmissionHistory-s2sRevisionTypeCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="s2sRevisionTypeCode" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Revision Type" />
    <property name="shortLabel" value="Revision Type" />
    <property name="maxLength" value="300" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="300" />
    </property>
    <property name="summary" value="Revision Type" />
    <property name="description" value="Revision Type" />
  </bean>
  <bean id="S2sSubmissionHistory-submissionTime" parent="S2sSubmissionHistory-submissionTime-parentBean" />

  <bean id="S2sSubmissionHistory-submissionTime-parentBean" abstract="true" parent="AttributeReferenceDummy-genericTimestamp">
    <property name="name" value="submissionTime" />
    <property name="label" value="Posted Timestamp" />
  </bean>
  <bean id="S2sSubmissionHistory-federalIdentifier" parent="S2sSubmissionHistory-federalIdentifier-parentBean" />

  <bean id="S2sSubmissionHistory-federalIdentifier-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="federalIdentifier" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Federal Identifier" />
    <property name="shortLabel" value="Federal Identifier" />
    <property name="maxLength" value="50" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="required" value="false" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="50" />
    </property>
    <property name="summary" value="Federal Identifier" />
    <property name="description" value="Federal Identifier" />
  </bean>
  <bean id="S2sSubmissionHistory-versionNumber" parent="S2sSubmissionHistory-versionNumber-parentBean" />

  <bean id="S2sSubmissionHistory-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="S2sSubmissionHistory-inquiryDefinition" parent="S2sSubmissionHistory-inquiryDefinition-parentBean" />

  <bean id="S2sSubmissionHistory-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="S2s Submission History" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="S2s Submission History" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="id" />
              <bean parent="FieldDefinition"
                    p:attributeName="proposalNumber"
                    p:forceInquiry="true" />
              <bean parent="FieldDefinition"
                    p:attributeName="originalProposalId" />
              <bean parent="FieldDefinition"
                    p:attributeName="submittedBy" />
              <bean parent="FieldDefinition"
                    p:attributeName="s2sSubmissionTypeCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="s2sRevisionTypeCode" />
              <bean parent="FieldDefinition"
                    p:attributeName="submissionTime" />
              <bean parent="FieldDefinition"
                    p:attributeName="federalIdentifier" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="S2sSubmissionHistory-lookupDefinition" parent="S2sSubmissionHistory-lookupDefinition-parentBean" />

  <bean id="S2sSubmissionHistory-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Submission History Lookup" />
    <property name="menubar" value="&lt;a href=&quot;index.jsp&quot;&gt;Main&lt;/a&gt;" />
    <property name="instructions" value="Look up a Submission History" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="proposalNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="originalProposalId" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="id" />
        <bean parent="FieldDefinition"
              p:attributeName="proposalNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="originalProposalId" />
        <bean parent="FieldDefinition"
              p:attributeName="submittedBy" />
        <bean parent="FieldDefinition"
              p:attributeName="s2sSubmissionTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="s2sRevisionTypeCode" />
        <bean parent="FieldDefinition"
              p:attributeName="submissionTime" />
        <bean parent="FieldDefinition"
              p:attributeName="federalIdentifier" />
      </list>
    </property>
  </bean>
</beans>
